</Georeferencing>

<RasterAnalysis rdf:about="#laplacian_filter_task">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  <hasProcessingTool rdf:resource="http://www.egc.org/ont/process/saga#laplacian_filter"/>
  <term:description xml:lang="en">Other Common Names: Laplacian, Laplacian of Gaussian, LoG, Marr Filter Standard kernel 1 (3x3):  0 | -1 |  0 -- + -- + -- -1 |  4 | -1 -- + -- + --  0 | -1 |  0 Standard kernel 2 (3x3): -1 | -1 | -1 -- + -- + -- -1 |  8 | -1 -- + -- + -- -1 | -1 | -1 Standard kernel 3 (3x3): -1 | -2 | -1 -- + -- + -- -2 | 12 | -2 -- + -- + -- -1 | -2 | -1</term:description>
  <core:prefLabel xml:lang="en"> Laplacian Filter task</core:prefLabel>
</RasterAnalysis>

