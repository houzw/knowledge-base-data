    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_database -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_database">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicDatabase"/>
        <skos:definition xml:lang="zh-cn">用计算机贮存、管理和检索水文资料的系统。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic database</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_drought -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_drought">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">由降水和地表水或地下水收支不平衡造成的异常水分短缺现 象。可以采用地表径流与其他因子组合成多因子指标以分析水文干旱。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic drought</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_effect -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_effect">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">水文情势对自然和人类活动等影响因素的响应。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic effect</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_effect_of_human_activities -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_effect_of_human_activities">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironment"/>
        <skos:definition xml:lang="zh-cn">由水工程、灌溉、开采地下水、水土保持、土地利用改变、城市 化、排水等引起降水、洪水、径流、地下水、沙量、水质等流域水文情 势的变化。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic effect of human activities</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_effect_of_reservoir -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_effect_of_reservoir">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">水库蓄水后导致库区、下游及受水或引水区水文规律发生变 化的一种现象。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic effect of reservoir</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_effect_of_urbanization -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_effect_of_urbanization">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#UrbanHydrology"/>
        <skos:definition xml:lang="zh-cn">城市化所及区域对水文过程和水文要素变化规律的影响。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic effect of urbanization</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_element -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_element">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">构成某一地点或区域在某一时间的水文情势的主要因素，是 描述水文情势的主要物理量，包括各种水文变量和水文现象，如降 水、蒸发、水位、流速、流量、含沙量、水质、水温等。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic element</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_element_identifier -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_element_identifier">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicDatabase"/>
        <skos:definition xml:lang="zh-cn">一个或一组标识水文要素、具有规律性和明确意义、易于计算 机和人工识别与处理的符号。采用英文字母和阿拉伯数字组合而 成，具有唯一性。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic element identifier</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_environment_of_urban -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_environment_of_urban">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#UrbanHydrology"/>
        <skos:definition xml:lang="zh-cn">城市下垫面条件下的水文循环过程的外部空间状况。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic environment of urban</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_experiment -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_experiment">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">为研究水文现象的物理过程及成因而开展的科学实验，包括 室外测试与室内实验。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic experiment</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_experiment_of_reservoir -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_experiment_of_reservoir">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicExperimentOfReservoir"/>
        <skos:definition xml:lang="zh-cn">为满足水库工程规划设计、施工和调度运行管理的需要，进行 水库水文、泥沙、水温等要素的观测实验，用以研究水库的水文泥 沙规律、径流与洪水还原计算、水库水文站观测部署、水库对径流 和泥沙拦蓄作用等。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic experiment of reservoir</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_experiment_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_experiment_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn"> 在天然和人为特定实验条件下，由一个或一组水文观测试验 项目的站点组成的专门场所。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic experiment station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_forecasting -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_forecasting">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">根据前期或现时已知的水文、气象等信息，运用水文学以及相 关学科的理论和方法，对河流、湖泊等水体未来一定时期内的水文 情势作出定量或定性的预报。对流域内降水的净雨过程所形成的出口断面流量过程的预报。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic forecasting</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_forecasting_for_construction_period -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_forecasting_for_construction_period">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">为了安全顺利地进行工程施工，根据施工不同阶段的要求所 作的水文预报。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic forecasting for construction period</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_frequency_distribution_curve -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_frequency_distribution_curve">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">水文变量与频率的关系曲线。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic frequency distribution curve</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_handbook -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_handbook">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">汇集气象、水文要素资料，经过统计、分析和地区综合，将水文 计算有关参数和特征值以图、表、公式等形式给出，供用户查算的 实用手册。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic handbook</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_information -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_information">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicInformation"/>
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StationNetwork"/>
        <skos:definition xml:lang="zh-cn">一切与水文相关的消息、声讯、数据、资料、符号、标识、指令等</skos:definition>
        <skos:definition xml:lang="zh-cn">河流、湖泊、水库和其他水体的水文及有关要素现时情况的及 时报告。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic information</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_information_sharing_platform -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_information_sharing_platform">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicDatabase"/>
        <skos:definition xml:lang="zh-cn">集水文信息加载、审核、发布、查阅、检索、系统管理、即时通信 等功能于一体的强大的水文信息共用平台。水文信息共享平台通 过将内、外部水文信息采集、处理，储存在服务器中，供政府机构及 有关单位共享使用，是用户储存、查阅、共享水文信息的重要工具。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic information sharing platform</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_instrument -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_instrument">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InstrumentAndMeasuring"/>
        <owl:sameAs rdf:resource="http://www.egc.org/ont/domain/hydrology#hydrometric_instrument"/>
        <skos:prefLabel xml:lang="en">hydrologic instrument</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_investigation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_investigation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicInvestigation"/>
        <skos:definition xml:lang="zh-cn">为弥补基本水文站网定位观测不足或其他特定目的，采用勘 测、观测、调查、考证、试验等手段采集水文信息及其有关资料的</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic investigation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_investigation_in_karst_areas -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_investigation_in_karst_areas">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicInvestigation"/>
        <skos:definition xml:lang="zh-cn">对岩溶地区实际地表集水面积、本流域与外流域间水量的交换数量、改变河川径流系列一致性的影响量等所进行的调查工作。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic investigation in karst areas</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_map_symbol -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_map_symbol">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">对水文专业地图上的地物、地貌符号的样式、规格、颜色、使用 以及标注和图廓整饰所作的统一规定。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic map symbol</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_mathematic_model -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_mathematic_model">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicModel"/>
        <skos:definition xml:lang="zh-cn">为模拟水文现象而建立的一种数学与逻辑结构。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic mathematic model</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_mathematic_model_of_watershed -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_mathematic_model_of_watershed">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicModel"/>
        <skos:definition xml:lang="zh-cn">模拟流域上水文过程的数学模型，包括蒸散发、产流、汇流等 子模型。有代表性的流域水文模型有我国的新安江流域模型，美 国的斯坦福（Stanford)流域模型、萨克拉门托（Sacramento )流域模型等。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic mathematic model of watershed</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_model -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_model">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicModel"/>
        <skos:definition xml:lang="zh-cn">为模拟水文现象而建立的实体结构或数学与逻辑结构。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic model</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_model_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_model_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FloodForecasting"/>
        <skos:definition xml:lang="zh-cn">根据降雨形成径流的物理机制与影响因素的关系，建立具有 一定预见期的确定性水文模型，把现时水文气象信息输入计算机， 以求得预报值的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic model method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_monitoring -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_monitoring">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Monitoring"/>
        <skos:definition xml:lang="zh-cn">通过水文站网对江河、湖泊、渠道、水库的水位、流量、水质、水温、泥沙、冰情、水下地形和地下水资源，以及降水量、蒸发量、墒情、风暴潮等实施观测，并进行分析和计算的活动。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic monitoring</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_monitoring_facility -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_monitoring_facility">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#MonitoringSettings"/>
        <skos:definition xml:lang="zh-cn">为配合水文要素监测、传输而修筑的各类水文观测站（井）房，各类水文信息接收处理与发布中心（站），水文缆道，测船码头，观测场地，监测井，监测标志，专用观测道路，水文通信、供电、供水、 排水系统，监测设备安装所必需的附属设施。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic monitoring facility</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_monitoring_for_emergency_response -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_monitoring_for_emergency_response">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Monitoring"/>
        <skos:definition xml:lang="zh-cn">在发生危害公众安全的涉水事件情况下，进行水文要素观测、 调查与分析等的工作。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic monitoring for emergency response</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_network_examination -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_network_examination">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StationNetworkManagement"/>
        <skos:definition xml:lang="zh-cn">按一定的原则和方法，对现有水文测站进行设站目的任务、在 站网整体功能中的作用等的检查和验证。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic network examination</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_observation_of_tidal_river -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_observation_of_tidal_river">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfTidalRiver"/>
        <skos:definition xml:lang="zh-cn">在感潮河段进行水文测验的全部作业。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic observation of tidal river</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_reconnaissance -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_reconnaissance">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Monitoring"/>
        <skos:definition xml:lang="zh-cn">在流域进行水文要素的观测或调查，收集水文及有关资料的 全部查勘及测量作业。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic reconnaissance</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_regime -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_regime">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">河流、湖泊、水库等水体各水文要素随时间的变化情况。包括 水位随时间的变化、一次洪水的流量过程、一年的流量过程、河川 径流量的年内和年际间的变化等。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic regime</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_regime_gauging_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_regime_gauging_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">只在汛期按规定报送水情的水文测站。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic regime gauging station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_regime_in_river -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_regime_in_river">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RiverRegime"/>
        <skos:definition xml:lang="zh-cn">河流的水位、流量等随时间的变化情势。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic regime in river</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_regionalization -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_regionalization">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StationNetwork"/>
        <skos:definition xml:lang="zh-cn">根据地区或流域的水文特征和自然地理条件所划分的不同水 文特征区域。在同一水文分区内，其各水体具有相似的水文状况 及变化规律。水文分区是水文站网规划和布设的基础。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic regionalization</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_series -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_series">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">水文变量按时间顺序排列所形成的数据系列。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic series</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_simulation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_simulation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicModel"/>
        <skos:definition xml:lang="zh-cn">对水文现象进行数学和逻辑的概化。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic simulation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_statistic_forecasting_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_statistic_forecasting_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FloodForecasting"/>
        <skos:definition xml:lang="zh-cn">根据概率论和数理统计的原理和方法，从大量历史水文、气象 资料中寻求水文要素随机变量的统计规律，建立随机水文模型进 行预报的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic statistic forecasting method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_statistics -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_statistics">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">用概率论、数理统计学原理和方法，研究水文变量随机规律及 其应用的技术。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic statistics</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_stochastic_analysis -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_stochastic_analysis">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">对既有确定性又有随机性水文现象分析研究的技术。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic stochastic analysis</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_year -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_year">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignAnnualRunoff"/>
        <skos:definition xml:lang="zh-cn">由当年枯季末至次年枯季末所经历的12个月时段，包括了一 年中水情的各个阶段的专用年度。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic year</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrologic_yearbook -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrologic_yearbook">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessing"/>
        <skos:definition xml:lang="zh-cn">按照统一的要求和规格并按流域水系统一编排卷册逐年刊印 的水文资料。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrologic yearbook</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrology -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrology">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#CommonTerms"/>
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrology"/>
        <skos:definition xml:lang="zh-cn">研究地球上水的形成、循环、时空分布、化学和物理性质以及 水与环境的相互关系的学科。</skos:definition>
        <skos:definition xml:lang="zh-cn">自然界中水的各种变化和运动等的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrology</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometeorologic_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometeorologic_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">进行水文和气象诸要素观测的水文测站。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometeorologic station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometeorology -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometeorology">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrology"/>
        <skos:definition xml:lang="zh-cn">研究水文循环和水分平衡中与降水、蒸发有关问题的学科，是 气象学与水文学之间的边缘学科。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometeorology</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometer -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometer">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysisDevices"/>
        <skos:definition xml:lang="zh-cn">用来测定液体密度的一种浮动式仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometer</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometer_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometer_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">沙样混勻连续在静水沉降过程中，用比重计测定其不同时刻 的浑液密度变化，来推求泥沙颗粒级配的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometer method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometric_boat -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometric_boat">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Device"/>
        <skos:definition xml:lang="zh-cn">配备水文测验设备，用来进行水文测验作业的专用船。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometric boat</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometric_bridge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometric_bridge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">进行水文测验作业的工作桥。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometric bridge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometric_cable_car -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometric_cable_car">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">悬吊在水文缆道行车上用来承载人员、设备并能在测量断面 任一垂线水面附近进行测验作业的设备。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometric cable car</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometric_cableway -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometric_cableway">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">为能将水文测验设备、仪器运送到测验断面内任一指定起点 距(水平位置）和垂线测点位置进行测验作业而架设的跨河索道 系统。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometric cableway</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometric_instrument -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometric_instrument">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InstrumentAndMeasuring"/>
        <skos:definition xml:lang="zh-cn">观测、记录、传输、处理水文要素的设备装置的统称。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometric instrument</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometric_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometric_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StationNetwork"/>
        <skos:definition xml:lang="zh-cn">为经常收集水文数据而在流域内的河、渠、湖、库或地表上设 立的各种水文观测场所的总称。按其作用，水文测站由国家基本 水文测站和各类专用水文测站组成。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometric station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometric_station_code -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometric_station_code">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">表征水文测站所在的流域（测验断面）位置、水文要素观测类 别及性质，便于计算机识别且由数字和字母组成的8位代码。 </skos:definition>
        <skos:prefLabel xml:lang="en">hydrometric station code</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometric_station_distribution_map -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometric_station_distribution_map">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessing"/>
        <skos:definition xml:lang="zh-cn">按一定的要求，将本流域或本地区的各种水文测站以不同符 号和统一的编号标绘在流域或地区相应位置上的地图。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometric station distribution map</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometric_station_evolution -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometric_station_evolution">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessing"/>
        <skos:definition xml:lang="zh-cn">说明测站的设立、停测、恢复、迁移，测站性质和类别的变动， 领导机关的转移等演化变革过程的总称。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometric station evolution</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometric_station_identification -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometric_station_identification">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">对水文测站有关水文测验的基本情况所作的查证和订正工 作，以作为选择水文资料整编方法和使用资料的依据。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometric station identification</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometric_station_network -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometric_station_network">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StationNetwork"/>
        <skos:definition xml:lang="zh-cn">在一定地区或流域内，按一定原则，用一定数量的各类水文测 站所构成的水文资料收集系统的总称。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometric station network</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometric_station_network_adjustment -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometric_station_network_adjustment">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StationNetworkManagement"/>
        <skos:definition xml:lang="zh-cn">根据水文业务技术和管理工作的需要，或受工程及其他人类活动影响，为使现有站网不断优化而进行的水文测站设立、升级、 降级、迁移、撤销、测报功能变更等工作。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometric station network adjustment</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometric_station_network_analysis -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometric_station_network_analysis">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StationNetworkManagement"/>
        <skos:definition xml:lang="zh-cn">为充分发挥站网整体功能，对现有站阿资料迸行的分析研究工作。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometric station network analysis</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometric_station_network_classification -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometric_station_network_classification">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StationNetwork"/>
        <skos:definition xml:lang="zh-cn">按照构成水文站网的水文测站之观测要素、报汛任务、布设位 置、测验工作模式等，对水文站网进行的类别划分工作。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometric station network classification</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometric_station_network_grade -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometric_station_network_grade">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StationNetwork"/>
        <skos:definition xml:lang="zh-cn">按照构成水文站网的水文测站之管理权限、服务目的和重要作用等，对水文站网的分档定级工作。水文站网分为国家基本站 网和专用站网。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometric station network grade</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometric_station_network_layout -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometric_station_network_layout">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StationNetworkManagement"/>
        <skos:definition xml:lang="zh-cn">按一定的原则和方法，经统一规划，对水文站网中的各类水文测站点进行的查勘、选址及具体定位工作。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometric station network layout</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometric_station_network_management -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometric_station_network_management">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StationNetwork"/>
        <skos:definition xml:lang="zh-cn">国家对水文站网中各类水文测站实行的分级分类规划、布设、测报功能分析评价、检验以及站网的优化和调整等工作。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometric station network management</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometric_station_network_optimization -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometric_station_network_optimization">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StationNetworkManagement"/>
        <skos:definition xml:lang="zh-cn">在一个地区或流域内，使水文站网能以较少的水文测站控制基本水文要素在时间和空间上的变化，且投资少，效率高，整体功 能强的分析工作。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometric station network optimization</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometric_station_network_planning -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometric_station_network_planning">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StationNetworkManagement"/>
        <skos:definition xml:lang="zh-cn">根据国家社会经济发展要求和水文业务技术需要，按照科学、经济、合理的原则，对国家、区域或流域各类水文站网中的水文测 站进行的总体布局工作。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometric station network planning</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometric_station_properties -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometric_station_properties">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">水文测站的水情变化规律、测验河段（监测区域）控制条件以 及水文要素间的关系特性之总称。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometric station properties</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometric_stationnetwork_density -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometric_stationnetwork_density">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StationNetwork"/>
        <skos:definition xml:lang="zh-cn">反映一个地区或流域内的水文测站数量多少的指标，以每站 平均控制面积或单位面积内站数来表示。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometric stationnetwork density</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometric_vehicle_for_tour_gauging -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometric_vehicle_for_tour_gauging">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Device"/>
        <skos:definition xml:lang="zh-cn">以巡回流动的方式，定期或不定期地对一个地区或流域内多 个水文测站进行水文要素观测的专用车。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometric vehicle for tour gauging</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometric_vehicle_on_bridge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometric_vehicle_on_bridge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Device"/>
        <skos:definition xml:lang="zh-cn">在桥上进行水文测验作业的专用车。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometric vehicle on bridge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometric_winch -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hydrometric_winch">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Device"/>
        <skos:definition xml:lang="zh-cn">把水文测验仪器设备送到指定测点位置而设置的专用机械传 动悬吊装置。按动力不同，分为手摇、电动和液压水文绞车；按应 用场合不同，分为船用、缆道和桥测水文绞车。</skos:definition>
        <skos:prefLabel xml:lang="en">hydrometric winch</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hydrometry -->

