        <skos:prefLabel xml:lang="en">tipping-bucket rainfall recorder</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tipping-bucketrainfall_sensor -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tipping-bucketrainfall_sensor">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservationDevices"/>
        <skos:definition xml:lang="zh-cn">以承雨翻斗交替翻转的次数计量雨量，发出相应翻斗翻转信 号的雨量传感器。</skos:definition>
        <skos:prefLabel xml:lang="en">tipping-bucketrainfall sensor</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#topographic_increasing_factor -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#topographic_increasing_factor">
