    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lower_confining_bed -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lower_confining_bed">
