        <skos:prefLabel xml:lang="en">tidal power resources</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tidal_prediction -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tidal_prediction">
