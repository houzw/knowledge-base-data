    <!-- http://www.egc.org/ont/domain/hydrology#ground_temperature -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ground_temperature">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
