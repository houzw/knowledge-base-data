    <!-- http://www.egc.org/ont/domain/hydrology#initial_and_receding_abstraction_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#initial_and_receding_abstraction_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignFlood"/>
        <skos:definition xml:lang="zh-cn">将实际的下渗过程简化为初损和后损两个阶段，初损是产流 前的总损失量，包括植物截留、填洼及产流前下渗的水量；后损为 流域产流后下渗的水量。</skos:definition>
        <skos:prefLabel xml:lang="en">initial and receding abstraction method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#initial_border_ice -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#initial_border_ice">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegime"/>
        <skos:definition xml:lang="zh-cn">初冬在岸边形成的薄冰带，气温升高往往就地融化或脱岸漂走。</skos:definition>
        <skos:prefLabel xml:lang="en">initial border ice</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#initial_condition -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#initial_condition">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">求解水动力学微分方程所需满足的一组开始时刻的条件方程。</skos:definition>
        <skos:prefLabel xml:lang="en">initial condition</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#initial_ice -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#initial_ice">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegime"/>
        <skos:definition xml:lang="zh-cn">在水面最早形成的薄冰。</skos:definition>
        <skos:prefLabel xml:lang="en">initial ice</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#initial_loss -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#initial_loss">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">产流前损失的降水量。</skos:definition>
        <skos:prefLabel xml:lang="en">initial loss</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#injection_cross-section -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#injection_cross-section">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">稀释法测流中，上游注人示踪剂溶液的断面。</skos:definition>
        <skos:prefLabel xml:lang="en">injection cross-section</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#injection_test -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#injection_test">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#GroundwaterAndSoilWaterExperiment"/>
        <skos:definition xml:lang="zh-cn">向井孔或洼地注水，同时测定水头和注水的关系，以分析水文 地质参数的试验。</skos:definition>
        <skos:prefLabel xml:lang="en">injection test</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#inlet -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#inlet">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:prefLabel xml:lang="en">inlet</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#input_of_water_vapor -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#input_of_water_vapor">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">一定时段通过一定区域边界输入的水汽量。</skos:definition>
        <skos:prefLabel xml:lang="en">input of water vapor</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#inspection_laboratory -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#inspection_laboratory">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentMonitoring"/>
        <skos:definition xml:lang="zh-cn">从事检验工作的实验室</skos:definition>
        <skos:prefLabel xml:lang="en">inspection laboratory</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#inspection_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#inspection_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentMonitoring"/>
        <skos:definition xml:lang="zh-cn">为进行检验而规定的技术程序。</skos:definition>
        <skos:prefLabel xml:lang="en">inspection method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#inspection_result -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#inspection_result">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentMonitoring"/>
        <skos:definition xml:lang="zh-cn">严格按照规定的测量方法所得的特性值。</skos:definition>
        <skos:prefLabel xml:lang="en">inspection result</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#instantaneous_sampler -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#instantaneous_sampler">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysisDevices"/>
        <skos:definition xml:lang="zh-cn">在河渠过水断面的预定测点，在瞬时内采集悬移质水样的仪 器，横式采样器属此类仪器.</skos:definition>
        <skos:prefLabel xml:lang="en">instantaneous sampler</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#instantaneous_unit_hydrograph -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#instantaneous_unit_hydrograph">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">净雨历时趋于无限小的单位线。</skos:definition>
        <skos:prefLabel xml:lang="en">instantaneous unit hydrograph</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#instream_water_use -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#instream_water_use">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SurveyAndAssessment"/>
        <skos:definition xml:lang="zh-cn">水力发电、航运、淡水养殖、冲沙、旅游、河道内环境等用户，只要求河流、水库、湖泊内有一定流量或水位的用水。</skos:definition>
        <skos:prefLabel xml:lang="en">instream water use</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#instrument_and_equipment_error -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#instrument_and_equipment_error">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
        <skos:definition xml:lang="zh-cn">观测时所使用的测量仪器、测具或其他设备，因制造精度所限而引起的误差。</skos:definition>
        <skos:prefLabel xml:lang="en">instrument and equipment error</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#instrument_constant -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#instrument_constant">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">与转子式流速仪的转子结构和旋转摩阻力有关的检定公式 常数。</skos:definition>
        <skos:prefLabel xml:lang="en">instrument constant</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#insulosity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#insulosity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">湖泊内所有岛屿总面积与湖泊面积的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">insulosity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#intake_flow_velocity_coefficient -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#intake_flow_velocity_coefficient">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">采样器测点处进水口的流速与相应天然流速的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">intake flow velocity coefficient</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#integer_programming -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#integer_programming">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterConservancyComputation"/>
        <skos:definition xml:lang="zh-cn">几个或全部变量在整数的约束之内的数学规划。</skos:definition>
        <skos:prefLabel xml:lang="en">integer programming</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#integrated_hydrologic_experiment -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#integrated_hydrologic_experiment">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">为两种以上实验目的而设立的具有特定意义或特殊作用的水 文实验。</skos:definition>
        <skos:prefLabel xml:lang="en">integrated hydrologic experiment</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#integrated_water_quality_model -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#integrated_water_quality_model">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#QualityAssessment"/>
        <skos:definition xml:lang="zh-cn">描述多个水质项目（组分)相互作用的数学表达式。</skos:definition>
        <skos:prefLabel xml:lang="en">integrated water quality model</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#interactive_forecast_system -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#interactive_forecast_system">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FloodForecasting"/>
        <skos:definition xml:lang="zh-cn">利用计算机网络技术，建立洪水预报及信息显示系统，通过人 机交互方式，实现预报模型参数自动优化及实时校正的洪水预报 系统。</skos:definition>
        <skos:prefLabel xml:lang="en">interactive forecast system</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#interbasin_water_transfer -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#interbasin_water_transfer">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterResources"/>
        <skos:definition xml:lang="zh-cn">通过工程措施将水资源较丰富流域的水调至水资源紧缺的流域。为防止水污染与可利用水量日益减少，保证人类生活和经济 发展对淡水需要，运用立法、行政、经济、技术等手段对水资源进行 管理的措施。</skos:definition>
        <skos:prefLabel xml:lang="en">interbasin water transfer</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#interception -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#interception">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">降水被植物枝叶拦截的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">interception</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#interference_added_drawdown -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#interference_added_drawdown">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">在相距较近的多个井（孔）中抽水时，其下降值与单井（孔）单 独抽水时的水位下降值之差。</skos:definition>
        <skos:prefLabel xml:lang="en">interference added drawdown</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#interflow -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#interflow">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <owl:sameAs rdf:resource="http://www.egc.org/ont/domain/hydrology#subsurface_flow"/>
        <skos:definition xml:lang="zh-cn">在土壤中相对不透水层界面上形成的一种水流。</skos:definition>
        <skos:prefLabel xml:lang="en">interflow</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#interlaboratory_comparison -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#interlaboratory_comparison">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentMonitoring"/>
        <skos:definition xml:lang="zh-cn">按照预先规定的条件，由两个或多个实验室对相同或类似检 测品指定值的实验结果对比。</skos:definition>
        <skos:prefLabel xml:lang="en">interlaboratory comparison</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#intermittent_gauging -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#intermittent_gauging">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Monitoring"/>
        <skos:definition xml:lang="zh-cn">水文测站资料经分析证明两水文要素如水位流量间历年关系 稳定，或其变化在允许误差范围内，对其中一要素如流量停测一段 时期后再行施测的测停相间的测验方法。</skos:definition>
        <skos:prefLabel xml:lang="en">intermittent gauging</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#intermittent_gauging_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#intermittent_gauging_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">对具备间测条件，并在年际间采取测、停相间的测验方法开展 水文要素测验工作的水文测站。</skos:definition>
        <skos:prefLabel xml:lang="en">intermittent gauging station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#intermittent_stream -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#intermittent_stream">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">干旱与半干旱地区，旱季河流干涸，雨季暴雨后或冰雪消融季 节才有短期水流流动的河流。</skos:definition>
        <skos:prefLabel xml:lang="en">intermittent stream</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#international_river -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#international_river">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <owl:sameAs rdf:resource="http://www.egc.org/ont/domain/hydrology#transboundary_river"/>
        <skos:definition xml:lang="zh-cn">通常指分隔或流经两个或两个以上国家的河流，即不是完全处于一个国境内的河流。有时特指通过条约规定对所有国家开放航行的流经多国的河流。</skos:definition>
        <skos:prefLabel xml:lang="en">international river</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#international_river_gauging_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#international_river_gauging_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">在出、人国境河流与国境界交汇处附近设置的水文测站。主 要任务是向出、入国境河流的邻国及有关国际组织通报汛情，或者 长期从事中华人民共和国与邻国的在跨界河流上的水文水资源监 测资料交换活动。</skos:definition>
        <skos:prefLabel xml:lang="en">international river gauging station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#intrusion_of_tidal_saltwater -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#intrusion_of_tidal_saltwater">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironment"/>
        <skos:definition xml:lang="zh-cn">感潮河段在涨潮时发生的海水上溯现象。</skos:definition>
        <skos:prefLabel xml:lang="en">intrusion of tidal saltwater</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#inverse_slope -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#inverse_slope">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RiverRegime"/>
        <skos:definition xml:lang="zh-cn">沿水流方向，单位水平距离的水面负高程差。</skos:definition>
        <skos:prefLabel xml:lang="en">inverse slope</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#inverse_thermal_stratification -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#inverse_thermal_stratification">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">在气温降至4°C以下湖水温度沿垂线分布，上层温度较低而 下层较高但不高于4°C的状况。</skos:definition>
        <skos:prefLabel xml:lang="en">inverse thermal stratification</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#investigated_flood -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#investigated_flood">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignFlood"/>
        <skos:definition xml:lang="zh-cn">通过现场调查、勘测、考证等手段获取的某次洪水。</skos:definition>
        <skos:prefLabel xml:lang="en">investigated flood</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#investigated_research_period -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#investigated_research_period">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignFlood"/>
        <skos:definition xml:lang="zh-cn">在实地勘查测算到若干可以定量的历史洪水的时期。</skos:definition>
        <skos:prefLabel xml:lang="en">investigated research period</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#investigation_data -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#investigation_data">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessing"/>
        <skos:definition xml:lang="zh-cn">采用勘测、调查、访问、考证等手段所获取的资料。</skos:definition>
        <skos:prefLabel xml:lang="en">investigation data</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#investigation_of_pollution_source -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#investigation_of_pollution_source">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#QualityAssessment"/>
        <skos:definition xml:lang="zh-cn">根据控制水污染、改善水环境质量的要求，对某一地区造成水 污染的原因进行的调查。</skos:definition>
        <skos:prefLabel xml:lang="en">investigation of pollution source</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#investigation_zone -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#investigation_zone">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicInvestigation"/>
        <skos:definition xml:lang="zh-cn">为便于水文调查工作开展和调查资料汇总，按一定规则将调 查流域划分成若干个调查的区域。</skos:definition>
        <skos:prefLabel xml:lang="en">investigation zone</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#irrigation_efficiency -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#irrigation_efficiency">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DevelopmentAndUtilization"/>
        <skos:definition xml:lang="zh-cn">灌入田间蓄积于土壤根系层中可供作物利用的水量与渠首相 应引水量的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">irrigation efficiency</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#irrigation_schedule -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#irrigation_schedule">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DevelopmentAndUtilization"/>
        <skos:definition xml:lang="zh-cn">按作物需水要求和不同灌水方法制订的灌水次数、每次灌水 的灌水时间和灌水定额以及灌溉定额的总称。</skos:definition>
        <skos:prefLabel xml:lang="en">irrigation schedule</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#irrigation_water_recharge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#irrigation_water_recharge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">灌溉渠道的渗漏以及田间灌溉水入渗补给地下水的过程。</skos:definition>
        <skos:prefLabel xml:lang="en">irrigation water recharge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#irrigation_water_use -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#irrigation_water_use">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SurveyAndAssessment"/>
        <skos:definition xml:lang="zh-cn">人工补充作物、林草正常生长的用水。</skos:definition>
        <skos:prefLabel xml:lang="en">irrigation water use</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#isochrone -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#isochrone">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">流域内经过一定的汇流时间同时到达出口断面的水质点在流 域上所处位置的连线。</skos:definition>
        <skos:prefLabel xml:lang="en">isochrone</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#isohyetal_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#isohyetal_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">用两相邻等雨量线雨量的平均值作为两雨量线之间的雨量， 以相应两相邻等雨量线间面积为权重来计算面雨量的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">isohyetal method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#isopercental_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#isopercental_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ProbableMaximumPrecipitationAndProbableMaximumFlood"/>
        <skos:definition xml:lang="zh-cn">当两地地形差异较大时，绘制时段雨量占该时段多年平均雨 量百分数等值线图，供进行暴雨移置综合改正的一种方法。</skos:definition>
        <skos:prefLabel xml:lang="en">isopercental method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#isopleth -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#isopleth">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">在某一时刻或时段，将水文要素值在空间上分布数值相同的 各点连接起来所形成的曲线。</skos:definition>
        <skos:prefLabel xml:lang="en">isopleth</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#isotope_hydrology -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#isotope_hydrology">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrology"/>
        <skos:definition xml:lang="zh-cn">用同位素测年与示踪方法研究天然水体的水循环过程的学科。</skos:definition>
        <skos:prefLabel xml:lang="en">isotope hydrology</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#item-by-item_investigation_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#item-by-item_investigation_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignAnnualRunoff"/>
        <skos:definition xml:lang="zh-cn">根据人类活动对河川径流的影响，分项目开展调查还原水量 的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">item-by-item investigation method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#jet_flow -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#jet_flow">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">水体从排放口喷出并进入另一流场内的一种水流。</skos:definition>
        <skos:prefLabel xml:lang="en">jet flow</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#juvenile_water -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#juvenile_water">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">地壳深处熔融的岩浆在上升过程中分异出水汽冷凝而成的水。</skos:definition>
        <skos:prefLabel xml:lang="en">juvenile water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#karst -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#karst">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">可溶性岩层长期被溶蚀以及由此而产生的各种地质地貌形态 的总称。</skos:definition>
        <skos:prefLabel xml:lang="en">karst</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#karst_aquifer -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#karst_aquifer">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">赋存地下水的可溶性岩层。</skos:definition>
        <skos:prefLabel xml:lang="en">karst aquifer</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#karst_cave -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#karst_cave">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">可溶性岩层被水溶蚀、破坏所形成的空洞穴的通称。</skos:definition>
        <skos:prefLabel xml:lang="en">karst cave</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#karst_ratio -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#karst_ratio">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">一定面积岩土层所测定的岩溶面与岩土层总面积的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">karst ratio</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#karst_water -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#karst_water">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">贮存于可溶性岩层溶隙（穴）中的地下水。</skos:definition>
        <skos:prefLabel xml:lang="en">karst water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#kinematic_wave -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#kinematic_wave">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">圣•维南方程组的动量方程中忽略惯性项和附加比降所描述 的洪水波。</skos:definition>
        <skos:prefLabel xml:lang="en">kinematic wave</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lacustrine_muck -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lacustrine_muck">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">湖泊沉积物经过交合作用形成的淤泥，是含无机物和有机物 微粒的组成体。</skos:definition>
        <skos:prefLabel xml:lang="en">lacustrine muck</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lag-and-route_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lag-and-route_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FloodForecasting"/>
        <skos:definition xml:lang="zh-cn">利用天然河道洪水波自上游向下游传播中的传播平移和调蓄滞后作用求得的汇流曲线来进行河道洪水演算的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">lag-and-route method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lake -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lake">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">陆地上的贮水洼地。由湖盆、湖水及其中所含物质组成的宽 阔水域的综合自然体。</skos:definition>
        <skos:prefLabel xml:lang="en">lake</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lake_basin -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lake_basin">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">注满湖水到一定水位下的陆地表面凹陷的盆地。</skos:definition>
        <skos:prefLabel xml:lang="en">lake basin</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lake_current -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lake_current">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">湖水沿一定方向的运动，根据成因可分为梯度流和漂流。</skos:definition>
        <skos:prefLabel xml:lang="en">lake current</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lake_ecosystem -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lake_ecosystem">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#AquaticEcosystern"/>
        <skos:definition xml:lang="zh-cn">湖泊生物群落与大气、湖水及湖底沉积物之间连续进行物质 交换和能量传递，形成结构复杂、功能协调的基本生态单元。</skos:definition>
        <skos:prefLabel xml:lang="en">lake ecosystem</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lake_gauging_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lake_gauging_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">在湖泊出口、进口或湖区进行水文要素观测而设置的水文测站。</skos:definition>
        <skos:prefLabel xml:lang="en">lake gauging station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lake_hydrologic_forecasting -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lake_hydrologic_forecasting">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">根据前期和现时水文、气象资料，对湖泊未来的水文情势所作 的预报。</skos:definition>
        <skos:prefLabel xml:lang="en">lake hydrologic forecasting</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lake_hydrology -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lake_hydrology">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrology"/>
        <owl:sameAs rdf:resource="http://www.egc.org/ont/domain/hydrology#limnology"/>
        <skos:definition xml:lang="zh-cn">研究湖泊和水库中水文现象的发生、发展和变化规律以及湖(库)水资源开发利用和保护的学科。</skos:definition>
        <skos:prefLabel xml:lang="en">lake hydrology</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lake_layering -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lake_layering">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">湖泊内不同水深的水质、水温、含沙量的分层现象。</skos:definition>
        <skos:prefLabel xml:lang="en">lake layering</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lake_ratio -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lake_ratio">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">流域内湖泊面积与流域面积的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">lake ratio</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lake_residence_period -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lake_residence_period">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">湖泊容积与年度出湖水量的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">lake residence period</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lake_resources -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lake_resources">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">湖泊的水资源、生物资源、矿产资源、旅游资源和气候资源等 的总称。</skos:definition>
        <skos:prefLabel xml:lang="en">lake resources</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lake_seiche -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lake_seiche">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">湖泊整个或局部水域周期性的摆动。</skos:definition>
        <skos:prefLabel xml:lang="en">lake seiche</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lake_stage_gauging_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lake_stage_gauging_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">设在湖泊区有代表性的地方进行水位观测的水文测站。</skos:definition>
        <skos:prefLabel xml:lang="en">lake stage gauging station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lake_water_quality_model -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lake_water_quality_model">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#QualityAssessment"/>
        <skos:definition xml:lang="zh-cn">描述湖泊水质的物理、化学和生物转化过程的数学表达式。</skos:definition>
        <skos:prefLabel xml:lang="en">lake water quality model</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lake_wave -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lake_wave">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">湖水质点在风力作用下围绕其平衡位置做周期性振动并沿一 定方向传播的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">lake wave</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lake_wind_denivellation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lake_wind_denivellation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">在风的切应力的作用下，使背风湖岸的水位降落和迎风湖岸 的水位增涨，使水面发生偏斜的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">lake wind denivellation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#land_evaporation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#land_evaporation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">流域内水面（冰雪)蒸发、土壤蒸发、散发(植物蒸腾）的总称。</skos:definition>
        <skos:prefLabel xml:lang="en">land evaporation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#land_subsidence -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#land_subsidence">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironment"/>
        <skos:definition xml:lang="zh-cn">在自然或人为因素作用下，某一范围的地表面在垂直方向发 生的高程降低的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">land subsidence</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#land_surface -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#land_surface">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">流域中除水系以外的陆域部分。</skos:definition>
        <skos:prefLabel xml:lang="en">land surface</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#large_evaporation_pool -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#large_evaporation_pool">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservationDevices"/>
        <skos:definition xml:lang="zh-cn">面积超过20m&lt;sup&gt;2&lt;/sup&gt;的水面漂浮、陆上蒸发池。</skos:definition>
        <skos:prefLabel xml:lang="en">large evaporation pool</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#large_flood -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#large_flood">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">重现期大于或等于20年，小于50年的洪水。</skos:definition>
        <skos:prefLabel xml:lang="en">large flood</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#laser_current_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#laser_current_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">应用激光多普勒频移原理，测量水中点流速的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">laser current meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#laser_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#laser_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">利用激光散射测量颗粒级配的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">laser method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#laser_particle_size_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#laser_particle_size_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysisDevices"/>
        <skos:definition xml:lang="zh-cn"> 激光照射到颗粒样品后，发生衍散射现象，散射角与颗粒直径 成反比关系，散射光的能量分布与颗粒直径的分布直接相关，通过 测量散射光的能量分布就可以得到颗粒直径分布的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">laser particle size meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#laser_stage_recorder -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#laser_stage_recorder">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StageWaterTemporatureAndWaveObservationDevices"/>
        <skos:definition xml:lang="zh-cn">将激光发射、接收器安装在水上某一已知高程位置，通过测量激光脉冲从发射、经水面反射到被接收的时间，计算记录水位的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">laser stage recorder</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lateral_recharge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lateral_recharge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn"> 侧向径流流入补给地下水的过程。</skos:definition>
        <skos:prefLabel xml:lang="en">lateral recharge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#latest_frost -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#latest_frost">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">每年入秋至第二年春末最后一次出现的霜。</skos:definition>
        <skos:prefLabel xml:lang="en">latest frost</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lead -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lead">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegime"/>
        <skos:definition xml:lang="zh-cn">封冻期间，河流中未冻结的狭长水沟。</skos:definition>
        <skos:prefLabel xml:lang="en">lead</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#leakage_coefficient -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#leakage_coefficient">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">弱透水层在垂直方向上传输越流水量能力的参数。</skos:definition>
        <skos:prefLabel xml:lang="en">leakage coefficient</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#leakage_recharge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#leakage_recharge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">在水头差作用下，相邻含水层通过上覆和下卧的弱透水层产 生垂直渗透补给含水层的过程。</skos:definition>
        <skos:prefLabel xml:lang="en">leakage recharge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#least_square_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#least_square_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">在残差满足V&lt;sup&gt;T&lt;/sup&gt;PV为最小的条件下，解算测量估值或参数估 值并进行精度估算的方法。其中V为残差向量，P为其权矩阵。</skos:definition>
        <skos:prefLabel xml:lang="en">least square method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#left_bank -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#left_bank">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">面向下游时河流的左侧边界。</skos:definition>
        <skos:prefLabel xml:lang="en">left bank</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#length_of_lake -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#length_of_lake">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">湖面的湖岸线上相距最远两点之间的直线距离。</skos:definition>
        <skos:prefLabel xml:lang="en">length of lake</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#length_of_lake_shoreline -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#length_of_lake_shoreline">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">湖面周边线的长度。</skos:definition>
        <skos:prefLabel xml:lang="en">length of lake shoreline</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#levee -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#levee">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">沿河、渠、湖、海岸边或行洪区、分洪区、围垦区的边缘修筑的 挡水建筑物。</skos:definition>
        <skos:prefLabel xml:lang="en">levee</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#level_survey -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#level_survey">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">用水准仪和水准标尺测定地面两点间高差的作业。</skos:definition>
        <skos:prefLabel xml:lang="en">level survey</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#limnology -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#limnology">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrology"/>
        <skos:prefLabel xml:lang="en">limnology</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#linear_channel -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#linear_channel">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">模拟洪水波运动，只有传播没有衰减和变形的概念性渠道。</skos:definition>
        <skos:prefLabel xml:lang="en">linear channel</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#linear_hydrologic_model -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#linear_hydrologic_model">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicModel"/>
        <skos:definition xml:lang="zh-cn">模型的解可以线性叠加而且保持比例因子不变的水文数学模型。</skos:definition>
        <skos:prefLabel xml:lang="en">linear hydrologic model</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#linear_programming -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#linear_programming">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterConservancyComputation"/>
        <skos:definition xml:lang="zh-cn">目标函数方程和全部约束方程都是线性方程的数学规划。</skos:definition>
        <skos:prefLabel xml:lang="en">linear programming</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#linear_reservoir -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#linear_reservoir">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">模拟洪水波运动，蓄量与出流量成正比的概念性水库。</skos:definition>
        <skos:prefLabel xml:lang="en">linear reservoir</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#local_flood -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#local_flood">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">局部地区发生较大以上量级的洪水。</skos:definition>
        <skos:prefLabel xml:lang="en">local flood</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#local_flood-reporting_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#local_flood-reporting_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">根据水文情报预报任务要求，承担着向省级及其以下防汛决 策机构报告河流、湖泊、水库和其他水体汛情任务的各类水文 测站。</skos:definition>
        <skos:prefLabel xml:lang="en">local flood-reporting station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#local_head_loss -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#local_head_loss">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RiverRegime"/>
        <skos:definition xml:lang="zh-cn">水流流动过程中，由于局部地区几何边界的急剧改变导致水 流急剧调整而损耗的能量。</skos:definition>
        <skos:prefLabel xml:lang="en">local head loss</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#local_scour -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#local_scour">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Sediment"/>
        <skos:definition xml:lang="zh-cn">河流因单宽流量增加或局部水流条件改变引起的较小范围河 床冲刷过程。</skos:definition>
        <skos:prefLabel xml:lang="en">local scour</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#local_storm_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#local_storm_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ProbableMaximumPrecipitationAndProbableMaximumFlood"/>
        <skos:definition xml:lang="zh-cn">用设计流域内实测特大暴雨为典型，将影响降水的水汽、动力 因子进行放大，推求可能最大降水的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">local storm method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#log-normal_distribution -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#log-normal_distribution">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">随机变量的对数呈正态的频率分布。</skos:definition>
        <skos:prefLabel xml:lang="en">log-normal distribution</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#long-_and_mid-term_planning_of_water_supply_and_demand -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#long-_and_mid-term_planning_of_water_supply_and_demand">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterResourcesPlanning"/>
        <skos:definition xml:lang="zh-cn">国家或地方政府用于安排辖区内中期和长期水资源开发、利 用的技术性指导文件。</skos:definition>
        <skos:prefLabel xml:lang="en">long- and mid-term planning of water supply and demand</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#long-term_rainfall_recorder -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#long-term_rainfall_recorder">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservationDevices"/>
        <skos:definition xml:lang="zh-cn">能长期连续运行的雨量计，记录周期常在一个月以上。 </skos:definition>
        <skos:prefLabel xml:lang="en">long-term rainfall recorder</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#longitudinal_profile -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#longitudinal_profile">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DepthAndCrossSectionMeasurement"/>
        <skos:definition xml:lang="zh-cn">沿水流方向的水面和河床纵向剖面。</skos:definition>
        <skos:prefLabel xml:lang="en">longitudinal profile</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#loop_cable -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#loop_cable">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <owl:sameAs rdf:resource="http://www.egc.org/ont/domain/hydrology#tow_cable"/>
        <skos:definition xml:lang="zh-cn">水文缆道系统中由岸上绞车控制，将仪器与行车沿断面水平 和铅直方向移动及定位的活动钢索。</skos:definition>
        <skos:prefLabel xml:lang="en">loop cable</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#loop_curve -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#loop_curve">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">一次洪水过程中的水位流量关系点依时间顺序所连成的绳套 形曲线。</skos:definition>
        <skos:prefLabel xml:lang="en">loop curve</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#losing_stream -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#losing_stream">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">通过地下补给外流域水量的河流。</skos:definition>
        <skos:prefLabel xml:lang="en">losing stream</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#loss_of_wetness -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#loss_of_wetness">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">进入雨量计承雨口的降雨在流入翻斗等雨量计量部件前被承 雨口等内壁吸附，而没有得到计量的部分雨量。</skos:definition>
        <skos:prefLabel xml:lang="en">loss of wetness</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#low-flow_investigation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#low-flow_investigation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicInvestigation"/>
        <skos:definition xml:lang="zh-cn">对未实测到的河流最低水位和最小流量的调查工作。</skos:definition>
        <skos:prefLabel xml:lang="en">low-flow investigation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#low-flow_period -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#low-flow_period">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignAnnualRunoff"/>
        <skos:definition xml:lang="zh-cn">年内河川流量显著低于年平均流量的时期。</skos:definition>
        <skos:prefLabel xml:lang="en">low-flow period</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#low-flow_river_bed -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#low-flow_river_bed">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">在低水时期（枯水期）存在有水流动的河床部分。</skos:definition>
        <skos:prefLabel xml:lang="en">low-flow river bed</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#low-flow_year -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#low-flow_year">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignAnnualRunoff"/>
        <skos:definition xml:lang="zh-cn">年降水量或年河川径流量显著小于正常值的年份。</skos:definition>
        <skos:prefLabel xml:lang="en">low-flow year</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#low-level_jet_stream -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#low-level_jet_stream">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">对流层下部距地面1000m、4000m的一支低空的强风带。 </skos:definition>
        <skos:prefLabel xml:lang="en">low-level jet stream</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#low_level_mire -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#low_level_mire">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Mire"/>
        <skos:definition xml:lang="zh-cn">地表水、地下水补给丰富，泥炭灰分含量一般达18%，植物所 需水分较多，莎草科植物占优势的沼泽。</skos:definition>
        <skos:prefLabel xml:lang="en">low level mire</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#low_lunitidal_interval -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#low_lunitidal_interval">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">当地月中天与随后出现的低潮的时间间隔。</skos:definition>
        <skos:prefLabel xml:lang="en">low lunitidal interval</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#low_tide -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#low_tide">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
