    <!-- http://www.egc.org/ont/domain/hydrology#probability -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#probability">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
