    <!-- http://www.egc.org/ont/domain/hydrology#Pitot_tube -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#Pitot_tube">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">通过量测垂直弯管上两个小孔之间的压差来推算流速的器件。</skos:definition>
        <skos:prefLabel xml:lang="en">Pitot tube</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#Poisson_process -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#Poisson_process">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">一种随机事件累计发生次数的最基本的独立增量过程。</skos:definition>
        <skos:prefLabel xml:lang="en">Poisson process</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#Priestley-Taylor -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#Priestley-Taylor">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ETMethod"/>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#Rt_reliability -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#Rt_reliability">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InstrumentAndMeasuring"/>
        <skos:definition xml:lang="zh-cn"> R(t)产品在规定条件下和规定时间内，完成规定功能的概率。</skos:definition>
        <skos:prefLabel xml:lang="en">Rt reliability</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#S-curve -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#S-curve">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">单位线各时段累积流量和时间的关系曲线，因其形似</skos:definition>
        <skos:prefLabel xml:lang="en">S-curve</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#SCS_Curve_Number -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#SCS_Curve_Number">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffMethod"/>
        <skos:prefLabel xml:lang="en">SCS Curve Number</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#Stahler_network_order -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#Stahler_network_order">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:prefLabel xml:lang="en">Stahler network order</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#Thiessen_polygon -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#Thiessen_polygon">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">由相邻雨量站连线的垂直平分线所构成的多边形，其面积作 为面雨量计算中点雨量的权重。</skos:definition>
        <skos:prefLabel xml:lang="en">Thiessen polygon</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#Variable_Storage -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#Variable_Storage">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RoutingMethod"/>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#Venturi_flow_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#Venturi_flow_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">通过量测收缩管段与进口管道之间的压差来推算管内流量的一种装置。</skos:definition>
        <skos:prefLabel xml:lang="en">Venturi flow meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#Venturi_flume -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#Venturi_flume">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">含有收缩段的一种测流槽。通过观测缩颈上游和缩颈处或缩颈下游两处水位，便可推算流量。</skos:definition>
        <skos:prefLabel xml:lang="en">Venturi flume</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ablation_area -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ablation_area">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Glacier"/>
        <skos:definition xml:lang="zh-cn">冰川平衡线以下物质平衡为负的部分。</skos:definition>
        <skos:prefLabel xml:lang="en">ablation area</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#absolute_datum -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#absolute_datum">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">将某一海滨地点平均海平面的高程定为零的水准基面。</skos:definition>
        <skos:prefLabel xml:lang="en">absolute datum</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#absolute_error -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#absolute_error">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
        <skos:definition xml:lang="zh-cn">测量值与真值之差。</skos:definition>
        <skos:prefLabel xml:lang="en">absolute error</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#absolute_humidity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#absolute_humidity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">单位容积空气中含有的水汽质量，即空气中的水汽密度，以 g/ma为单位。它是空气水汽绝对含量的一种度量。</skos:definition>
        <skos:prefLabel xml:lang="en">absolute humidity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#accumulated_sediment_weig_percent -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#accumulated_sediment_weig_percent">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">泥沙颗粒级配小于或大于某粒径的累积沙重百分数。</skos:definition>
        <skos:prefLabel xml:lang="en">accumulated sediment weig percent</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#accumulated_stream_source -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#accumulated_stream_source">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:prefLabel xml:lang="en">accumulated stream source</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#accumulation_area -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#accumulation_area">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Glacier"/>
        <skos:definition xml:lang="zh-cn">冰川平衡线以上物质平衡为正的部分。 </skos:definition>
        <skos:prefLabel xml:lang="en">accumulation area</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#accumulation_of_melt_water -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#accumulation_of_melt_water">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegime"/>
        <skos:definition xml:lang="zh-cn">冰面上大面积水洼的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">accumulation of melt water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#accumulative_rain_gauge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#accumulative_rain_gauge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservationDevices"/>
        <skos:definition xml:lang="zh-cn">无人驻守的雨量站所使用的雨量器，它配备有融雪用的防冻 剂和防止水蒸发的油液，可测量较长时间的雨雪降水总量。</skos:definition>
        <skos:prefLabel xml:lang="en">accumulative rain gauge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#accuracy -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#accuracy">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
        <skos:definition xml:lang="zh-cn">测量值与真值接近的程度，反映了随机误差与系统误差的综 合结果。</skos:definition>
        <skos:prefLabel xml:lang="en">accuracy</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#accuracy_standard -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#accuracy_standard">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">评定预报方案精度的标准。</skos:definition>
        <skos:prefLabel xml:lang="en">accuracy standard</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#acid_deposition -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#acid_deposition">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">大气中酸性污染物的自然沉降，分为干沉降和湿沉降。</skos:definition>
        <skos:prefLabel xml:lang="en">acid deposition</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#acidity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#acidity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">水介质与氢氧根离子反应的定量能力。</skos:definition>
        <skos:prefLabel xml:lang="en">acidity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#acoustic_Doppler_current_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#acoustic_Doppler_current_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">向水中发射声波，测量仪器所在点随水流运动的悬浮粒子反 射声波的多普勒频移来确定该点水流速度的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">acoustic Doppler current meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#acoustic_Doppler_current_profile -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#acoustic_Doppler_current_profile">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <props:hasAbbreviation>ADCP</props:hasAbbreviation>
        <skos:definition xml:lang="zh-cn">利用超声波回波的多普勒频移直接测出断面流速分布的流速仪。应用一些其他测量数据，可以自动得到流量。</skos:definition>
        <skos:prefLabel xml:lang="en">acoustic Doppler current profile</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#acoustic_Doppler_current_profile_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#acoustic_Doppler_current_profile_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">利用声学多普勒剖面流速仪进行流速或流量测验的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">acoustic Doppler current profile method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#acoustic_Doppler_flow_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#acoustic_Doppler_flow_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">安装在水底，应用声学多普勒剖面流速仪原理和超声测深原 理，同时测得垂线流速分布和水深，根据断面资料，自动计算得到 流速、流量的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">acoustic Doppler flow meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#acoustic_Doppler_flowdirection_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#acoustic_Doppler_flowdirection_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">可以测量流向的声学多普勒流速仪，主要包括走航式多普勒 剖面流速仪、一些声学多普勒点流速仪。</skos:definition>
        <skos:prefLabel xml:lang="en">acoustic Doppler flowdirection meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#acoustic_Doppler_pipe_flow_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#acoustic_Doppler_pipe_flow_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">应用声学多普勒原理测量管内流速的管道流量计。</skos:definition>
        <skos:prefLabel xml:lang="en">acoustic Doppler pipe flow meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#acoustic_current_meter_by_time_difference_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#acoustic_current_meter_by_time_difference_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">测量超声波在水中传播时顺水和逆水的传播时间差，由此测 得水层平均流速的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">acoustic current meter by time difference method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#acoustic_evaporimeter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#acoustic_evaporimeter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservationDevices"/>
        <skos:definition xml:lang="zh-cn">&gt;用超声波测量水位的方法自动测量蒸发桶内的水位，得到蒸 发量的仪器。可以自动记录、遥测传输蒸发量。</skos:definition>
        <skos:prefLabel xml:lang="en">acoustic evaporimeter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#acoustic_flow_measurement_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#acoustic_flow_measurement_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">应用声波（超声波）测量水流流速、流量的方法统称。主要应 用时差法、多普勒法两种原理。</skos:definition>
        <skos:prefLabel xml:lang="en">acoustic flow measurement method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#acoustic_flow_measuring_system_by_time_difference_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#acoustic_flow_measuring_system_by_time_difference_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">声学时差法流速仪和水位测量传感器同时工作，测得流速、水位，根据断面资料，自动得到流量，构成时差法流量测量系统。</skos:definition>
        <skos:prefLabel xml:lang="en">acoustic flow measuring system by time difference method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#acoustic_ice_thickness_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#acoustic_ice_thickness_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegimeObservationDevices"/>
        <skos:definition xml:lang="zh-cn">应用超声波测厚原理测量冰厚的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">acoustic ice thickness meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#acoustic_particle_size_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#acoustic_particle_size_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysisDevices"/>
        <skos:definition xml:lang="zh-cn">应用不同粒径泥沙对相应频率超声波的衰减性能不同，测量 泥沙颗粒直径分布的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">acoustic particle size meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#acoustic_pipe_flow_meter_by_time_difference_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#acoustic_pipe_flow_meter_by_time_difference_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">应用声学时差法原理测量管内流速的管道流量计。</skos:definition>
        <skos:prefLabel xml:lang="en">acoustic pipe flow meter by time difference method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#acoustic_sediment_concentration_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#acoustic_sediment_concentration_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysisDevices"/>
        <skos:definition xml:lang="zh-cn">应用超声波穿过含沙量不同的水体，其强度衰减不同的原理， 测量含沙量的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">acoustic sediment concentration meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#acoustic_snow_gauge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#acoustic_snow_gauge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservationDevices"/>
        <skos:definition xml:lang="zh-cn">应用超声测量原理测量积雪厚度的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">acoustic snow gauge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#actual_surface_drainage_area -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#actual_surface_drainage_area">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicInvestigation"/>
        <skos:definition xml:lang="zh-cn">在地形图上量算的岩溶地区地表集水面积，减去本流域经地 下暗河流入外流域的闭流区面积，加上外流域经地下暗河流入本 流域的闭流区面积所得到的集水面积。</skos:definition>
        <skos:prefLabel xml:lang="en">actual surface drainage area</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#additional_slope -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#additional_slope">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RiverRegime"/>
        <skos:definition xml:lang="zh-cn">洪水波水面比降与同水位下恒定流水面比降之差。</skos:definition>
        <skos:prefLabel xml:lang="en">additional slope</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#adjusting_factor_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#adjusting_factor_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">水位流量关系点据呈单式绳套状分布，采用以涨落率为主要 参数来校正测验河段受洪水涨落影响时所建立的一组曲线，用水 位推求流量的一种方法。</skos:definition>
        <skos:prefLabel xml:lang="en">adjusting factor method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#adjustment -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#adjustment">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
        <skos:definition xml:lang="zh-cn">利用最小二乘原理估算测量的最佳估值或最或然值的方法。 </skos:definition>
        <skos:prefLabel xml:lang="en">adjustment</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#adjustment_factor_of_frazil_slush -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#adjustment_factor_of_frazil_slush">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegimeObservation"/>
        <skos:definition xml:lang="zh-cn">平均冰花密度与密实冰块密度的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">adjustment factor of frazil slush</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#aeration_zone -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#aeration_zone">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">地面以下潜水面以上与大气相通的地带。</skos:definition>
        <skos:prefLabel xml:lang="en">aeration zone</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#agglomerated_border_ice -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#agglomerated_border_ice">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegime"/>
        <skos:definition xml:lang="zh-cn">受风和水流作用，流动冰花或冰块被冲到岸边或与岸冰边冻 结而成的冰带，其表面常不平整。</skos:definition>
        <skos:prefLabel xml:lang="en">agglomerated border ice</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#agricultural_drought -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#agricultural_drought">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">因降水或供水不足导致农田水量供需不平衡，阻碍农作物正 常生长发育的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">agricultural drought</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#agricultural_hydrology -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#agricultural_hydrology">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrology"/>
        <skos:definition xml:lang="zh-cn">研究水分-土壤-植物系统中与作物生长有关的水文问题以及 植物散发和土壤水的运动规律的学科。</skos:definition>
        <skos:prefLabel xml:lang="en">agricultural hydrology</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#agricultural_sewage -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#agricultural_sewage">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">农田径流、农业灌溉、牲畜饲养及农产品加工等过程排放的污水。</skos:definition>
        <skos:prefLabel xml:lang="en">agricultural sewage</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#air_line_correction -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#air_line_correction">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DepthAndCrossSectionMeasurement"/>
        <skos:definition xml:lang="zh-cn">用悬索悬吊铅鱼或测深锤测深时，对水面以上悬索偏斜影响 所作的改正，即悬索支点到水面的垂直距离与该部分实测悬索长 度的差值。</skos:definition>
        <skos:prefLabel xml:lang="en">air line correction</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#air_mass -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#air_mass">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">温度、湿度和大气静力稳定度等物理属性水平分布比较均匀 的大范围气块。</skos:definition>
        <skos:prefLabel xml:lang="en">air mass</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#air_medium_ultrasonic_stage_recorder -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#air_medium_ultrasonic_stage_recorder">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StageWaterTemporatureAndWaveObservationDevices"/>
        <skos:definition xml:lang="zh-cn">换能器安装在水上，以空气作为超声波传播介质的超声波水 位计。</skos:definition>
        <skos:prefLabel xml:lang="en">air medium ultrasonic stage recorder</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#air_temperature -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#air_temperature">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">空气的温度。我国气象站所测气温，指百叶箱中离地面1. 5m 高处的温度表所测得的空气温度。</skos:definition>
        <skos:prefLabel xml:lang="en">air temperature</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#algal_blooms -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#algal_blooms">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydroecology"/>
        <skos:definition xml:lang="zh-cn">藻类在河流、湖泊水体中异常繁殖并大面积覆盖水面的一种 现象，水体呈蓝、绿或暗褐色，是淡水水体富营养化的一种特征。</skos:definition>
        <skos:prefLabel xml:lang="en">algal blooms</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#alkalinity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#alkalinity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">水介质与氢离子反应的定量能力。</skos:definition>
        <skos:prefLabel xml:lang="en">alkalinity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#allowable_groundwater_exploitation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#allowable_groundwater_exploitation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">通过技术经济合理的取水方案，在整个开采区内动水位不低于设计值，出水量、水质和水温变化在允许范围内，不影响已建水源的正常开采，不发生危害性的环境地质现象的前提下，单位时间 内从水文地质单元或取水地段中能够取得的水量。</skos:definition>
        <skos:prefLabel xml:lang="en">allowable groundwater exploitation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#alternating_current -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#alternating_current">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <owl:sameAs rdf:resource="http://www.egc.org/ont/domain/hydrology#reversing_current"/>
        <skos:definition xml:lang="zh-cn">受地形的限制，水质点大致在一个水平方向做往复运动的 潮流。</skos:definition>
        <skos:prefLabel xml:lang="en">alternating current</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#amount_of_wastewater_discharge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#amount_of_wastewater_discharge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#QualityAssessment"/>
        <skos:definition xml:lang="zh-cn">第二产业、第三产业和城镇居民生活等用水户排放的已被污染的水量，不包括火电直流冷却水排放量和矿坑排水量。</skos:definition>
        <skos:prefLabel xml:lang="en">amount of wastewater discharge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#analogue_model -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#analogue_model">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicModel"/>
        <skos:definition xml:lang="zh-cn">用另一种物理量来比拟水文现象的某些特性的模型。</skos:definition>
        <skos:prefLabel xml:lang="en">analogue model</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#analogy_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#analogy_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignFlood"/>
        <skos:definition xml:lang="zh-cn">当设计流域既没有流量资料又没有暴雨资料时，采用地区综 合推求设计洪水的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">analogy method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#anchor_ice -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#anchor_ice">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegime"/>
        <skos:definition xml:lang="zh-cn">水面以下冻结于河底或建筑物上的冰。</skos:definition>
        <skos:prefLabel xml:lang="en">anchor ice</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#annual_exceedance_series -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#annual_exceedance_series">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">由每年超过一定量级的数据所组成的水文系列。</skos:definition>
        <skos:prefLabel xml:lang="en">annual exceedance series</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#antecedent_and_subsequent_flow_correlation_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#antecedent_and_subsequent_flow_correlation_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Low-flowForecasting"/>
        <skos:definition xml:lang="zh-cn">对于枯季降水量很小，枯季地下径流量补给较稳定的流域，建立前期和后期流量的关系预报枯季来水的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">antecedent and subsequent flow correlation method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#antecedent_rainfall -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#antecedent_rainfall">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">用流域前期降雨量折减值表示流域前期土壤含水量的指标。</skos:definition>
        <skos:prefLabel xml:lang="en">antecedent rainfall</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#anticyclone -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#anticyclone">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">大气流场中，在北（南）半球呈顺逆时针旋转的大型涡旋，在 同一高度上，反气旋中心的气压比周围高。</skos:definition>
        <skos:prefLabel xml:lang="en">anticyclone</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#apparatus-sampling_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#apparatus-sampling_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">用仪器采集推移质样品乘以效率系数，以推求推移质输沙率 的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">apparatus-sampling method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#applied_hydrology -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#applied_hydrology">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrology"/>
        <skos:definition xml:lang="zh-cn">运用水文学及有关学科的理论和方法，研究解决实际水文问 题的途径和方法，为相关行业提供水文数据、水文参数和水文预报 服务的学科。</skos:definition>
        <skos:prefLabel xml:lang="en">applied hydrology</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#applied_rainstorm_runoff_charts -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#applied_rainstorm_runoff_charts">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">用于无资料或资料不足的中小流域设计暴雨及其洪水计算的图表。</skos:definition>
        <skos:prefLabel xml:lang="en">applied rainstorm runoff charts</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#approach_channel -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#approach_channel">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">水流呈缓流状况，或弗汝德数小于0. 5，有足够顺直段长，适 宜于测量要求的邻近测流建筑物或水工建筑物上游的一段河槽。</skos:definition>
        <skos:prefLabel xml:lang="en">approach channel</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#approach_velocity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#approach_velocity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">行近河槽内某断面的平均流速。</skos:definition>
        <skos:prefLabel xml:lang="en">approach velocity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#aquatic_ecology_monitoring -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#aquatic_ecology_monitoring">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#AquaticEcologyMonitoring"/>
        <skos:definition xml:lang="zh-cn">通过对水生生物、水文要素、水环境质量等的监测和数据收 集，分析评价水生态的现状和变化，为水生态系统保护与修复提供 依据的活动。</skos:definition>
        <skos:prefLabel xml:lang="en">aquatic ecology monitoring</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#aquatic_ecosystem -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#aquatic_ecosystem">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#AquaticEcosystern"/>
        <skos:definition xml:lang="zh-cn">水生生物群落与水环境相互作用、相互制约，通过物质循环和 能量流动，共同构成的具有一定结构和功能的动态平衡系统。分 为淡水生态系统和海洋生态系统。</skos:definition>
        <skos:prefLabel xml:lang="en">aquatic ecosystem</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#aquatic_organism -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#aquatic_organism">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydroecology"/>
        <skos:definition xml:lang="zh-cn">全部或部分生活在各种水域中的动物和植物，包括淡水生物 和海洋生物。</skos:definition>
        <skos:prefLabel xml:lang="en">aquatic organism</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#aquatic_organism_monitoring -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#aquatic_organism_monitoring">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#AquaticEcologyMonitoring"/>
        <skos:definition xml:lang="zh-cn">对水体中水生生物的种群、个体数量、生理功能或群落结构变 化所进行的测定。</skos:definition>
        <skos:prefLabel xml:lang="en">aquatic organism monitoring</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#aquifer -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#aquifer">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">能够贮存、渗透的饱水岩土层。</skos:definition>
        <skos:prefLabel xml:lang="en">aquifer</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#aquifer_boundary -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#aquifer_boundary">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">含水层具有统一水力特性的边界。</skos:definition>
        <skos:prefLabel xml:lang="en">aquifer boundary</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#aquifuge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#aquifuge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <owl:sameAs rdf:resource="http://www.egc.org/ont/domain/hydrology#confining_bed"/>
        <skos:prefLabel xml:lang="en">aquifuge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#aquitard -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#aquitard">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">结构较致密，透水性较弱，储水能力较小的岩土层。</skos:definition>
        <skos:prefLabel xml:lang="en">aquitard</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#aquitard_boundary -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#aquitard_boundary">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">渗透性较弱的含水层边界。</skos:definition>
        <skos:prefLabel xml:lang="en">aquitard boundary</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#arbitrary_datum -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#arbitrary_datum">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">为计算水文测站水位或高程而假定的水准基面。常在测站附 近没有国家水准点，或者一时不具备接测条件的情况下使用。</skos:definition>
        <skos:prefLabel xml:lang="en">arbitrary datum</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#area_of_lake -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#area_of_lake">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">湖泊在一定水位时的水面面积。</skos:definition>
        <skos:prefLabel xml:lang="en">area of lake</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#area_of_runoff_yield -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#area_of_runoff_yield">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">由降雨和融雪在流域内产生径流的面积。</skos:definition>
        <skos:prefLabel xml:lang="en">area of runoff yield</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#area_rainfall -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#area_rainfall">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">某一时段内一定面积上的平均雨量。</skos:definition>
        <skos:prefLabel xml:lang="en">area rainfall</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#argumentation_on_influencing_monitoring_setting -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#argumentation_on_influencing_monitoring_setting">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#MonitoringSettings"/>
        <skos:definition xml:lang="zh-cn">针对水文监测环境内或其上下游的地面构筑物以及人为活动 等对水文测报工作的影响，组织幵展的影响分析评价和解决影响 问题的方案论证工作。</skos:definition>
        <skos:prefLabel xml:lang="en">argumentation on influencing monitoring setting</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#aridity_index -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#aridity_index">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">一个地区某一时段水面蒸发量与降水量的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">aridity index</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#arithmetic_mean_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#arithmetic_mean_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">每个测量值之和与测量值个数的比值，是计算平均值的一种 方法。</skos:definition>
        <skos:prefLabel xml:lang="en">arithmetic mean method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#armoured_cable -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#armoured_cable">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Device"/>
        <skos:definition xml:lang="zh-cn">集悬吊、传信功能于一体，芯内装有传输信号电缆的可承重钢 丝绳。</skos:definition>
        <skos:prefLabel xml:lang="en">armoured cable</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#artesian_well -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#artesian_well">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">承压水位高于当地地面自行喷出或溢出地表的井。</skos:definition>
        <skos:prefLabel xml:lang="en">artesian well</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#artificial_control -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#artificial_control">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">通过工程措施使水位流量关系稳定形成的测站控制。</skos:definition>
        <skos:prefLabel xml:lang="en">artificial control</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#artificial_drainage_network -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#artificial_drainage_network">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">由人工构筑的运河、渠道或经整治的旧河道形成主支流纵横 交错的网状系统。</skos:definition>
        <skos:prefLabel xml:lang="en">artificial drainage network</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#artificial_error -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#artificial_error">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
        <skos:definition xml:lang="zh-cn">观测时受人的感觉器官的局限性影响，使仪器安置、读数等所 产生的误差。</skos:definition>
        <skos:prefLabel xml:lang="en">artificial error</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#artificial_pollution_source -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#artificial_pollution_source">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#QualityAssessment"/>
        <skos:definition xml:lang="zh-cn">由人类活动而形成的水体污染源。</skos:definition>
        <skos:prefLabel xml:lang="en">artificial pollution source</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#artificial_precipitation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#artificial_precipitation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironment"/>
        <skos:definition xml:lang="zh-cn">用人工催化方法促成云层产生降水、增加降水或改变降水分 布的措施。</skos:definition>
        <skos:prefLabel xml:lang="en">artificial precipitation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#artificial_rainfall_device -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#artificial_rainfall_device">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffExperiment"/>
        <skos:definition xml:lang="zh-cn">人为设计可控制雨强、雨型的降雨装置。</skos:definition>
        <skos:prefLabel xml:lang="en">artificial rainfall device</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#artificial_recharge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#artificial_recharge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironment"/>
        <skos:definition xml:lang="zh-cn">借助工程措施，将地表水或其他水源的水引入地下含水层的 过程。</skos:definition>
        <skos:prefLabel xml:lang="en">artificial recharge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#astronomic_tide -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#astronomic_tide">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">主要由月球和太阳的引潮力作用所产生的潮汐。由月球引潮 力所产生的潮汐称太阴潮，由太阳引潮力所产生的潮汐称太阳潮。 </skos:definition>
        <skos:prefLabel xml:lang="en">astronomic tide</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#asymmetric_coefficient_of_basin -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#asymmetric_coefficient_of_basin">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">流域内干流左右两岸流域面积之差与两岸流域平均面积的 比值。</skos:definition>
        <skos:prefLabel xml:lang="en">asymmetric coefficient of basin</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#atmospheric_circulation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#atmospheric_circulation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">大气层具有一定稳定性的各种气流运行的综合现象，一般指 给定时段内大范围大气运动的基本状况。</skos:definition>
        <skos:prefLabel xml:lang="en">atmospheric circulation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#atmospheric_pressure -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#atmospheric_pressure">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">由地球周围大气重量而产生的压强。通常用单位横截面积上 所承受的铅直大气柱质量表示。</skos:definition>
        <skos:prefLabel xml:lang="en">atmospheric pressure</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#atmospheric_water_vapor_content -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#atmospheric_water_vapor_content">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">大气中所含气态水的数量。通常以单位截面积气柱中所含水 汽全部凝结成液态水时在气柱内形成的相当水深来表示。</skos:definition>
        <skos:prefLabel xml:lang="en">atmospheric water vapor content</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#atmospheric_water_vapor_flux -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#atmospheric_water_vapor_flux">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">单位时间内通过单位垂直面积所输送的水汽量。</skos:definition>
        <skos:prefLabel xml:lang="en">atmospheric water vapor flux</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#atmospheric_water_vapor_transport -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#atmospheric_water_vapor_transport">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">大气中的水分随气流从一个区域输送到另一区域或从低（高) 空输送到高（低）空的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">atmospheric water vapor transport</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#attenuation_cross-section -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#attenuation_cross-section">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentMonitoring"/>
        <skos:definition xml:lang="zh-cn">污染物流经一定距离而达到最大程度混合，经稀释、降解，浓 度有明显降低的采样断面。</skos:definition>
        <skos:prefLabel xml:lang="en">attenuation cross-section</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#attenuation_of_flood_wave -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#attenuation_of_flood_wave">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">洪水波波前的附加比降大于波后的附加比降，波前的运动速 度大于波后，使洪水波在运动过程中波长不断加大，波高不断减小 的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">attenuation of flood wave</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#aufeis -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#aufeis">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegime"/>
        <skos:definition xml:lang="zh-cn">冰上水洼、流水、冰上冒水、雪融化遇到寒冷生成的冰。</skos:definition>
        <skos:prefLabel xml:lang="en">aufeis</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#automatic-recording_instrument -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#automatic-recording_instrument">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InstrumentAndMeasuring"/>
        <skos:definition xml:lang="zh-cn">能将被测量值自动记录下来的设备装置。</skos:definition>
        <skos:prefLabel xml:lang="en">automatic-recording instrument</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#automatic_gauging_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#automatic_gauging_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">对水文要素采取无人值守自动监测的水文测站。根据水文测 站对监测要素所采取的自动化观测程度分为全自动监测站或某要 素自动监测站。</skos:definition>
        <skos:prefLabel xml:lang="en">automatic gauging station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#automatic_point_gauge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#automatic_point_gauge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StageWaterTemporatureAndWaveObservationDevices"/>
        <skos:definition xml:lang="zh-cn">具有自动升降及控制功能的水位测针，可以自动测得水面位 置，并输出水面数据。</skos:definition>
        <skos:prefLabel xml:lang="en">automatic point gauge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#automatic_sampling -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#automatic_sampling">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentMonitoring"/>
        <skos:definition xml:lang="zh-cn">采样过程中不需人干预，通过仪器设备按预先编定的程序进 行连续或不连续的采样。</skos:definition>
        <skos:prefLabel xml:lang="en">automatic sampling</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#automatic_water_quality_monitoring_system -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#automatic_water_quality_monitoring_system">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentMonitoringDevices"/>
        <skos:definition xml:lang="zh-cn">对指定水体的水质参数自动进行测量、分析处理和显示记录 的成套设备。</skos:definition>
        <skos:prefLabel xml:lang="en">automatic water quality monitoring system</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#autoregression_analysis -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#autoregression_analysis">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">对水文变量系列中相隔一定时间和空间的观测值之间的回归 分析。</skos:definition>
        <skos:prefLabel xml:lang="en">autoregression analysis</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#autoregressive_moving_average_model -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#autoregressive_moving_average_model">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">由自回归模型与滑动平均模型综合而成的模型，它可在最小 方差意义下对平稳时间序列进行逼近预报和控制。</skos:definition>
        <skos:prefLabel xml:lang="en">autoregressive moving average model</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#autumn_flood -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#autumn_flood">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">秋季由暴雨引起的洪水。</skos:definition>
        <skos:prefLabel xml:lang="en">autumn flood</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#auxiliary_gauge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#auxiliary_gauge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StageWaterTemporatureAndWaveObservationDevices"/>
        <skos:definition xml:lang="zh-cn">当测验河段出现横比降或在利用堰闸、隧洞、涵洞测流有淹没 出流时，在河流对岸或下游专门设立的水尺。</skos:definition>
        <skos:prefLabel xml:lang="en">auxiliary gauge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#auxiliary_hydrologic_regime_gauging_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#auxiliary_hydrologic_regime_gauging_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">当水情达到一定标准时或因临时需要，按规定报送水情的水 文测站。</skos:definition>
        <skos:prefLabel xml:lang="en">auxiliary hydrologic regime gauging station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#auxiliary_hydrometric_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#auxiliary_hydrometric_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">为补充国家基本水文测站不足而设置的一个或一组简易专用 水文测站。</skos:definition>
        <skos:prefLabel xml:lang="en">auxiliary hydrometric station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#available_surface_water_resources_amount -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#available_surface_water_resources_amount">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SurveyAndAssessment"/>
        <skos:definition xml:lang="zh-cn">以流域为单元，在保护生态环境和水资源可持续利用的前提 下，在可预见的未来，通过经济合理、技术可行的措施，在当地地表 水资源中可供河道外开发利用的最大水量（按不重复水量计）。</skos:definition>
        <skos:prefLabel xml:lang="en">available surface water resources amount</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#available_water_resources_amount -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#available_water_resources_amount">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SurveyAndAssessment"/>
        <skos:definition xml:lang="zh-cn">以流域为单元，在保护生态环境和水资源可持续利用的前提 下，在可预见的未来，通过经济合理、技术可行的措施，在当地水资 源中可供河道外开发利用的最大水量（按不重复水量计）。</skos:definition>
        <skos:prefLabel xml:lang="en">available water resources amount</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#available_water_supply -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#available_water_supply">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SurveyAndAssessment"/>
        <skos:definition xml:lang="zh-cn">考虑来水和用水条件，通过各种工程措施可提供的最大水量。</skos:definition>
        <skos:prefLabel xml:lang="en">available water supply</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#available_yield_of_groundwater -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#available_yield_of_groundwater">
