        <skos:prefLabel xml:lang="en">medium Hood</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#medium_and_long-term_hydrologic_forecasting -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#medium_and_long-term_hydrologic_forecasting">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
