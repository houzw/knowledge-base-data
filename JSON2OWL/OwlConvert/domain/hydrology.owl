        <skos:prefLabel xml:lang="en">medium and long-term hydrologic forecasting</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#medium_level_mire -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#medium_level_mire">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Mire"/>
        <skos:definition xml:lang="zh-cn">介于低位沼泽和高位沼泽之间的沼泽。</skos:definition>
        <skos:prefLabel xml:lang="en">medium level mire</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#meso_and_micro-scale_system -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#meso_and_micro-scale_system">
