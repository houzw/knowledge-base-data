    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#perched_stream -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#perched_stream">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
