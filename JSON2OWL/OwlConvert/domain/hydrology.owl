        <skos:definition xml:lang="zh-cn">为研究水资源的合理开发利用、研究工程对河川径流和水流 条件的变化影响，评价工程的经济和环境效果等所进行的有关分 析计算。</skos:definition>
        <skos:prefLabel xml:lang="en">water conservancy computation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#water_deficit -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#water_deficit">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SurveyAndAssessment"/>
        <skos:definition xml:lang="zh-cn">需水量与可供水量之差。</skos:definition>
        <skos:prefLabel xml:lang="en">water deficit</skos:prefLabel>
    </owl:NamedIndividual>
    


