    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#overlap_reservoir_capacity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#overlap_reservoir_capacity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
