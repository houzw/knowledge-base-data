    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#perennial_stream -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#perennial_stream">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">不受人为影响的情况下，终年连续流水或有水的河流。</skos:definition>
        <skos:prefLabel xml:lang="en">perennial stream</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#permanent_snowcover -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#permanent_snowcover">
