    <!-- http://www.egc.org/ont/domain/hydrology#thunderstorm -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#thunderstorm">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">由对流旺盛的积雨云引起的、伴有闪电雷鸣和强阵雨的局地风暴，有时还伴有冰雹和龙卷风。</skos:definition>
        <skos:prefLabel xml:lang="en">thunderstorm</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tidal_bore -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tidal_bore">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">发生于潮差较大的喇叭口形河口或港湾的一种潮水暴涨的 现象。</skos:definition>
        <skos:prefLabel xml:lang="en">tidal bore</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tidal_current -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tidal_current">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">海水在月球和太阳等引潮力作用下产生的周期性水平流动。</skos:definition>
        <skos:prefLabel xml:lang="en">tidal current</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tidal_current_curve -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tidal_current_curve">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfTidalRiver"/>
        <skos:definition xml:lang="zh-cn">潮流含流速、流量随时间变化的过程线。</skos:definition>
        <skos:prefLabel xml:lang="en">tidal current curve</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tidal_current_limit -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tidal_current_limit">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfTidalRiver"/>
        <skos:definition xml:lang="zh-cn">潮流沿入海河道向上游传播时，潮流所能到达的河道最远处。</skos:definition>
        <skos:prefLabel xml:lang="en">tidal current limit</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tidal_current_table -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tidal_current_table">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfTidalRiver"/>
        <skos:definition xml:lang="zh-cn">预报某一地点或海域未来一定时间内的潮流情况而制成 的表。</skos:definition>
        <skos:prefLabel xml:lang="en">tidal current table</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tidal_cycle -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tidal_cycle">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">潮汐在一个潮汐日、半个朔望月或更长时间内的涨落变化 周期。</skos:definition>
        <skos:prefLabel xml:lang="en">tidal cycle</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tidal_discharge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tidal_discharge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfTidalRiver"/>
        <skos:definition xml:lang="zh-cn">单位时间内通过某断面的潮流量，有涨潮流量与落潮流量 之分。</skos:definition>
        <skos:prefLabel xml:lang="en">tidal discharge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tidal_factor_relation_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tidal_factor_relation_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">以潮汐要素与潮量或平均流量的相关关系所建立的关系曲 线，用以推求感潮河段流量的各种方法的总称。</skos:definition>
        <skos:prefLabel xml:lang="en">tidal factor relation method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tidal_flat -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tidal_flat">
