        <skos:prefLabel xml:lang="en">pore water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#porosity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#porosity">
