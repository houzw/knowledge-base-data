        <skos:prefLabel xml:lang="en">pan-cake ice</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#parallel_drainage_pattern -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#parallel_drainage_pattern">
