        <skos:prefLabel xml:lang="en">water demand of production</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#water_demand_of_rural_resident_and_livestock -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#water_demand_of_rural_resident_and_livestock">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterResourcesPlanning"/>
        <skos:definition xml:lang="zh-cn">农村地区居民家庭日常生活需要使用的水量。</skos:definition>
        <skos:prefLabel xml:lang="en">water demand of rural resident and livestock</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#water_demand_prediction -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#water_demand_prediction">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterResourcesPlanning"/>
        <skos:definition xml:lang="zh-cn">对某个需水对象在规划水平年的水量需求的预先测定。</skos:definition>
        <skos:prefLabel xml:lang="en">water demand prediction</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#water_depth_of_submergence_tolerance_of_crop -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#water_depth_of_submergence_tolerance_of_crop">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DrainageOfSurfaceWaterAndDrainageOfSubsurfaceWater"/>
