        <skos:prefLabel xml:lang="en">main cable</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#main_channel -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#main_channel">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
