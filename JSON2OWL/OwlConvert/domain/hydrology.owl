        <skos:prefLabel xml:lang="en">main channel</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#main_cross-section_migration_number -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#main_cross-section_migration_number">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicDatabase"/>
        <skos:definition xml:lang="zh-cn">同一水文站各时期主断面的编号。按其迁移时间先后，依次排序。</skos:definition>
        <skos:prefLabel xml:lang="en">main cross-section migration number</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#main_level_mire -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#main_level_mire">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Mire"/>
        <skos:definition xml:lang="zh-cn">受大气降水补给，植物所需养分贫乏，泥炭灰分含量不足4%，以泥炭藓等贫营养植物为主的沼泽。</skos:definition>
        <skos:prefLabel xml:lang="en">main level mire</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#main_river -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#main_river">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">在水系中，汇集流域径流的主干河流。</skos:definition>
        <skos:prefLabel xml:lang="en">main river</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#major_national_hydrometric_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#major_national_hydrometric_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">在国家统一规划的基本水文测站中，符合国家《水文站网管理 办法》相关条件要求，在控制流域面积、年径流（蓄水</skos:definition>
        <skos:prefLabel xml:lang="en">major national hydrometric station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#marigmm -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#marigmm">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfTidalRiver"/>
        <skos:definition xml:lang="zh-cn">潮位随时间变化的过程线。</skos:definition>
        <skos:prefLabel xml:lang="en">marigmm</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#marine_ecosystem -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#marine_ecosystem">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#AquaticEcosystern"/>
        <skos:definition xml:lang="zh-cn">海洋中由生物群落及其环境相互作用所构成的自然系统。 </skos:definition>
        <skos:prefLabel xml:lang="en">marine ecosystem</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#marine_hydrology -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#marine_hydrology">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrology"/>
        <skos:definition xml:lang="zh-cn">研究海水的物理性质和海水各种运动的发生、发展规律的学科。</skos:definition>
        <skos:prefLabel xml:lang="en">marine hydrology</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#mark_test -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#mark_test">
