    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#peak_discharge_modulus">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
