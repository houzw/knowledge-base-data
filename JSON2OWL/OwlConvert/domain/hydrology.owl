        <skos:prefLabel xml:lang="en">masscurve</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#mathematic_programming -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#mathematic_programming">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterConservancyComputation"/>
        <skos:definition xml:lang="zh-cn">为达到某种目标，在满足给定的约束条件的各种可能方案中， 给出一种最佳方案而使用的数学方法。</skos:definition>
        <skos:prefLabel xml:lang="en">mathematic programming</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#matric_potential -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#matric_potential">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
