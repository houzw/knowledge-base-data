        <skos:prefLabel xml:lang="en">positive balance</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#positive_correlation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#positive_correlation">
