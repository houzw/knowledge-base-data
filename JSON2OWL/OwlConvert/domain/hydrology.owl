        <skos:definition xml:lang="zh-cn">潮汐和潮流影响所波及的河段。</skos:definition>
        <skos:prefLabel xml:lang="en">tidal reach</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tidal_table -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tidal_table">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfTidalRiver"/>
