        <skos:prefLabel xml:lang="en">station year</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#stationary_datum -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#stationary_datum">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
