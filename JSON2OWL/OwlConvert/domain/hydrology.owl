        <skos:prefLabel xml:lang="en">tidal level observation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tidal_level_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tidal_level_station">
