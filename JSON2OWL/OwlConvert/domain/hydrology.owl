        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignFlood"/>
        <skos:definition xml:lang="zh-cn">超过设计标准的洪水。</skos:definition>
        <skos:prefLabel xml:lang="en">extraordinary Hood</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#extraordinary_low-flow_year -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#extraordinary_low-flow_year">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignAnnualRunoff"/>
        <skos:definition xml:lang="zh-cn">年降水量或年河川径流量为历年最小值或接近历年最小值的 年份。</skos:definition>
        <skos:prefLabel xml:lang="en">extraordinary low-flow year</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#extreme_value -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#extreme_value">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">水文要素时空变化中最大、最小值的总称。</skos:definition>
        <skos:prefLabel xml:lang="en">extreme value</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#extreme_value_distribution -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#extreme_value_distribution">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">样本中极大(小）观测值的概率分布，常使用的极值分布有弗 雷歇分布、耿贝尔分布、维布尔分布等。</skos:definition>
        <skos:prefLabel xml:lang="en">extreme value distribution</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#extremevalue_series -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#extremevalue_series">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">从一定时段中选出最大或最小值所组成的水文系列。</skos:definition>
        <skos:prefLabel xml:lang="en">extremevalue series</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#fall_exponent_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#fall_exponent_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">当测验河段受变动回水影响或受变动回水与洪水涨落的综合影响时，通过落差Z。及优选落差指数建立水位Z与综合变量的单一关系曲线，用水位推求流量的一种方法。</skos:definition>
        <skos:prefLabel xml:lang="en">fall exponent method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#fall_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#fall_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">以落差作为参变数，处理受变动回水影响下的水位流量关系 的各种方法的总称。</skos:definition>
        <skos:prefLabel xml:lang="en">fall method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#fan-shaped_drainage_pattern -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#fan-shaped_drainage_pattern">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">干支流组合而成的流域轮廓形如扇状的水系。</skos:definition>
        <skos:prefLabel xml:lang="en">fan-shaped drainage pattern</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#field -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#field">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicDatabase"/>
        <skos:definition xml:lang="zh-cn">层次数据模型中数据的最小单位，描述实体的属性。</skos:definition>
        <skos:prefLabel xml:lang="en">field</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#field_experimentation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#field_experimentation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">在实际使用场合，为验证仪器、装置、系统运行正确性而进行 的试验。在实验室模拟条件下，为验证仪器、装置、系统运行正确性而 进行的试验。</skos:definition>
        <skos:prefLabel xml:lang="en">field experimentation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#field_moisture_capacity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#field_moisture_capacity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SoilMoisture"/>
        <skos:definition xml:lang="zh-cn">土壤因分子吸附力和毛管引力作用所能保持的最大水量。</skos:definition>
        <skos:prefLabel xml:lang="en">field moisture capacity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#filtration_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#filtration_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">浓缩的水样通过滤纸滤去清水，将留在滤纸上的泥沙一起烘 干、称重，然后减去滤纸重，以推求干沙质量的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">filtration method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#first-ice_date -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#first-ice_date">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegimeObservation"/>
        <skos:definition xml:lang="zh-cn">在一个结冰期内，河流上首次出现冰情现象的日期，即结冰开 始日期。</skos:definition>
        <skos:prefLabel xml:lang="en">first-ice date</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#first_frost -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#first_frost">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">每年入秋至第二年春末第一次出现的霜。</skos:definition>
        <skos:prefLabel xml:lang="en">first frost</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#fissure -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#fissure">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">在各种应力作用下岩石破裂后形成的裂缝状孔隙。</skos:definition>
        <skos:prefLabel xml:lang="en">fissure</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#fissure_ratio -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#fissure_ratio">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">一定面积岩土层所测定的裂隙面与岩土层总面积的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">fissure ratio</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#fissure_water -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#fissure_water">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">赋存于岩体裂隙中的地下水。</skos:definition>
        <skos:prefLabel xml:lang="en">fissure water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#fissured_aquifer -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#fissured_aquifer">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">以裂隙为贮水空间的含水层。</skos:definition>
        <skos:prefLabel xml:lang="en">fissured aquifer</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#fixed_ADCP -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#fixed_ADCP">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">固定安装在水面、河底、岸边，测量某一垂线、某一水层上一定 距离内的流速分布的声学剖面流速仪。安装在水底、岸边时可以测得水位。</skos:definition>
        <skos:prefLabel xml:lang="en">fixed ADCP</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#fixed_border_ice -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#fixed_border_ice">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegime"/>
        <skos:definition xml:lang="zh-cn">气温低且稳定，初生岸冰逐渐成为牢固的冰带。</skos:definition>
        <skos:prefLabel xml:lang="en">fixed border ice</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#fixed_point-fixed_area_relationship -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#fixed_point-fixed_area_relationship">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignStorm"/>
        <skos:definition xml:lang="zh-cn">固定位置的点雨深与固定的流域或区域的面平均雨深的 关系。</skos:definition>
        <skos:prefLabel xml:lang="en">fixed point-fixed area relationship</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flash_flood -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flash_flood">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">历时很短而洪峰流量较大的山区骤发性洪水。</skos:definition>
        <skos:prefLabel xml:lang="en">flash flood</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flash_flood_and_debris_flow_experiment -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flash_flood_and_debris_flow_experiment">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffExperiment"/>
        <skos:definition xml:lang="zh-cn">研究山洪和泥石流的产生机制和变化规律的科学实验。 </skos:definition>
        <skos:prefLabel xml:lang="en">flash flood and debris flow experiment</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flashy_stream -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flashy_stream">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">易于暴发山洪的河流。</skos:definition>
        <skos:prefLabel xml:lang="en">flashy stream</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flat-V_weir -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flat-V_weir">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">具有夹角为钝角的V形过水断面的三角形剖面堰。</skos:definition>
        <skos:prefLabel xml:lang="en">flat-V weir</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flat_freeze-up -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flat_freeze-up">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegime"/>
        <skos:definition xml:lang="zh-cn">在水流平缓的河段，先产生冰桥，流冰花或冰块沿冰桥平铺上溯，导致河段封冻的现象；或两岸岸冰较宽，天气骤冷，敞露水面迅 速冻结，形成表面较平整封冻冰盖的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">flat freeze-up</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#float -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#float">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StageWaterTemporatureAndWaveObservationDevices"/>
        <skos:definition xml:lang="zh-cn">一扁平圆筒，工作时处于漂浮在水面的位置，随水面升降而升 降，通过其悬索带动水位轮转动，测得水位变化。</skos:definition>
        <skos:definition xml:lang="zh-cn">受流体浮托漂浮于水流表层或水下用以测定水流速度的人工 或天然漂浮物。</skos:definition>
        <skos:prefLabel xml:lang="en">float</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#float-type_evaporimeter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#float-type_evaporimeter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservationDevices"/>
        <skos:definition xml:lang="zh-cn">用浮子水位计的原理，在与蒸发桶联通的连通管内测量水面变化，自记和遥测传输蒸发量的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">float-type evaporimeter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#float-type_flow-direction_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#float-type_flow-direction_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">应用牵引式漂浮物观测水面流向的设施。</skos:definition>
        <skos:prefLabel xml:lang="en">float-type flow-direction meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#float-type_groundwater_gauge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#float-type_groundwater_gauge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#GroundwaterObservationDevices"/>
        <skos:definition xml:lang="zh-cn"> 用于地下水位测量的浮子式水位计。</skos:definition>
        <skos:prefLabel xml:lang="en">float-type groundwater gauge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#float-type_rainfall_recorder -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#float-type_rainfall_recorder">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservationDevices"/>
        <skos:definition xml:lang="zh-cn">利用浮子感应浮子室中收集的雨水量的水位，发出相应信号， 并记录雨量过程的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">float-type rainfall recorder</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#float-type_stage_recorder -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#float-type_stage_recorder">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StageWaterTemporatureAndWaveObservationDevices"/>
        <skos:definition xml:lang="zh-cn">利用水面的浮子跟随水面一同升降，并将它的运动通过水位 轮传给记录装置或指示装置的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">float-type stage recorder</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#float_cableway -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#float_cableway">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">在测验河段上游设置的以跨河循环索为载体，通过投放设备 将浮标运送到测验断面内任一指定起点距位置的缆道。</skos:definition>
        <skos:prefLabel xml:lang="en">float cableway</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#float_coefficient -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#float_coefficient">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">断面流量（平均流速）与用浮标法测得的虚流量（水面或中泓 平均流速）的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">float coefficient</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#float_flow_velocity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#float_flow_velocity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">浮标随水流运动的速度，即浮标通过测验河段上、下浮标断面 间的距离与其历时的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">float flow velocity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#float_gauge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#float_gauge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StageWaterTemporatureAndWaveObservationDevices"/>
        <skos:definition xml:lang="zh-cn">顶部安装垂直水尺的浮筒，浮筒漂浮在水面，随水面升降，顶 部水尺指示出水位。</skos:definition>
        <skos:prefLabel xml:lang="en">float gauge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#float_gauging_cross-section -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#float_gauging_cross-section">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">用浮标法测定流量而设置的上、中、下三个断面。中断面可与 流速仪测流断面或基本水尺断面重合。</skos:definition>
        <skos:prefLabel xml:lang="en">float gauging cross-section</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#float_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#float_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">通过测定水中的天然或人工漂浮物随水流运动的速度，结合 断面资料及浮标系数来推求流量的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">float method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#float_thrower -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#float_thrower">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">可在浮标过河索上移动、投放浮标的装置。</skos:definition>
        <skos:prefLabel xml:lang="en">float thrower</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#floating_evaporation_pan -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#floating_evaporation_pan">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservationDevices"/>
        <skos:definition xml:lang="zh-cn">由浮筏架托并漂浮于大面积水域水面的蒸发器。</skos:definition>
        <skos:prefLabel xml:lang="en">floating evaporation pan</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#floating_ice -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#floating_ice">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegime"/>
        <skos:definition xml:lang="zh-cn">浮于水中的任何形式的冰。</skos:definition>
        <skos:prefLabel xml:lang="en">floating ice</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#floating_ice_cover -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#floating_ice_cover">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegime"/>
        <skos:definition xml:lang="zh-cn">封冻冰盖脱离两岸呈整片浮于水面的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">floating ice cover</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#floating_siphon -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#floating_siphon">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">在浓缩示踪剂溶液液面上漂浮的虹吸管。由于虹吸管的进出 流管口落差恒定，可以基本保证示踪剂溶液等速率流出。</skos:definition>
        <skos:prefLabel xml:lang="en">floating siphon</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#floatrod -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#floatrod">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">底端系有重物的一种浮动测速杆，它在水流中以近乎垂直的 状态随水流运动，其入水深度可以调节，用以测定垂线平均流速。</skos:definition>
        <skos:prefLabel xml:lang="en">floatrod</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flocculation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flocculation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">水中的细颗粒泥沙在一定条件下结合成集合体的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">flocculation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">河、湖在较短时间内发生的流量急骤增加、水位明显上升的水流现象。</skos:definition>
        <skos:prefLabel xml:lang="en">flood</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood-reporting -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood-reporting">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicInformation"/>
        <skos:definition xml:lang="zh-cn">利用各种通信手段及时准确地向有关部门和地区报告雨情以 及河流、湖泊、水库和其他水体的水情和水质的工作。</skos:definition>
        <skos:prefLabel xml:lang="en">flood-reporting</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood-reporting_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood-reporting_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">承担报汛任务的水文测站或气象台站。</skos:definition>
        <skos:prefLabel xml:lang="en">flood-reporting station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood-reporting_station_network -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood-reporting_station_network">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StationNetwork"/>
        <skos:definition xml:lang="zh-cn">根据水文情报预报任务要求，由报送河流、湖泊、水库和其他 水体汛情的国家基本水文测站、专用水文测站、气象台站所构成的 资料收集与信息传输系统。</skos:definition>
        <skos:prefLabel xml:lang="en">flood-reporting station network</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood_control -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood_control">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">根据洪水规律与洪灾特点，采取对策以尽量防止或减轻洪水 危害的工程或非工程措施。</skos:definition>
        <skos:prefLabel xml:lang="en">flood control</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood_control_standard -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood_control_standard">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">根据防洪保护对象的重要性和经济合理性，由国家制定的防 御不同等级洪水的标准。</skos:definition>
        <skos:prefLabel xml:lang="en">flood control standard</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood_defense -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood_defense">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">为保障保护区和工程的安全，防止洪水灾害发生，汛期对堤 防、闸、坝、铁路、桥梁等所进行的防守和险情抢修工作。</skos:definition>
        <skos:prefLabel xml:lang="en">flood defense</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood_diversion_stage -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood_diversion_stage">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">在河道上采取分洪措施时的分洪起始水位。</skos:definition>
        <skos:prefLabel xml:lang="en">flood diversion stage</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood_forecasting -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood_forecasting">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">根据洪水形成和运动规律，利用前期和现时水文、气象等信 息，对未来的洪水情况所作的预报。</skos:definition>
        <skos:prefLabel xml:lang="en">flood forecasting</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood_frequency_combination_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood_frequency_combination_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignFlood"/>
        <skos:definition xml:lang="zh-cn">以水库断面及区间天然洪水频率曲线为基础，研究各分区洪 水的所有可能组合情况，计算各种组合情况下的调洪对设计断面 洪水的影响，从而推求出设计断面受水库调洪影响的洪水频率曲 线及设计值的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">flood frequency combination method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood_investigation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood_investigation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicInvestigation"/>
        <skos:definition xml:lang="zh-cn">为推算某次洪水的洪峰水位和流量、总量、过程及其重现期等所进行的现场调查和资料收集工作。</skos:definition>
        <skos:prefLabel xml:lang="en">flood investigation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood_investigation_at_afixed_point -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood_investigation_at_afixed_point">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicInvestigation"/>
        <skos:definition xml:lang="zh-cn">在无水文站的中小河流上，选择固定河流或断面，每年进行一 次最高水位和最大流量的调查工作。</skos:definition>
        <skos:prefLabel xml:lang="en">flood investigation at afixed point</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood_mark -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood_mark">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicInvestigation"/>
        <skos:definition xml:lang="zh-cn">一次洪水的最高洪水位在岸边或浸水建筑物上所遗留的泥 印、水迹、人工刻记以及一切能够代表最高洪水到达位置的痕迹。</skos:definition>
        <skos:prefLabel xml:lang="en">flood mark</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood_peak -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood_peak">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">由起涨至落平的整个过程或洪水过程线的峰顶。</skos:definition>
        <skos:prefLabel xml:lang="en">flood peak</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood_plain -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood_plain">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">江河两岸、湖周、海滨等易受洪水淹没的区域。</skos:definition>
        <skos:prefLabel xml:lang="en">flood plain</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood_risk_analysis -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood_risk_analysis">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">为预防和减轻洪泛区的洪灾损失，对洪泛区发生洪水灾害的 可能性和损失进行的分析。</skos:definition>
        <skos:prefLabel xml:lang="en">flood risk analysis</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood_routing_of_reservoir -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood_routing_of_reservoir">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterConservancyComputation"/>
        <skos:definition xml:lang="zh-cn">水库拦蓄洪水，改变人库洪水过程线，以满足下游河道防洪要 求的径流调节。</skos:definition>
        <skos:prefLabel xml:lang="en">flood routing of reservoir</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood_season -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood_season">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">河流在一年中有规律发生洪水的时期。</skos:definition>
        <skos:prefLabel xml:lang="en">flood season</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood_series -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood_series">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignFlood"/>
        <skos:definition xml:lang="zh-cn">对洪水成因相同的洪水要素，采用一定方法选样如年最大值 法等所组成的系列。</skos:definition>
        <skos:prefLabel xml:lang="en">flood series</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood_stochastic_simulation_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood_stochastic_simulation_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignFlood"/>
        <skos:definition xml:lang="zh-cn">利用随机模拟技术，建立设计断面及各分区洪水过程线的模 拟方法。通过长系列调洪演算，求得设计断面受水库调洪影响后 的洪水系列，据此求得设计断面洪水频率曲线及设计值的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">flood stochastic simulation method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood_tidal_current -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood_tidal_current">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">沿河槽向内陆流动的潮流。</skos:definition>
        <skos:prefLabel xml:lang="en">flood tidal current</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood_tidal_range -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood_tidal_range">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">低潮位至随后的高潮位的差值。</skos:definition>
        <skos:prefLabel xml:lang="en">flood tidal range</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood_tidal_volume -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood_tidal_volume">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfTidalRiver"/>
        <skos:definition xml:lang="zh-cn">落潮憩流至涨潮憩流之间通过某断面的总水量。</skos:definition>
        <skos:prefLabel xml:lang="en">flood tidal volume</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood_tide -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood_tide">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">一个潮期内水位上升的过程。</skos:definition>
        <skos:prefLabel xml:lang="en">flood tide</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood_volume -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood_volume">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">一次洪水过程中或在给定时段内通过河流某一断面的洪水 体积。</skos:definition>
        <skos:prefLabel xml:lang="en">flood volume</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood_warning -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood_warning">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicInformation"/>
        <skos:definition xml:lang="zh-cn">当出现或即将发生洪水灾害时，为了减免生命财产遭受损失而发出的告警。</skos:definition>
        <skos:prefLabel xml:lang="en">flood warning</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood_wave -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood_wave">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">洪水下泄时，其流量和水位随时间变化形成的非恒定流动。</skos:definition>
        <skos:prefLabel xml:lang="en">flood wave</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flood_zone -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flood_zone">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#CommonTerms"/>
        <skos:prefLabel xml:lang="en">flood zone</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#floodcontrol_capacity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#floodcontrol_capacity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">防洪高水位至防洪限制水位之间的水库容积。</skos:definition>
        <skos:prefLabel xml:lang="en">floodcontrol capacity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow-direction_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow-direction_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">测量水流方向的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">flow-direction meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_and_sediment_measurement_in_fluctuant_backwater_zone_of_a_reservoir -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_and_sediment_measurement_in_fluctuant_backwater_zone_of_a_reservoir">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfReservior"/>
        <skos:definition xml:lang="zh-cn">在水库的变动回水区内，布设固定的水、沙测验断面，收集水 流泥沙运动同河床演变的关系等资料的工作。</skos:definition>
        <skos:prefLabel xml:lang="en">flow and sediment measurement in fluctuant backwater zone of a reservoir</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_concentration -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_concentration">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">扣除损失后的雨水或雪水沿地表、地下汇集于河网或汇集到 流域出口断面的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">flow concentration</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_concentration_curve -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_concentration_curve">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">单位入流经过沿程滞蓄作用，在出口断面形成的流量过程线。</skos:definition>
        <skos:prefLabel xml:lang="en">flow concentration curve</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_direction_correction -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_direction_correction">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">测速垂线的水流方向与断面不垂直，并超过一定允许范围，对 所测流速进行的改正，或测流断面不垂直于断面平均流向，并超出 一定范围，对所测流量进行的改正。</skos:definition>
        <skos:prefLabel xml:lang="en">flow direction correction</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_direction_measurement -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_direction_measurement">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">测量水流方向的作业。</skos:definition>
        <skos:prefLabel xml:lang="en">flow direction measurement</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_energy_distribution_and_transfer_in_river -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_energy_distribution_and_transfer_in_river">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Sediment"/>
        <skos:definition xml:lang="zh-cn">河道过水断面内单位水体能量的分布及转换过程。</skos:definition>
        <skos:prefLabel xml:lang="en">flow energy distribution and transfer in river</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_measureing_equipment_by_moving_boat -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_measureing_equipment_by_moving_boat">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Device"/>
        <skos:definition xml:lang="zh-cn">利用动船法施测流速、水深和方向、起点距以推求流量的专用 仪器设备的总称。</skos:definition>
        <skos:prefLabel xml:lang="en">flow measureing equipment by moving boat</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_measurement_by_hydraulic_structure -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_measurement_by_hydraulic_structure">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">利用水工泄水建筑物，通过实测水头等水力因素及闸门开度 或电功率，经率定流量系数或效率后，来计算流量的一种测流 方法。</skos:definition>
        <skos:prefLabel xml:lang="en">flow measurement by hydraulic structure</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_meter_by_current_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_meter_by_current_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">应用转子式流速仪测量固定过水断面流速，测量、计算流量的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">flow meter by current meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_plane_map -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_plane_map">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">由测量获得测验河段水体表面的流速和流向的平面分布图。 </skos:definition>
        <skos:prefLabel xml:lang="en">flow plane map</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_regime -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_regime">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">在一定的过界条件下，由各种作用力的对比关系所决定的水 流运动状态。</skos:definition>
        <skos:prefLabel xml:lang="en">flow regime</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_similarity_criterion -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_similarity_criterion">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicModel"/>
        <skos:definition xml:lang="zh-cn">模型和原体的水流系统，在空间和时间范围内保持相似所必 须遵循的准则。</skos:definition>
        <skos:prefLabel xml:lang="en">flow similarity criterion</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_structure_in_river -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_structure_in_river">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Sediment"/>
        <skos:definition xml:lang="zh-cn">河道水流内部运动特征及运动要素的空间分布。</skos:definition>
        <skos:prefLabel xml:lang="en">flow structure in river</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_velocity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_velocity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RiverRegime"/>
        <skos:definition xml:lang="zh-cn">水的质点在单位时间内沿流程移动的距离。</skos:definition>
        <skos:prefLabel xml:lang="en">flow velocity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_velocity-measuring_vertical -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_velocity-measuring_vertical">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">在测验断面内进行测点流速测量的所在竖直线。</skos:definition>
        <skos:prefLabel xml:lang="en">flow velocity-measuring vertical</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_velocity_and_cross-section_area_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_velocity_and_cross-section_area_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">通过实测断面上的流速和过水断面面积来推求流量的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">flow velocity and cross-section area method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_velocity_at_a_point -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_velocity_at_a_point">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">在测验断面内任一垂线测点所测得的水流速度。</skos:definition>
        <skos:prefLabel xml:lang="en">flow velocity at a point</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_velocity_coefficient_at_a_point -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_velocity_coefficient_at_a_point">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">垂线平均流速与该垂线上某一测点流速的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">flow velocity coefficient at a point</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_velocity_coefficient_on_water_surface -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_velocity_coefficient_on_water_surface">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">垂线（断面）平均流速与垂线水面点（水面平均）流速的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">flow velocity coefficient on water surface</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_velocity_distribution -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_velocity_distribution">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">在水流剖面上，各点流速在剖面上的分布。</skos:definition>
        <skos:prefLabel xml:lang="en">flow velocity distribution</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_velocity_distribution_at_a_crosssection -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_velocity_distribution_at_a_crosssection">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">在横断面上，各点流速在全断面流场内的分布。通常用断面 等流速线来表亦。</skos:definition>
        <skos:prefLabel xml:lang="en">flow velocity distribution at a crosssection</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_velocity_gradient -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_velocity_gradient">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">流速分布曲线上每单位间距的流速增量。</skos:definition>
        <skos:prefLabel xml:lang="en">flow velocity gradient</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_velocity_measurement -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_velocity_measurement">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">测量水质点在单位时间内沿水流方向的移动距离的作业。</skos:definition>
        <skos:prefLabel xml:lang="en">flow velocity measurement</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_velocity_of_groundwater -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_velocity_of_groundwater">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">地下水径流通过含水层过水断面的平均速度。</skos:definition>
        <skos:prefLabel xml:lang="en">flow velocity of groundwater</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_velocity_on_water_surface -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_velocity_on_water_surface">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">水流表面水质点在单位时间内所移动的距离。</skos:definition>
        <skos:prefLabel xml:lang="en">flow velocity on water surface</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_velocity_pulsation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_velocity_pulsation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">水流中某水质点的运动速度的大小和方向随时间不断变化的 现象。</skos:definition>
        <skos:prefLabel xml:lang="en">flow velocity pulsation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_velocity_pulsation_error -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_velocity_pulsation_error">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
        <skos:definition xml:lang="zh-cn">在一次流量测验过程中用有限的测速历时，因水流脉动影响 对流量所造成的误差。</skos:definition>
        <skos:prefLabel xml:lang="en">flow velocity pulsation error</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flow_with_hyperconcentration_of_sediment -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flow_with_hyperconcentration_of_sediment">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Sediment"/>
        <skos:definition xml:lang="zh-cn"> 含沙量达到每立方米数百千克或以上，水流规律已不再符合 牛顿流体规律的挟沙水流。</skos:definition>
        <skos:prefLabel xml:lang="en">flow with hyperconcentration of sediment</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flowing_water_calibration_tank -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flowing_water_calibration_tank">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">用于流速流量量测仪器检测用的动态水槽。水槽内的水是流 动的，被检测仪器固定在水流中。</skos:definition>
        <skos:prefLabel xml:lang="en">flowing water calibration tank</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#fluctuant_backwater_zone_of_a_reservoir -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#fluctuant_backwater_zone_of_a_reservoir">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfReservior"/>
        <skos:definition xml:lang="zh-cn">水库达最高蓄水位时的回水上游端至最低蓄水位时的回水上 游端之间的库区段。</skos:definition>
        <skos:prefLabel xml:lang="en">fluctuant backwater zone of a reservoir</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#fluctuating_rate_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#fluctuating_rate_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">以涨落率为参数所建立的一组曲线，用水位推求流量的一种 方法。</skos:definition>
        <skos:prefLabel xml:lang="en">fluctuating rate method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#fluctuating_zone -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#fluctuating_zone">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">水库季节性水位涨落使库区被淹没土地周期性出露于水面的 区域，又称消落区。</skos:definition>
        <skos:prefLabel xml:lang="en">fluctuating zone</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#fluctuation_range_of_stage -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#fluctuation_range_of_stage">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservation"/>
        <skos:definition xml:lang="zh-cn">一定时段内，某观测点的最高水位与最低水位的差值。</skos:definition>
        <skos:prefLabel xml:lang="en">fluctuation range of stage</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#fluctuation_rate -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#fluctuation_rate">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservation"/>
        <skos:definition xml:lang="zh-cn">一定时段内，水位涨落差与时间间隔的比值，即单位时间的水 位涨落值。</skos:definition>
        <skos:prefLabel xml:lang="en">fluctuation rate</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flume -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flume">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">具有规定形状和尺寸的可用来测定流量的人工槽。</skos:definition>
        <skos:prefLabel xml:lang="en">flume</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#fluorescent_tracer -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#fluorescent_tracer">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">主要是一些荧光染料，用特殊的灯或其他激发荧光的装置，可 以激发与荧光示踪剂浓度有关的荧光强度。</skos:definition>
        <skos:prefLabel xml:lang="en">fluorescent tracer</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#fluvial_process -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#fluvial_process">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Sediment"/>
        <skos:definition xml:lang="zh-cn">在水流与河床相互作用下，河道形态在不同时期的变化。</skos:definition>
        <skos:prefLabel xml:lang="en">fluvial process</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#fluvial_process_observation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#fluvial_process_observation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ObservationExperimentOnFluvialProcess"/>
        <skos:definition xml:lang="zh-cn">对河床形态变化过程的观测。</skos:definition>
        <skos:prefLabel xml:lang="en">fluvial process observation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#fluvial_process_of_estuary -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#fluvial_process_of_estuary">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">由水动力因素变化引起河口河床的变迁。</skos:definition>
        <skos:prefLabel xml:lang="en">fluvial process of estuary</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#fluviatile_lake -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#fluviatile_lake">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">由于河流的改道、截弯取直、淤积等，使原河道变成了湖盆，其 外形特点多是弯月形或牛轭形，故又称牛轭湖。</skos:definition>
        <skos:prefLabel xml:lang="en">fluviatile lake</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flux_divergence_field_of_water_vapor -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flux_divergence_field_of_water_vapor">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">在空间中，水汽通量散度所形成的场。</skos:definition>
        <skos:prefLabel xml:lang="en">flux divergence field of water vapor</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#flux_divergence_of_water_vapor -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#flux_divergence_of_water_vapor">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">单位时间汇入单位体积或从该体积辐射出去的水汽量。 </skos:definition>
        <skos:prefLabel xml:lang="en">flux divergence of water vapor</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#fog -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#fog">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">近地层空气中悬浮的大量微小水滴或冰晶微粒的集合体，使 水平能见度降低到1km以下的天气现象。</skos:definition>
        <skos:prefLabel xml:lang="en">fog</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#forecast_error -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#forecast_error">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">预报值与实际发生值之差。</skos:definition>
        <skos:prefLabel xml:lang="en">forecast error</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#forecast_lead_time -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#forecast_lead_time">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">从发布预报起至预报事件发生的时间间隔。</skos:definition>
        <skos:prefLabel xml:lang="en">forecast lead time</skos:prefLabel>
    </owl:NamedIndividual>
    


