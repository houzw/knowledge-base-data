        <skos:prefLabel xml:lang="en">time drift</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#time_series -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#time_series">
