        <skos:prefLabel xml:lang="en">water demand assessment for ecosystem</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#water_demand_for_balance_between_water_and_sediment -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#water_demand_for_balance_between_water_and_sediment">
