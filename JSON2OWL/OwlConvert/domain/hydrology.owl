        <skos:prefLabel xml:lang="en">hand-operating cableway</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hand_electric_wave_current_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hand_electric_wave_current_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">手持操作，测量水面流速的电波流速仪。</skos:definition>
        <skos:prefLabel xml:lang="en">hand electric wave current meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hand_ultrasonic_sounder -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hand_ultrasonic_sounder">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DepthAndCross-sectionMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">在测船上人工测量水深的超声波测深仪。一般是单一频率， 功能简单。</skos:definition>
        <skos:prefLabel xml:lang="en">hand ultrasonic sounder</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hanging_glacier -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hanging_glacier">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Glacier"/>
        <skos:definition xml:lang="zh-cn">位于雪线高度附近陡峭的山坡上，无明显的积累区和消融区 的冰川，此外，山脊线两侧还有形态很不规则的山地冰川，是山岳 冰川中体积小的冰川。</skos:definition>
        <skos:prefLabel xml:lang="en">hanging glacier</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hard_water -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hard_water">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">钙镁盐含量超过150mg/L(以CaCO3计）为硬水，300mg/L 以上为极硬水。</skos:definition>
        <skos:prefLabel xml:lang="en">hard water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hardness -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hardness">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">反映水的含盐特性，天然水中以钙盐和镁盐为主，其值为水中 钙、镁、铁、锰、锶、铝等溶解盐类的总量。</skos:definition>
        <skos:prefLabel xml:lang="en">hardness</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#harmonic_analysis -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#harmonic_analysis">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">对水文系列中周期项谐波成分的分析工作。</skos:definition>
        <skos:prefLabel xml:lang="en">harmonic analysis</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#harmonic_analysis_of_tide -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#harmonic_analysis_of_tide">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">根据实际观测的潮汐资料，将潮汐分解为许多简谐振动的分 潮，以计算各个分潮调和常数的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">harmonic analysis of tide</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#head -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#head">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RiverRegime"/>
        <skos:definition xml:lang="zh-cn">单位质量水体所具有的机械能。包括位置水头、压强水头和 流速水头。</skos:definition>
        <skos:prefLabel xml:lang="en">head</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#head_loss -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#head_loss">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RiverRegime"/>
        <skos:definition xml:lang="zh-cn">水流流动过程中，单位质量水体因克服摩擦阻力做功而损失 的能量。</skos:definition>
        <skos:prefLabel xml:lang="en">head loss</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#head_of_weir_crest -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#head_of_weir_crest">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">堰顶溢流时，堰上游水面未发生降落处的水位与堰体上缘最低点高程之差。</skos:definition>
        <skos:prefLabel xml:lang="en">head of weir crest</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#headcut_scour -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#headcut_scour">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Sediment"/>
        <skos:definition xml:lang="zh-cn">河流因下游水位跌落而引起自下游向上游发展的河床冲刷过程。</skos:definition>
        <skos:prefLabel xml:lang="en">headcut scour</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#headwaters -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#headwaters">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">河流最初形成地表水流的源头部分。</skos:definition>
        <skos:prefLabel xml:lang="en">headwaters</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#heat_balance -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#heat_balance">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">地球上任一区域或水体，在一定时段内，通过各种方式得到的 热量和失去的热量之差等于该区域或水体的蓄热变量。</skos:definition>
        <skos:prefLabel xml:lang="en">heat balance</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#heat_island_effect_of_urban -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#heat_island_effect_of_urban">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#UrbanHydrology"/>
        <skos:definition xml:lang="zh-cn">城市温度高于郊野温度的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">heat island effect of urban</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#heating_type_rain_and_snow_recorder -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#heating_type_rain_and_snow_recorder">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservationDevices"/>
        <skos:definition xml:lang="zh-cn">可以测量降雨，也可以自动用加热方法融解雪、雹等固体降 水，再用测量降雨的方法，测量雪水当量的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">heating type rain and snow recorder</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#heavy_flood -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#heavy_flood">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">重现期大于或等于50年的洪水。</skos:definition>
        <skos:prefLabel xml:lang="en">heavy flood</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#height_of_gate_opening -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#height_of_gate_opening">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">水工建筑物过水闸底板或堰顶至闸门下缘的垂直距离。</skos:definition>
        <skos:prefLabel xml:lang="en">height of gate opening</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#height_of_weir -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#height_of_weir">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">堰体上缘的最低高程与堰底板或上游河床高程之差。</skos:definition>
        <skos:prefLabel xml:lang="en">height of weir</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#heterogeneous_water-bearing_medium -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#heterogeneous_water-bearing_medium">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">渗透系数等随空间坐标变化而变化的多孔介质。</skos:definition>
        <skos:prefLabel xml:lang="en">heterogeneous water-bearing medium</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hierarchical_data_model -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hierarchical_data_model">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicDatabase"/>
        <skos:definition xml:lang="zh-cn">以记录型为结点的有根定向树或林的模型。</skos:definition>
        <skos:prefLabel xml:lang="en">hierarchical data model</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hierarchical_database -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hierarchical_database">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicDatabase"/>
        <skos:definition xml:lang="zh-cn">按层次关系结构模型设计的数据库。</skos:definition>
        <skos:prefLabel xml:lang="en">hierarchical database</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#high-flow_period -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#high-flow_period">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignAnnualRunoff"/>
        <skos:definition xml:lang="zh-cn">年内河川流量显著高于年平均流量的时期。</skos:definition>
        <skos:prefLabel xml:lang="en">high-flow period</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#high_efficiency_storm -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#high_efficiency_storm">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ProbableMaximumPrecipitationAndProbableMaximumFlood"/>
        <skos:definition xml:lang="zh-cn">降水效率较大的暴雨。</skos:definition>
        <skos:prefLabel xml:lang="en">high efficiency storm</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#high_lunitidal_interval -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#high_lunitidal_interval">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">当地月中天与随后出现的高潮的时间间隔。</skos:definition>
        <skos:prefLabel xml:lang="en">high lunitidal interval</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#high_tide -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#high_tide">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">一个潮汐涨落周期中出现的最高水位。</skos:definition>
        <skos:prefLabel xml:lang="en">high tide</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#highest_safety_stage -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#highest_safety_stage">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">能保证防洪工程或防护区安全运行的最高洪水位。</skos:definition>
        <skos:prefLabel xml:lang="en">highest safety stage</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#highest_stage -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#highest_stage">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservation"/>
        <skos:definition xml:lang="zh-cn">一定时段内，某观测点所出现的瞬时最高水位。</skos:definition>
        <skos:prefLabel xml:lang="en">highest stage</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hillslope_hydrology -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hillslope_hydrology">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrology"/>
        <skos:definition xml:lang="zh-cn">研究山坡水文物理过程的机理及分析计算方法的水文学分支 学科。</skos:definition>
        <skos:prefLabel xml:lang="en">hillslope hydrology</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#histogram -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#histogram">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">以柱形表示水文要素时段量的时间分配图。</skos:definition>
        <skos:prefLabel xml:lang="en">histogram</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#historical_Hood_investigation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#historical_Hood_investigation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicInvestigation"/>
        <skos:definition xml:lang="zh-cn">对某河段历史上发生大洪水的水位、流量、重现期和发生情况 所进行的调查工作。</skos:definition>
        <skos:prefLabel xml:lang="en">historical Hood investigation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#historical_flood -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#historical_flood">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignFlood"/>
        <skos:definition xml:lang="zh-cn">历史上无水文记录期曾发生过的大洪水，其量级应明显大于 实测洪水系列中为首的几次洪水。</skos:definition>
        <skos:prefLabel xml:lang="en">historical flood</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hole-angle_capillary_water -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hole-angle_capillary_water">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">包气带中，在土壤颗粒接触间隙，由于构成毛细管而形成弯液 面，滞留在孔角上的水。</skos:definition>
        <skos:prefLabel xml:lang="en">hole-angle capillary water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#homogeneous_frequency_enlargement -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#homogeneous_frequency_enlargement">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignFlood"/>
        <skos:definition xml:lang="zh-cn">将典型过程线的洪峰（雨峰）及各时段的洪量（雨量）进行放 大，以符合设计频率的洪峰（雨峰）及各时段洪量（雨量）的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">homogeneous frequency enlargement</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#homogeneous_hydrologic_region -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#homogeneous_hydrologic_region">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">自然地理条件（土壤性质、地形、地质条件和植被状况等），水 文特征（降水、蒸散发、径流过程及其年际、年内分配特征等）以及 人类活动影响近似的地区。</skos:definition>
        <skos:prefLabel xml:lang="en">homogeneous hydrologic region</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#homogeneous_multiple_enlargement -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#homogeneous_multiple_enlargement">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignFlood"/>
        <skos:definition xml:lang="zh-cn">用设计特征值与典型年相应特征值的比值放大典型过程线的 方法。</skos:definition>
        <skos:prefLabel xml:lang="en">homogeneous multiple enlargement</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hook_gauge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hook_gauge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StageWaterTemporatureAndWaveObservationDevices"/>
        <skos:definition xml:lang="zh-cn">由一根带有针形细钩的测杆所构成的水尺。测量时将钩尖沉 入水下后再向上提升，直到它接触水面，根据尺上读数来确定 水位。</skos:definition>
        <skos:prefLabel xml:lang="en">hook gauge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#horizontal_control_survey -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#horizontal_control_survey">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">建立各级平面控制网，测定控制点在投影面上的平面直角坐 标的测量作业。</skos:definition>
        <skos:prefLabel xml:lang="en">horizontal control survey</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#horizontal_water_vapor_transport -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#horizontal_water_vapor_transport">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">大气中的水分随着气流沿水平方向从一个地区运移到另一个 地区的过程。</skos:definition>
