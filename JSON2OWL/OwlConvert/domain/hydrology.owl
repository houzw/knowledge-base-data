        <skos:prefLabel xml:lang="en">water equivalent of snow</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#water_erosion -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#water_erosion">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Sediment"/>
        <skos:definition xml:lang="zh-cn">在降雨和水流作用下，土壤、土壤母质及其地面组成物质被破 坏、剥蚀、转运和沉积的全部过程。</skos:definition>
        <skos:prefLabel xml:lang="en">water erosion</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#water_flow_over_ice -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#water_flow_over_ice">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegime"/>
        <skos:definition xml:lang="zh-cn">冰面上流水的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">water flow over ice</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#water_level_measurement_using_agraduted_steel_tape -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#water_level_measurement_using_agraduted_steel_tape">
