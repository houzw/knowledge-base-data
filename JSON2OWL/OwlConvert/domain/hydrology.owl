        <skos:prefLabel xml:lang="en">standard current meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#standard_deviation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#standard_deviation">
