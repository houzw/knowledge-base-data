        <skos:prefLabel xml:lang="en">maximum allowable discharge amount</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#maximum_depth_of_lake -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#maximum_depth_of_lake">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">湖面至湖盆最低处的竖直距离。</skos:definition>
        <skos:prefLabel xml:lang="en">maximum depth of lake</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#maximum_error -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#maximum_error">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
