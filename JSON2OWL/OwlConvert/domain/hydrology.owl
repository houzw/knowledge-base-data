        <skos:prefLabel xml:lang="en">water environment</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#water_environment_capacity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#water_environment_capacity">
