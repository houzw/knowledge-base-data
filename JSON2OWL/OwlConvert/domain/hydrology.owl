        <skos:prefLabel xml:lang="en">permeable bed</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#permeable_boundary -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#permeable_boundary">
