    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tidal_power_resources -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tidal_power_resources">
