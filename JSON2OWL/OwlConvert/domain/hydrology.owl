        <skos:prefLabel xml:lang="en">still storage</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#still_water_calibration_tank -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#still_water_calibration_tank">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">配有专门设施用来进行流速仪检定的专用静水水槽。基本为长水槽型式。</skos:definition>
        <skos:prefLabel xml:lang="en">still water calibration tank</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#stilling_well -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#stilling_well">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservation"/>
        <skos:definition xml:lang="zh-cn">为避免风浪影响，在岸边或水体中建造的与被测水体连通、测 井内外水位相同的专用水位观测井。</skos:definition>
        <skos:prefLabel xml:lang="en">stilling well</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#stochastic_generating_series -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#stochastic_generating_series">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">应用统计试验法产生的满足一定统计特性和要求长度的水文 时序系列。</skos:definition>
        <skos:prefLabel xml:lang="en">stochastic generating series</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#stochastic_hydrologic_model -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#stochastic_hydrologic_model">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicModel"/>
        <skos:definition xml:lang="zh-cn">描述水文现象随机性规律的水文数学模型。</skos:definition>
        <skos:prefLabel xml:lang="en">stochastic hydrologic model</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#stochastic_hydrology -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#stochastic_hydrology">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrology"/>
        <skos:definition xml:lang="zh-cn">研究既含确定性成分又含随机性成分的水文现象的学科。</skos:definition>
        <skos:prefLabel xml:lang="en">stochastic hydrology</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#stochastic_process -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#stochastic_process">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">随机变量随时间变化的过程。</skos:definition>
        <skos:prefLabel xml:lang="en">stochastic process</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#stochastic_programming -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#stochastic_programming">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterConservancyComputation"/>
        <skos:definition xml:lang="zh-cn">某些变量是随机变量的数学规划。</skos:definition>
        <skos:prefLabel xml:lang="en">stochastic programming</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#stochastic_simulation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#stochastic_simulation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">根据随机过程的统计特征，采用数学模型模拟随机过程的 技术。</skos:definition>
        <skos:prefLabel xml:lang="en">stochastic simulation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#stochastic_simulation_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#stochastic_simulation_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterConservancyComputation"/>
        <skos:definition xml:lang="zh-cn">用水文时间序列分析的理论的方法，建立径流系列的随机模型，模拟出足够长的径流系列，然后按照时历法进行调节计算的 方法。</skos:definition>
        <skos:prefLabel xml:lang="en">stochastic simulation method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#stochastic_term -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#stochastic_term">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">水文系列中的纯随机成分。</skos:definition>
        <skos:prefLabel xml:lang="en">stochastic term</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#storage-discharge_curve -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#storage-discharge_curve">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">表示河段蓄泄关系的曲线。</skos:definition>
        <skos:prefLabel xml:lang="en">storage-discharge curve</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#storage_capacity_of_rainfall -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#storage_capacity_of_rainfall">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">在植被生长季节，集水区植被保蓄降雨的能力。</skos:definition>
        <skos:prefLabel xml:lang="en">storage capacity of rainfall</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#storage_of_lake -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#storage_of_lake">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">湖泊在一定水位形成湖面以下湖盆的总储量。</skos:definition>
        <skos:prefLabel xml:lang="en">storage of lake</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#storativity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#storativity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">地下水头（水位）上升或下降一个单位高度时，从单位底面 积其含水层厚度等于一个单位高度的柱体中所贮存或释放的水量。</skos:definition>
        <skos:prefLabel xml:lang="en">storativity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#storativity_capacity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#storativity_capacity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">岩土能容纳一定水量的能力。</skos:definition>
        <skos:prefLabel xml:lang="en">storativity capacity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#storm_center -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#storm_center">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">伴有强风、强降水等猛烈天气的天气系统的中心地区，如低涡 中心、气旋波中心等。</skos:definition>
        <skos:prefLabel xml:lang="en">storm center</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#storm_combination_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#storm_combination_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ProbableMaximumPrecipitationAndProbableMaximumFlood"/>
        <skos:definition xml:lang="zh-cn">将若干场特大暴雨，按气象学原理合理地组合成一场理想的 典型暴雨，并适S地进行放大来推求可能最大降水的方法。该方 法主要适用于推求大面积、长历时的可能最大降水。</skos:definition>
        <skos:prefLabel xml:lang="en">storm combination method</skos:prefLabel>
    </owl:NamedIndividual>
    


