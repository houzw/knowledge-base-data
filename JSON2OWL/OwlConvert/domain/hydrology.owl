    <!-- http://www.egc.org/ont/domain/hydrology#stand_of_tide -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#stand_of_tide">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">潮汐涨落过程中，低潮时出现的水位短时间不动的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">stand of tide</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#standard_container -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#standard_container">
