    <!-- http://www.egc.org/ont/domain/hydrology#Bailers -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#Bailers">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#GroundwaterObservationDevices"/>
        <skos:definition xml:lang="zh-cn">一种通用的管状地下水采样器，上下两端或只在下端有自动 开关的单向球阀，控制地下水的进出，保证采到要求深度处的地下 水水样。</skos:definition>
        <skos:prefLabel xml:lang="en">Bailers</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#Beaufort_wind_scale -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#Beaufort_wind_scale">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">表示风速强度等级的单位。19世纪由英国人蒲福提出，根据海面状况与陆面状况及其他物体受风影响程度来确定风的大小。</skos:definition>
        <skos:prefLabel xml:lang="en">Beaufort wind scale</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#E-601B_standard_evaporation_pan -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#E-601B_standard_evaporation_pan">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservationDevices"/>
        <skos:definition xml:lang="zh-cn">在E-601型蒸发器的基础上，对桶身材料、形状、水面测量方 法等作了改进，成为我国水文、气象统一使用的蒸发器。</skos:definition>
        <skos:prefLabel xml:lang="en">E-601B standard evaporation pan</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#E-601_type_evaporation_pan -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#E-601_type_evaporation_pan">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservationDevices"/>
        <skos:definition xml:lang="zh-cn">一种埋入地面以下，器口高于地面的测量蒸发量的仪器，主要由蒸发桶、水圈、溢流桶和测针四部分组成。</skos:definition>
        <skos:prefLabel xml:lang="en">E-601 type evaporation pan</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#Epper_effect -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#Epper_effect">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">在检定槽中检定流速仪时，由于流速仪及其悬杆向前运动产 生的波峰引起湿润断面高度的增加和相对速度降低的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">Epper effect</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#Green_Ampt -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#Green_Ampt">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffMethod"/>
        <skos:prefLabel xml:lang="en">Green &amp; Ampt</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#H-ADCP -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#H-ADCP">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">固定安装在岸边，测量此水深处水层上一定距离内流速分布 的声学剖面流速仪。</skos:definition>
        <skos:prefLabel xml:lang="en">H-ADCP</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#Hargreaves -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#Hargreaves">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ETMethod"/>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#Hood_plain -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#Hood_plain">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">位于河床主槽两侧，在洪水时被淹没，中水时出露的滩地。</skos:definition>
        <skos:prefLabel xml:lang="en">Hood plain</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#How_velocity_distribution_at_a_vertical -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#How_velocity_distribution_at_a_vertical">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">在测速垂线上，沿垂线水深方向上各点流速的部分。通常用 垂线流速分布曲线来表示。</skos:definition>
        <skos:prefLabel xml:lang="en">How velocity distribution at a vertical</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#Kalman_filtering_technique_of_hydrology -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#Kalman_filtering_technique_of_hydrology">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FloodForecasting"/>
        <skos:definition xml:lang="zh-cn">对水文过程误差进行滤波，用以校正未来水文预测值的一种 技术。</skos:definition>
        <skos:prefLabel xml:lang="en">Kalman filtering technique of hydrology</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#L-moment_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#L-moment_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">基于次序统计量期望值的线性组合，估计概率分布或频率曲 线参数的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">L-moment method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#Mariotte_vessel -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#Mariotte_vessel">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">盛放浓缩示踪剂溶液的专门密封容器。在液体流出的过程中，可以基本保证内部压力不变，从而使示踪剂溶液基本等速率流出。马利奥桶常用于其他需要对水流恒压恒流的试验。</skos:definition>
        <skos:prefLabel xml:lang="en">Mariotte vessel</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#Markov_process -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#Markov_process">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">随机过程的一种，在已知目前状态的条件下，其未来的演变不 依赖于它以往的演变。</skos:definition>
        <skos:prefLabel xml:lang="en">Markov process</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#Muskingum -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#Muskingum">
