        <skos:prefLabel xml:lang="en">tide generating force</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tide_type -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tide_type">
