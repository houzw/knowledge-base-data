        <skos:prefLabel xml:lang="en">glaciation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#glacier -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#glacier">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Glacier"/>
        <skos:definition xml:lang="zh-cn">在两极或高山地区，一段长时间内固态降水量超过融化量和 蒸发量，在重力作用下能自行沿地面缓慢运动，长期存在的天然巨 大冰体。</skos:definition>
        <skos:prefLabel xml:lang="en">glacier</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#glacier_flow -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#glacier_flow">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Glacier"/>
        <skos:definition xml:lang="zh-cn">冰川冰在重力作用下自源头向末端的移动。</skos:definition>
        <skos:prefLabel xml:lang="en">glacier flow</skos:prefLabel>
    </owl:NamedIndividual>
    


