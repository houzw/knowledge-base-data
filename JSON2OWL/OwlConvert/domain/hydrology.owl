        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FloodForecasting"/>
        <skos:definition xml:lang="zh-cn">以相当半个抵偿河长处的水位或本站测流平均时间后移一个时段的水位，与断面流量所建立的单一水位流量关系用水位推求 流量的一种方法。</skos:definition>
        <skos:definition xml:lang="zh-cn">在演算河段长等于特征河长时，假定蓄量和出流间存在线性 关系进行河道洪水演算的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">characteristic river length method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#characteristic_storage_of_reservoir -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#characteristic_storage_of_reservoir">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">水库特征水位以下或两特征水位之间的水库容积，如总库容、 死库容、兴利库容、防洪库容、调洪库容等。</skos:definition>
        <skos:prefLabel xml:lang="en">characteristic storage of reservoir</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#characteristic_water_level_of_reservoir -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#characteristic_water_level_of_reservoir">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">水库在不同时期为完成不同任务，需控制达到或允许消落的 各种库水位，如正常蓄水位、死水位、防洪限制水位、防洪高水位、 设计洪水位、校核洪水位等。</skos:definition>
        <skos:prefLabel xml:lang="en">characteristic water level of reservoir</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#check_benchmark -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#check_benchmark">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">用来引测和检查水文测站断面、水尺和其他设备高程的水准 点。根据需要设在便于引测的地点。</skos:definition>
        <skos:prefLabel xml:lang="en">check benchmark</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#check_cross-section -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#check_cross-section">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentMonitoring"/>
        <skos:definition xml:lang="zh-cn">河流流经城镇或排污区上游，未受污染影响的采样断面。</skos:definition>
        <skos:prefLabel xml:lang="en">check cross-section</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#check_flood -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#check_flood">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignFlood"/>
        <skos:definition xml:lang="zh-cn">工程在非常运用条件下符合校核标准的设计洪水。</skos:definition>
        <skos:prefLabel xml:lang="en">check flood</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#check_flood_level -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#check_flood_level">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">水库或其他水工建筑物遇到校核洪水时，在坝前或建筑物前 达到的最高水位。</skos:definition>
        <skos:prefLabel xml:lang="en">check flood level</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#check_gauge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#check_gauge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StageWaterTemporatureAndWaveObservationDevices"/>
        <skos:definition xml:lang="zh-cn">为校订自记水位记录的水位数值而设置的水尺。</skos:definition>
        <skos:prefLabel xml:lang="en">check gauge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#chemical_degradation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#chemical_degradation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">在催化反应或非催化反应下的污染物降解。</skos:definition>
        <skos:prefLabel xml:lang="en">chemical degradation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#chemical_oxygen_demand -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#chemical_oxygen_demand">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">在规定条件下，用氧化剂处理水样时，水中被氧化物质消耗的 该氧化剂数量折算的氧量。</skos:definition>
        <skos:prefLabel xml:lang="en">chemical oxygen demand</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#chemical_runoff -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#chemical_runoff">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironment"/>
        <skos:definition xml:lang="zh-cn">溶解质在水流携带下的迁移。</skos:definition>
        <skos:prefLabel xml:lang="en">chemical runoff</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#chemical_tracer -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#chemical_tracer">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">主要是各种盐类，其含量可以用相应的水质检测方法测量。</skos:definition>
        <skos:prefLabel xml:lang="en">chemical tracer</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#chemical_transformation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#chemical_transformation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">污染物在水体中的光化学氧化、氧化还原、络合分解等过程。</skos:definition>
        <skos:prefLabel xml:lang="en">chemical transformation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#chronological_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#chronological_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">当测验河段受某一因素或综合因素影响而连续变化时，水位 流量关系点据分布散乱，参照水位变化过程及水位面积或水位流 速关系变化趋势依测点时序定线，用水位推求流量的一种方法。</skos:definition>
        <skos:prefLabel xml:lang="en">chronological method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#chronological_series_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#chronological_series_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterConservancyComputation"/>
        <skos:definition xml:lang="zh-cn">以实测径流系列为基础，按一定的水库调度规则，依历时顺序 逐时段进行水库径流调节计算方法。</skos:definition>
        <skos:prefLabel xml:lang="en">chronological series method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#cirque_glacier -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#cirque_glacier">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Glacier"/>
        <skos:definition xml:lang="zh-cn">发育在河谷源头或谷地两侧围椅状的粒雪盆中，冰斗底部平 坦，而壁龛陡峻，没有或仅有很短小冰舌的冰川。</skos:definition>
        <skos:prefLabel xml:lang="en">cirque glacier</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#city_drought -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#city_drought">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">城市因遇特枯水年或连续枯水年，造成供水水源不足，实际供 水量低于正常供水量，生活、生产和生态环境受到影响的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">city drought</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#climate -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#climate">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">某地区多年天气状况及变化特征的综合。</skos:definition>
        <skos:prefLabel xml:lang="en">climate</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#climate_change -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#climate_change">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">泛指各种时间尺度的气候演变。</skos:definition>
        <skos:prefLabel xml:lang="en">climate change</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#climatic_regionalization -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#climatic_regionalization">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">根据气候的不同类型，按一定指标将全球或某一地区划分为 若干气候特征相似的区域。</skos:definition>
        <skos:prefLabel xml:lang="en">climatic regionalization</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#climatic_zone -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#climatic_zone">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">根据地面气候纬向分布的相似性，划分与纬圈大致平行的带 状气候区。有极地气候、温带气候、副热带气候、热带气候和赤道气候。</skos:definition>
        <skos:prefLabel xml:lang="en">climatic zone</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#clogging_of_river_sediment_flow -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#clogging_of_river_sediment_flow">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Sediment"/>
        <skos:definition xml:lang="zh-cn">高含沙水流中含沙量超过某一极限值时，在洪峰突然降落，流 速迅速减小的情况下，整个水流已不能保持流动状态，而是停滞不 前形成的河槽堵塞现象。</skos:definition>
        <skos:prefLabel xml:lang="en">clogging of river sediment flow</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#closure_error -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#closure_error">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
        <skos:definition xml:lang="zh-cn">在平面或高程测量中，由于测量存在误差，致使几何图形的理 论要求与外业测量成果不拟合所产生的不符合值。</skos:definition>
        <skos:prefLabel xml:lang="en">closure error</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#coaxial_correlation_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#coaxial_correlation_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">用潮差与平均流量建立相关关系并选择一两个潮汐要素作为 参数，通过多个象限相关定线，用以推求潮量或平均流量的一种 方法。</skos:definition>
        <skos:prefLabel xml:lang="en">coaxial correlation method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#coefficient_of_hydrodynamic_dispersion -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#coefficient_of_hydrodynamic_dispersion">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">在多孔介质中溶质分子扩散和机械弥散作用的综合参数。</skos:definition>
        <skos:prefLabel xml:lang="en">coefficient of hydrodynamic dispersion</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#coefficient_of_mechanical_dispersion -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#coefficient_of_mechanical_dispersion">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">在多孔介质中溶质对流弥散能力指标。</skos:definition>
        <skos:prefLabel xml:lang="en">coefficient of mechanical dispersion</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#coefficient_of_molecular_diffusion -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#coefficient_of_molecular_diffusion">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">多孔介质中在分子扩散作用下溶质运移能力的指标。</skos:definition>
        <skos:prefLabel xml:lang="en">coefficient of molecular diffusion</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#coefficient_of_precipitation_recharge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#coefficient_of_precipitation_recharge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">在一定时段内，单位面积上降水入渗补给地下水的水量与同 时段内降水量的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">coefficient of precipitation recharge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#coefficient_of_pressure_conductivity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#coefficient_of_pressure_conductivity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">承压含水层水头变化传递速度的参数。</skos:definition>
        <skos:prefLabel xml:lang="en">coefficient of pressure conductivity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#coefficient_of_skewness -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#coefficient_of_skewness">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">表示随机变量对其均值的不对称程度，用系列的三阶中心矩 与其标准差立方的比值表示。</skos:definition>
        <skos:prefLabel xml:lang="en">coefficient of skewness</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#coefficient_of_variation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#coefficient_of_variation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">表示随机变量对其均值的相对离散程度，用系列的标准差与 其均值的比值表示。</skos:definition>
        <skos:prefLabel xml:lang="en">coefficient of variation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#coefficient_of_water_table_conductivity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#coefficient_of_water_table_conductivity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">潜水含水层水位变化传播速度的参数.</skos:definition>
        <skos:prefLabel xml:lang="en">coefficient of water table conductivity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#coefficient_of_wave_velocity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#coefficient_of_wave_velocity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">波速与断面平均流速的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">coefficient of wave velocity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#cold_water_hazard_of_reservoir -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#cold_water_hazard_of_reservoir">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironment"/>
        <skos:definition xml:lang="zh-cn">水库下泄低温水引起的危害。</skos:definition>
        <skos:prefLabel xml:lang="en">cold water hazard of reservoir</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#collapsible_sampler -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#collapsible_sampler">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysisDevices"/>
        <skos:definition xml:lang="zh-cn">借助皮囊容器柔性以传导和调整器内外压力使其平衡，测前 将皮囊内空气排空的积时式悬移质采样器。</skos:definition>
        <skos:prefLabel xml:lang="en">collapsible sampler</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#color_change_of_ice_cover -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#color_change_of_ice_cover">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegime"/>
        <skos:definition xml:lang="zh-cn">融冰过程中，封冻冰盖表面颜色发生变化的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">color change of ice cover</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#combination_weir -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#combination_weir">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">含有两个以上不同堰型或尺寸，可分级测流的堰。</skos:definition>
        <skos:prefLabel xml:lang="en">combination weir</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#combined-discharge_forecasting_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#combined-discharge_forecasting_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FloodForecasting"/>
        <skos:definition xml:lang="zh-cn">在有两条以上支流汇人的河段，根据上游站相应流量之和与 下游站相应流量或相应水位建立的相应关系，进行下游站相应流 量或相应水位预报的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">combined-discharge forecasting method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#combined_standard_uncertainty -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#combined_standard_uncertainty">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
        <skos:definition xml:lang="zh-cn">当测量结果是由若干个其他量的值求得时，按其他各量的方差算得的标准不确定度。</skos:definition>
        <skos:prefLabel xml:lang="en">combined standard uncertainty</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#communication -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#communication">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataAcquisitionSystem"/>
        <skos:definition xml:lang="zh-cn">通过有线或无线设施对语言、文字、图像等信息进行传输、变 换和处理的过程。</skos:definition>
        <skos:prefLabel xml:lang="en">communication</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#comparative_gauging -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#comparative_gauging">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Monitoring"/>
        <skos:definition xml:lang="zh-cn">建立同一系统中相同要素的物理运动伴随关系数学表达的观 测活动，或对设备状态进行比较的测试。</skos:definition>
        <skos:prefLabel xml:lang="en">comparative gauging</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#comparative_test -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#comparative_test">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">在相同的现场或室内条件下，和性能更好的其他型号仪器或 和已被确认的标准仪器进行的比较试验。</skos:definition>
        <skos:prefLabel xml:lang="en">comparative test</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#complete_set_of_rain_gauging_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#complete_set_of_rain_gauging_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">为分析中小河流降水径流关系而在水文站以上集水区内设置 的一定数量的降水量（雨量）站。</skos:definition>
        <skos:prefLabel xml:lang="en">complete set of rain gauging station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#compound_channel -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#compound_channel">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">由主槽与左、右滩地或与一岸滩地组成的河槽。</skos:definition>
        <skos:prefLabel xml:lang="en">compound channel</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#comprehensive_water_resources_assessment -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#comprehensive_water_resources_assessment">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SurveyAndAssessment"/>
        <skos:definition xml:lang="zh-cn">在水资源数量、质量和开发利用现状评价以及对环境影响评 价的基础上，对水资源时空分布特征、利用状况及与经济社会发展 的协调程度等各个方面所作的综合评价。</skos:definition>
        <skos:prefLabel xml:lang="en">comprehensive water resources assessment</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#computation_of_drainage_of_surface_water -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#computation_of_drainage_of_surface_water">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DrainageOfSurfaceWaterAndDrainageOfSubsurfaceWater"/>
        <skos:definition xml:lang="zh-cn">根据排涝标准进行的排水渠道设计排水流量和设计排涝水位 的分析计算工作。</skos:definition>
        <skos:prefLabel xml:lang="en">computation of drainage of surface water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#computation_of_reservoir_backwater -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#computation_of_reservoir_backwater">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">水库蓄水后在各种设计条件下库区沿程水位壅高情况的分析计算。</skos:definition>
        <skos:prefLabel xml:lang="en">computation of reservoir backwater</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#computation_of_reservoir_sedimentation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#computation_of_reservoir_sedimentation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">水库库区泥沙淤积过程、淤积分布及冲淤平衡等的分析计算。</skos:definition>
        <skos:prefLabel xml:lang="en">computation of reservoir sedimentation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#computation_of_river_channel_scour_below_reservoir -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#computation_of_river_channel_scour_below_reservoir">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">水库泄水对下游河道冲刷过程及冲刷相对平衡的分析计算。</skos:definition>
        <skos:prefLabel xml:lang="en">computation of river channel scour below reservoir</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#computation_of_runoff_adjusted_to_current_condition -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#computation_of_runoff_adjusted_to_current_condition">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignAnnualRunoff"/>
        <skos:definition xml:lang="zh-cn">在人类活动影响大的地区，把人类活动对河川径流的影响水 量计入实测径流中，对尚未受人类活动影响的径流资料修正到现 有条件下的分析计算。</skos:definition>
        <skos:prefLabel xml:lang="en">computation of runoff adjusted to current condition</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#computation_of_runoff_regulation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#computation_of_runoff_regulation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterConservancyComputation"/>
        <skos:definition xml:lang="zh-cn">通常指水库或湖泊蓄泄作用，根据水资源综合利用目标，对河 川径流和地下径流在时间过程和地区分布进行调整的水利计算。</skos:definition>
        <skos:prefLabel xml:lang="en">computation of runoff regulation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#computation_of_runoff_restored_to_original_condition -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#computation_of_runoff_restored_to_original_condition">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignAnnualRunoff"/>
        <skos:definition xml:lang="zh-cn">在人类活动影响大的地区，把人类活动对河川径流的影响水 量计入实测径流中，对径流资料复原的分析计算。</skos:definition>
        <skos:prefLabel xml:lang="en">computation of runoff restored to original condition</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#computation_of_sediment_runoff -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#computation_of_sediment_runoff">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">一定时段内通过河流指定过水断面泥沙总量及其年内分配的分析计算。</skos:definition>
        <skos:prefLabel xml:lang="en">computation of sediment runoff</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#concave_bank -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#concave_bank">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">弯曲河段两岸中，离河槽曲率中心远的一岸。</skos:definition>
        <skos:prefLabel xml:lang="en">concave bank</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#concentrated_database -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#concentrated_database">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicDatabase"/>
        <skos:definition xml:lang="zh-cn">经过合并，清除了冗余数据的数据库。</skos:definition>
        <skos:prefLabel xml:lang="en">concentrated database</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#concentration_control_of_pollutant_discharge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#concentration_control_of_pollutant_discharge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#QualityAssessment"/>
        <skos:definition xml:lang="zh-cn">根据污水排放的允许浓度标准值，调控污染源排放的污染物。</skos:definition>
        <skos:prefLabel xml:lang="en">concentration control of pollutant discharge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#conceptual_hydrologic_model -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#conceptual_hydrologic_model">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicModel"/>
        <skos:definition xml:lang="zh-cn">对水文现象进行概化的水文数学模型。</skos:definition>
        <skos:prefLabel xml:lang="en">conceptual hydrologic model</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#condensation_recharge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#condensation_recharge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">水汽凝结形成的重力水下渗补给地下水的过程。</skos:definition>
        <skos:prefLabel xml:lang="en">condensation recharge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#condensation_water -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#condensation_water">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">进入包气带岩土空隙中的水蒸气遇冷凝结而形成的水。</skos:definition>
        <skos:prefLabel xml:lang="en">condensation water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#conditional_probability -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#conditional_probability">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">某一随机事件在特定条件下的概率。</skos:definition>
        <skos:prefLabel xml:lang="en">conditional probability</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#confidence_level -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#confidence_level">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
        <skos:definition xml:lang="zh-cn">对总体参数进行区间估计时，如果对于先给很小的概率a，能找到一个区间使得a在此置信区间的概率为1 一a的值，即</skos:definition>
        <skos:prefLabel xml:lang="en">confidence level</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#confined_level -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#confined_level">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">承压水观测井内稳定水位的高程。</skos:definition>
        <skos:prefLabel xml:lang="en">confined level</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#confined_water -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#confined_water">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">充满于上、下两个相对隔水层之的承受静水压力的地下水。</skos:definition>
        <skos:prefLabel xml:lang="en">confined water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#confined_water_head -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#confined_water_head">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">承压水位高出承压含水层顶板的高度。</skos:definition>
        <skos:prefLabel xml:lang="en">confined water head</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#confining_bed -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#confining_bed">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">结构致密、透水性极弱的导水速率不足以对井或泉提供明显 水量的岩土层。</skos:definition>
        <skos:prefLabel xml:lang="en">confining bed</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#confining_boundary -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#confining_boundary">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">渗透性极差的含水层边界，即法线方向水力梯度（流量）等于 零的边界。</skos:definition>
        <skos:prefLabel xml:lang="en">confining boundary</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#constant-level_tank -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#constant-level_tank">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">盛放浓缩示踪剂溶液的专门容器。可以使其液位恒定，以保证等速率流出，注入水流。</skos:definition>
        <skos:prefLabel xml:lang="en">constant-level tank</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#constant-rate_injection_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#constant-rate_injection_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">将已知浓度的示踪剂以均匀的速度注人测验河段的上断面， 测定下游充分混匀的取样断面的示踪剂浓度来推求流量的一种稀</skos:definition>
        <skos:prefLabel xml:lang="en">constant-rate injection method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#constant_fall_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#constant_fall_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">当测验河段受变动回水影响时，采用以某一落差固定值为定 落差作参数所建立的一组曲线，用水位推求流量的一种方法。</skos:definition>
        <skos:prefLabel xml:lang="en">constant fall method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#constant_tidal_factor_relation_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#constant_tidal_factor_relation_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">以纵坐标为潮差或有效波高横坐标为平均流量，并选择某一 特定潮汐要素作参数所建立的一组曲线，用以推求潮量或潮流量 的一种方法。</skos:definition>
        <skos:prefLabel xml:lang="en">constant tidal factor relation method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#constraint_equation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#constraint_equation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterConservancyComputation"/>
        <skos:definition xml:lang="zh-cn">数学模型中各项变量应满足的条件方程或条件不等式。</skos:definition>
        <skos:prefLabel xml:lang="en">constraint equation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#consumptive_water_use -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#consumptive_water_use">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SurveyAndAssessment"/>
        <skos:definition xml:lang="zh-cn">供水量中耗于蒸散发、构成产品成分及被严重污染的用水。</skos:definition>
        <skos:prefLabel xml:lang="en">consumptive water use</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#continental_ice_sheet_glacier -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#continental_ice_sheet_glacier">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Glacier"/>
        <skos:definition xml:lang="zh-cn">发育在两极地区，不受地形约束，表面呈凸起的盾状，冰流由 中央向四周流动的冰体。</skos:definition>
        <skos:prefLabel xml:lang="en">continental ice sheet glacier</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#continuing_loss -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#continuing_loss">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">开始产流之后损失的降水量。</skos:definition>
        <skos:prefLabel xml:lang="en">continuing loss</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#continuity_equation_of_flow -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#continuity_equation_of_flow">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">描述水流运动中质量平衡的方程式，是质量守恒定律在水流 运动中的应用。</skos:definition>
        <skos:prefLabel xml:lang="en">continuity equation of flow</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#continuous_high-flow_years -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#continuous_high-flow_years">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignAnnualRunoff"/>
        <skos:definition xml:lang="zh-cn">连续多年降水量或河川径流量大于正常值的年组。</skos:definition>
        <skos:prefLabel xml:lang="en">continuous high-flow years</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#continuous_low-flow_years -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#continuous_low-flow_years">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignAnnualRunoff"/>
        <skos:definition xml:lang="zh-cn">连续多年降水量或河川径流量小于正常值的年组。</skos:definition>
        <skos:prefLabel xml:lang="en">continuous low-flow years</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#contracted_weir -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#contracted_weir">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">引起水流侧向收缩的堰，堰的长度小于河宽。</skos:definition>
        <skos:prefLabel xml:lang="en">contracted weir</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#control_console_of_hydrometric_cableway -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#control_console_of_hydrometric_cableway">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Device"/>
        <skos:definition xml:lang="zh-cn">控制水文缆道运行并记录及显示缆道测验仪器数据的工 作台。</skos:definition>
        <skos:prefLabel xml:lang="en">control console of hydrometric cableway</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#control_cross-section -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#control_cross-section">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentMonitoring"/>
        <skos:definition xml:lang="zh-cn">排污区下游，能反映本污染区污染状况的采样断面。</skos:definition>
        <skos:prefLabel xml:lang="en">control cross-section</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#control_reach -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#control_reach">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">对某断面水位流量关系起控制作用的河段。</skos:definition>
        <skos:prefLabel xml:lang="en">control reach</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#control_survey -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#control_survey">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">在一定区域内，为地形或工程测量建立控制网所进行的测量 工作，是高程控制测量和平面控制测量的总称。</skos:definition>
        <skos:prefLabel xml:lang="en">control survey</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#conventional_true_value -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#conventional_true_value">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">对于给定目的具有适当不确定度的、赋予特定量的值，有时该 值是约定采用的。</skos:definition>
        <skos:prefLabel xml:lang="en">conventional true value</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#convergence_component_of_storm -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#convergence_component_of_storm">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ProbableMaximumPrecipitationAndProbableMaximumFlood"/>
        <skos:definition xml:lang="zh-cn">由地形产生水汽辐合作用所产生的暴雨影响量。</skos:definition>
        <skos:prefLabel xml:lang="en">convergence component of storm</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#convergence_zone_of_water_vapor -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#convergence_zone_of_water_vapor">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">水汽从四周向内汇集的地区。</skos:definition>
        <skos:prefLabel xml:lang="en">convergence zone of water vapor</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#converging_reach -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#converging_reach">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">顺水流方向上，横断面面积逐渐减小的河段。</skos:definition>
        <skos:prefLabel xml:lang="en">converging reach</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#convert_coefficient_of_evaporation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#convert_coefficient_of_evaporation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservation"/>
        <skos:definition xml:lang="zh-cn">大型蒸发器或标准蒸发器测得的蒸发量与普通蒸发器测得的 蒸发量的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">convert coefficient of evaporation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#convex_bank -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#convex_bank">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">弯曲河段两岸中，离河槽曲率中心近的一岸。</skos:definition>
        <skos:prefLabel xml:lang="en">convex bank</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#cooling_water_use -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#cooling_water_use">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DevelopmentAndUtilization"/>
        <skos:definition xml:lang="zh-cn">工业生产过程中降低温度的用水，可分为直流冷却用水和循 环冷却用水两类。</skos:definition>
        <skos:prefLabel xml:lang="en">cooling water use</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#cord_drive_horizontal_sampler -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#cord_drive_horizontal_sampler">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysisDevices"/>
        <skos:definition xml:lang="zh-cn">采样筒横置，在水面上用拉绳方式，瞬间关上采样筒盖的采 样器。</skos:definition>
        <skos:prefLabel xml:lang="en">cord drive horizontal sampler</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#correcting_coefficient_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#correcting_coefficient_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">当测验河段受结冰影响时，采用以畅流期的水位流量关系曲 线为标准曲线，实测冰期流量与其同水位时标准曲线查得的流量 的比值为改正系数点绘过程线，用水位推求流量的一种方法。</skos:definition>
        <skos:prefLabel xml:lang="en">correcting coefficient method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#correcting_stage_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#correcting_stage_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">当测验河段受断面冲淤水草生长或结冰影响时，水位流量关 系点据较为散乱，通过确定一条水位流量关系的标准曲线，并量取 各测点与标准曲线的纵差为水位改正数点绘水位改正数过程线， 用水位推求流量的一种方法。</skos:definition>
        <skos:prefLabel xml:lang="en">correcting stage method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#corrective_gauging -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#corrective_gauging">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Monitoring"/>
        <skos:definition xml:lang="zh-cn">按一定技术要求，对水文测站基本设施的位置高程控制点或 水位流量关系等级所进行的校正测量作业。</skos:definition>
        <skos:prefLabel xml:lang="en">corrective gauging</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#correctness -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#correctness">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
        <skos:definition xml:lang="zh-cn">测量结果与被测量的真值之间的一致程度。</skos:definition>
        <skos:prefLabel xml:lang="en">correctness</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#correlation_coefficient -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#correlation_coefficient">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">表示两个或两个以上水文变量之间线性相关程度的指标。</skos:definition>
        <skos:prefLabel xml:lang="en">correlation coefficient</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#correlation_diagram -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#correlation_diagram">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">用来反映变量之间统计相关关系的图。</skos:definition>
        <skos:prefLabel xml:lang="en">correlation diagram</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#corresponding_discharge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#corresponding_discharge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FloodForecasting"/>
        <skos:definition xml:lang="zh-cn">河段上、下游站同位相的流量。</skos:definition>
        <skos:prefLabel xml:lang="en">corresponding discharge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#corresponding_river_discharge_forecasting_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#corresponding_river_discharge_forecasting_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FloodForecasting"/>
        <skos:definition xml:lang="zh-cn">根据上游站传播到下游站时的相应流量及其传播速度的变化 规律，建立其经验关系，据以进行预报下游站流量的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">corresponding river discharge forecasting method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#corresponding_river_stage_forecasting_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#corresponding_river_stage_forecasting_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FloodForecasting"/>
        <skos:definition xml:lang="zh-cn">根据上游站传播到下游站时的相应水位及其传播速度的变化 规律，建立其经验关系，据以进行预报下游站水位的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">corresponding river stage forecasting method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#counter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#counter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InstrumentAndMeasuring"/>
        <skos:definition xml:lang="zh-cn">具有有限个数位，以机械或电子方法，显示在一定时间内进入 的脉冲数目的器件。</skos:definition>
        <skos:prefLabel xml:lang="en">counter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#counter_for_cableway_sounder -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#counter_for_cableway_sounder">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DepthAndCross-sectionMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">缆道测深时，测量测深钢丝绳测深时放出长度的计数器。一 般安装在缆道绞车上。</skos:definition>
        <skos:prefLabel xml:lang="en">counter for cableway sounder</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#counter_for_sounder -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#counter_for_sounder">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DepthAndCross-sectionMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">测量测深钢丝绳测深时放出长度的计数器。一般安装在测流 绞车上。</skos:definition>
        <skos:prefLabel xml:lang="en">counter for sounder</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#crater_lake -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#crater_lake">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">火山停止喷发后，火山口内积水而形成的湖泊。面积小而 深度大。</skos:definition>
        <skos:prefLabel xml:lang="en">crater lake</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#crest_stage_gauge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#crest_stage_gauge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StageWaterTemporatureAndWaveObservationDevices"/>
        <skos:definition xml:lang="zh-cn">汛期用于测记洪峰水位的水尺。</skos:definition>
        <skos:prefLabel xml:lang="en">crest stage gauge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#crevice -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#crevice">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <owl:sameAs rdf:resource="http://www.egc.org/ont/domain/hydrology#fissure"/>
        <skos:prefLabel xml:lang="en">crevice</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#critical-depth_flume -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#critical-depth_flume">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">喉道中能产生临界流的量水槽。</skos:definition>
        <skos:prefLabel xml:lang="en">critical-depth flume</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#critical_depth -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#critical_depth">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RiverRegime"/>
        <skos:definition xml:lang="zh-cn">明渠中某断面水流发生临界流时的水深。</skos:definition>
        <skos:prefLabel xml:lang="en">critical depth</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#critical_depth_of_phreatic_water_evaporation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#critical_depth_of_phreatic_water_evaporation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">不发生潜水蒸发的最小埋深。</skos:definition>
        <skos:prefLabel xml:lang="en">critical depth of phreatic water evaporation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#critical_discharge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#critical_discharge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RiverRegime"/>
        <skos:definition xml:lang="zh-cn">明渠中某断面保持临界水深时临界流的流量。</skos:definition>
        <skos:prefLabel xml:lang="en">critical discharge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#critical_flow -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#critical_flow">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RiverRegime"/>
        <skos:definition xml:lang="zh-cn">在给定流量情况下，水流中的总能头最小，或在给定总能头情 况下，流量最大的水流，其弗汝德数等于1，水面扰动不会传播到上游。</skos:definition>
        <skos:prefLabel xml:lang="en">critical flow</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#critical_rainfall_intensity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#critical_rainfall_intensity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignFlood"/>
        <skos:definition xml:lang="zh-cn">由于河道往往具有在高水时流速趋于稳定的水力学特征，从 而使中小洪水的非线性汇流特征在大洪水时转变为线性回流，这个转变时的雨强称为临界雨强。</skos:definition>
        <skos:prefLabel xml:lang="en">critical rainfall intensity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#critical_velocity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#critical_velocity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RiverRegime"/>
        <skos:definition xml:lang="zh-cn">明渠水流中发生临界水深时的断面平均流速。</skos:definition>
        <skos:definition xml:lang="zh-cn">转子式流速仪检定曲线图中，低速曲线部分过渡到直线部分 的转折点处的水流速度。</skos:definition>
        <skos:prefLabel xml:lang="en">critical velocity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#cross-section -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#cross-section">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DepthAndCrossSectionMeasurement"/>
        <skos:definition xml:lang="zh-cn">垂直于水流平均流向的河流断面。</skos:definition>
        <skos:prefLabel xml:lang="en">cross-section</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#cross-section_area -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#cross-section_area">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DepthAndCrossSectionMeasurement"/>
        <skos:definition xml:lang="zh-cn">横断面的某一水位线与湿周所包围的面积。</skos:definition>
        <skos:prefLabel xml:lang="en">cross-section area</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#cross-section_characteristics -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#cross-section_characteristics">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">断面位置、几何特征、组成物质、水流形态及其变化的性质。</skos:definition>
        <skos:prefLabel xml:lang="en">cross-section characteristics</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#cross-section_control -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#cross-section_control">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">由石梁、急滩、弯道等对水文站水位流量关系稳定性形成的测 站控制。</skos:definition>
        <skos:prefLabel xml:lang="en">cross-section control</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#cross-section_mixing_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#cross-section_mixing_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">按一定技术要求将断面上各垂线或各测点用积深法或选点法 所采集的水样混合成一个水样，经处理求得全断面平均含沙量的 方法。</skos:definition>
        <skos:prefLabel xml:lang="en">cross-section mixing method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#cross-section_sign -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#cross-section_sign">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">设在水文断面两岸用来标示断面位置和方向的永久性标牌。 </skos:definition>
        <skos:prefLabel xml:lang="en">cross-section sign</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#cross-section_stake -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#cross-section_stake">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">设在水文断面两岸的测量标志桩。</skos:definition>
        <skos:prefLabel xml:lang="en">cross-section stake</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#cross-section_surveymethod -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#cross-section_surveymethod">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfReservior"/>
        <skos:definition xml:lang="zh-cn">从坝址至库尾布置若干个可控制库区地形的断面，并进行断 面测量，按时段始末两次测量的增量以计算时段淤积量的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">cross-section surveymethod</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#cross-segmentation_sampling -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#cross-segmentation_sampling">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignFlood"/>
        <skos:definition xml:lang="zh-cn">由于洪水出现的偶然性，各年分期洪水的最大值不一定正好 在所定的分期内，可能往前或往后错开几天选样。</skos:definition>
        <skos:prefLabel xml:lang="en">cross-segmentation sampling</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#cubature -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#cubature">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">在某个固定时段内，将流经管道或渠槽的水引人体积经过标 定的容器，称量这些水的总体积(或重量），即可得到单位时间流过 的水体积(或重量）即流量。</skos:definition>
        <skos:prefLabel xml:lang="en">cubature</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#cumulative_frequency -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#cumulative_frequency">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">等于及大于等于及小于某水文要素值出现可能性的量度。</skos:definition>
        <skos:prefLabel xml:lang="en">cumulative frequency</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#cup-type_current_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#cup-type_current_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">以旋杯作为转子的流速仪，其旋杯绕着竖轴转动，转速与周围 水流的流速成单值对应关系。</skos:definition>
        <skos:prefLabel xml:lang="en">cup-type current meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#current_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#current_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">测定水流运动速度的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">current meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#current_meter_calibration -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#current_meter_calibration">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">建立转子式流速仪转子的旋转速率以及其他流速仪测速示值 与水流速度之间对应关系的全部作业。</skos:definition>
        <skos:prefLabel xml:lang="en">current meter calibration</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#current_meter_counter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#current_meter_counter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">接收、记录转子式流速仪脉冲信号的专用计数器。按功能不同可以分为：只发出音响、灯光信号，依靠人工计数的计数器；可以动计时、计数的计数器；可以直接计算流速的计数器。</skos:definition>
        <skos:prefLabel xml:lang="en">current meter counter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#current_meter_gauging_cross-section -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#current_meter_gauging_cross-section">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">用流速仪法测定流量而设置的断面。它可与基本水尺断面重合，同时又可用于输沙率测验。</skos:definition>
        <skos:prefLabel xml:lang="en">current meter gauging cross-section</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#current_meter_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#current_meter_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">用流速仪实测断面上各部分的流速，并与相应部分过水断面 面积的乘积来推求流量的方法。简称流速仪法。</skos:definition>
        <skos:prefLabel xml:lang="en">current meter method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#curve_fitting_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#curve_fitting_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">用具有一定数学形式的频率曲线来拟合水文变量点据，以确 定总体统计参数的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">curve fitting method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#curve_fitting_test -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#curve_fitting_test">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">在水文测验中，检验水位流量关系实测点偏离曲线正负符号 的排列有无明显的系统偏离的一种统计检验方法。</skos:definition>
        <skos:prefLabel xml:lang="en">curve fitting test</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#curve_of_depth_distribution_of_lake -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#curve_of_depth_distribution_of_lake">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">反映深度与相应湖水面积之间的关系曲线。</skos:definition>
        <skos:prefLabel xml:lang="en">curve of depth distribution of lake</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#cycle_term -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#cycle_term">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">水文系列中随时程呈周期变化的成分。</skos:definition>
        <skos:prefLabel xml:lang="en">cycle term</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#cycling_use_of_water -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#cycling_use_of_water">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SurveyAndAssessment"/>
        <skos:definition xml:lang="zh-cn">引用的水量在供水工艺流程中反复被使用的水。</skos:definition>
        <skos:prefLabel xml:lang="en">cycling use of water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#cycling_use_rate -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#cycling_use_rate">
