        <skos:prefLabel xml:lang="en">perched water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#perennial_hydrologic_regime_gauging_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#perennial_hydrologic_regime_gauging_station">
