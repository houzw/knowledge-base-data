        <skos:prefLabel xml:lang="en">moisture transmission rate and moisture wind joint enlargement</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#moisture_transmission_value_enlargement -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#moisture_transmission_value_enlargement">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ProbableMaximumPrecipitationAndProbableMaximumFlood"/>
