        <skos:prefLabel xml:lang="en">particle diameter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#peak-volume_relation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#peak-volume_relation">
