        <skos:definition xml:lang="zh-cn">通过仪器设备，按预先编定的程序对若干水质参数进行连续 或不连续的在线自动采样和测定。</skos:definition>
        <skos:prefLabel xml:lang="en">on-line automatic monitoring of water quality</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#open-water_width -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#open-water_width">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegimeObservation"/>
        <skos:definition xml:lang="zh-cn">测验断面上固定冰层覆盖以外的自由水面宽度。</skos:definition>
        <skos:prefLabel xml:lang="en">open-water width</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#open_channel -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#open_channel">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">具有自由表面水流的通道。根据它的形成可分为天然明渠和 人工明渠，前者如天然河道，后者如人工输水渠道、运河以及未充 满水流的管道等。</skos:definition>
        <skos:prefLabel xml:lang="en">open channel</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#open_channel_flow -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#open_channel_flow">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RiverRegime"/>
        <skos:definition xml:lang="zh-cn">天然或人工河流中具有自由水面的水流。</skos:definition>
        <skos:prefLabel xml:lang="en">open channel flow</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#operational_forecasting -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#operational_forecasting">
