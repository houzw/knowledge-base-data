        <skos:prefLabel xml:lang="en">moisture property of mire</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#moisture_transmission_rate_and_moisture_wind_joint_enlargement -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#moisture_transmission_rate_and_moisture_wind_joint_enlargement">
