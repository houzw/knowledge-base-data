    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#torrential_rain">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">降雨强度和降雨量均相当大的雨。lh内降雨量等于或大于16mm，或连续12h降雨量等于或大于30mm，或连续24h降雨量 等于或大于50mm的降水。按降水强度大小，暴雨又分为三个等 级，即24h降水量大于或等于50mm的称暴雨；100mm〜200mm 的称大暴雨；200mm以上的称特大暴雨。</skos:definition>
        <skos:prefLabel xml:lang="en">torrential rain</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tortuosity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tortuosity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">沿河流中心线两点间的实际长度与其直线距离的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">tortuosity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#total_amount_control_of_sewage_discharge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#total_amount_control_of_sewage_discharge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#QualityAssessment"/>
        <skos:definition xml:lang="zh-cn">在一定水体功能、水质标准和目标要求下，对水域允许排入的污染物总量的限定、监督和管理。</skos:definition>
        <skos:prefLabel xml:lang="en">total amount control of sewage discharge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#total_amount_of_sewage_discharge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#total_amount_of_sewage_discharge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#QualityAssessment"/>
        <skos:definition xml:lang="zh-cn">某一时段内从排污口排出的某种污染物的总量，是该时段内 污水的总排放量与该污染物平均浓度的乘积、瞬间污染物浓度的 时间积分值或排污系数统计值。</skos:definition>
        <skos:prefLabel xml:lang="en">total amount of sewage discharge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#total_dissolved_solids -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#total_dissolved_solids">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">水样经过过滤后，在一定的温度下烘干，所得的固体残渣，包 括不易挥发的可溶性盐类、有机物及能通过滤器的不溶性微粒等。</skos:definition>
        <skos:prefLabel xml:lang="en">total dissolved solids</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#total_inflow_of_river_network -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#total_inflow_of_river_network">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">雨水降落到地面后，某一时段内产生的地面流、壤中流和地下 径流，通过不同介质面，或经过地面调蓄，分时段汇入河网的水流。</skos:definition>
        <skos:prefLabel xml:lang="en">total inflow of river network</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#total_oxygen_demand -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#total_oxygen_demand">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">水中全部还原性物质氧化成简单稳定的无机物质及有机物质 过程中所消耗的氧量。</skos:definition>
        <skos:prefLabel xml:lang="en">total oxygen demand</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#total_runoff -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#total_runoff">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">在一定时段内通过水文测验断面的总水量。</skos:definition>
        <skos:prefLabel xml:lang="en">total runoff</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#total_sediment_load -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#total_sediment_load">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Sediment"/>
        <skos:definition xml:lang="zh-cn">通过某一过水断面的全部泥沙，为床沙质和冲泻质的总和，或 悬移质和推移质的总和。</skos:definition>
        <skos:definition xml:lang="zh-cn">通过河流某一断面的悬移质和推移质的总沙量。</skos:definition>
        <skos:prefLabel xml:lang="en">total sediment load</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#total_storage_capacity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#total_storage_capacity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">校核洪水位以下的水库容积。</skos:definition>
        <skos:prefLabel xml:lang="en">total storage capacity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tour_gauging -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tour_gauging">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Monitoring"/>
        <skos:definition xml:lang="zh-cn">水文专业人员以巡回流动的方式定期或不定期地对一个地区 或流域内各观测点的流量等水文要素所进行的观测作业。</skos:definition>
        <skos:prefLabel xml:lang="en">tour gauging</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tour_gauging_base -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tour_gauging_base">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#MonitoringSettings"/>
        <skos:definition xml:lang="zh-cn">为实施某一区域或流域的水文巡测而设立的，集人员、装备、 信息处理、通信保障、技术指导、物资配送于一体的巡测工作辖区 业务管理中心。</skos:definition>
        <skos:prefLabel xml:lang="en">tour gauging base</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tour_gauging_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tour_gauging_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">对部分或全部水文要素视其变化定时或不定时到现场进行测 验的水文测站。</skos:definition>
        <skos:prefLabel xml:lang="en">tour gauging station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tow_cable -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tow_cable">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:prefLabel xml:lang="en">tow cable</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tracer -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tracer">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">注入水流中的一种离子、化合物或放射性物质，以跟踪水流中 某种成分的变化过程。</skos:definition>
        <skos:prefLabel xml:lang="en">tracer</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tracking-type_wire_weight_stage_gauge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tracking-type_wire_weight_stage_gauge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StageWaterTemporatureAndWaveObservationDevices"/>
        <skos:definition xml:lang="zh-cn">具有自动升降及控制功能的悬锤式水位计，可以自动测得水 面位置，并输出水面数据。</skos:definition>
        <skos:prefLabel xml:lang="en">tracking-type wire weight stage gauge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tranquil_break-up -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tranquil_break-up">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegime"/>
        <skos:definition xml:lang="zh-cn">水势平缓，水位、流量没有急剧变化，主要由热力因素作用引 起的开河。</skos:definition>
        <skos:prefLabel xml:lang="en">tranquil break-up</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#transboundary_river -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#transboundary_river">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:prefLabel xml:lang="en">transboundary river</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#transboundary_river_gauging_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#transboundary_river_gauging_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">在河流与国境界、行政区界的交汇处附近设置的水文测站。 通常分为国界站（国际河流水文站）、行政区界站等。</skos:definition>
        <skos:prefLabel xml:lang="en">transboundary river gauging station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#transformation_among_precipitation_and_surface_water_and_groundwater -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#transformation_among_precipitation_and_surface_water_and_groundwater">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterResources"/>
        <skos:definition xml:lang="zh-cn">降水、地表水、地下水之间的水量交换和平衡关系。</skos:definition>
        <skos:prefLabel xml:lang="en">transformation among precipitation and surface water and groundwater</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#transformation_among_precipitation_surface_water_soil_water_and_groundwater -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#transformation_among_precipitation_surface_water_soil_water_and_groundwater">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterResources"/>
        <skos:definition xml:lang="zh-cn">降水、地表水、土壤水、地下水之间的水量交换和平衡关系。</skos:definition>
        <skos:prefLabel xml:lang="en">transformation among precipitation surface water soil water and groundwater</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#transformation_experiment_among_precipitation-_surface_water_and_groundwater -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#transformation_experiment_among_precipitation-_surface_water_and_groundwater">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffExperiment"/>
        <skos:definition xml:lang="zh-cn">针对降水、地表水、地下水之间的水量循环和平衡关系而开展的科学实验。</skos:definition>
        <skos:prefLabel xml:lang="en">transformation experiment among precipitation- surface water and groundwater</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#transformation_experiment_among_precipitation-surface_water_-_soil_water_and_groundwater -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#transformation_experiment_among_precipitation-surface_water_-_soil_water_and_groundwater">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffExperiment"/>
        <skos:definition xml:lang="zh-cn"> 针对降水、地表水、土壤水、地下水之间的水量循环和平衡关 系而开展的科学实验。</skos:definition>
        <skos:prefLabel xml:lang="en">transformation experiment among precipitation-surface water - soil water and groundwater</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#transformation_of_pollutant -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#transformation_of_pollutant">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">污染物在水体中通过物理、化学、生物的作用，改变形态或转 变为另一种污染物的过程。</skos:definition>
        <skos:prefLabel xml:lang="en">transformation of pollutant</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#transformation_of_river_pattern -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#transformation_of_river_pattern">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Sediment"/>
        <skos:definition xml:lang="zh-cn">由于来水来沙和河床边界条件的改变引起的各种河流类型间 的相互转变过程。</skos:definition>
        <skos:prefLabel xml:lang="en">transformation of river pattern</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#transition_curve -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#transition_curve">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">在两条临时曲线之间采用自然过渡或连时序过渡或内插曲线 过渡处理所定的曲线。</skos:definition>
        <skos:prefLabel xml:lang="en">transition curve</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#transmission_by_wireless_signal -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#transmission_by_wireless_signal">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">一种水文仪器信号传输的特殊方式。水文仪器在水下测量 时，以悬挂仪器的普通钢丝绳为一条通路，以水体为另一通路，构 成信号传输回路，将水下信号传到水上。</skos:definition>
        <skos:prefLabel xml:lang="en">transmission by wireless signal</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#transmissivity_coefficient -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#transmissivity_coefficient">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">含水层输水能力的参数，为渗透系数与含水层厚度的乘积。</skos:definition>
        <skos:prefLabel xml:lang="en">transmissivity coefficient</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#transpiration -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#transpiration">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">土壤中的水分经由植物叶面和枝干以水汽形式进人大气的 现象。</skos:definition>
        <skos:prefLabel xml:lang="en">transpiration</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#transport_of_pollutant -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#transport_of_pollutant">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">污染物在水体中移动、富集、分散与消失过程，包括机械迁移、 物理化学迁移和生物迁移.</skos:definition>
        <skos:prefLabel xml:lang="en">transport of pollutant</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#transverse_slope_of_water_surface -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#transverse_slope_of_water_surface">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RiverRegime"/>
        <skos:definition xml:lang="zh-cn">水流受到离心力或其他外力作用而形成的垂直于纵向水流的 水面比降。</skos:definition>
        <skos:prefLabel xml:lang="en">transverse slope of water surface</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#trapezoidal_notch_weir -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#trapezoidal_notch_weir">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">堰壁缺口形状为梯形的薄壁堰。</skos:definition>
        <skos:prefLabel xml:lang="en">trapezoidal notch weir</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#traverse_survey -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#traverse_survey">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">在地面上选择一条适宜的路线并在其中一些点设置测站，采 取测边和测角方式来测定这些点的水平坐标位置的作业。</skos:definition>
        <skos:prefLabel xml:lang="en">traverse survey</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tream_density -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tream_density">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">单位流域面积上的河流数目。</skos:definition>
        <skos:prefLabel xml:lang="en">tream density</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#trellis_drainage_pattern -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#trellis_drainage_pattern">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">由干支流沿着两组垂直相交的构造线发育而成的水系。</skos:definition>
        <skos:prefLabel xml:lang="en">trellis drainage pattern</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#trend_analysis -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#trend_analysis">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">分析水文序列中增加或者递减趋势的分析方法。</skos:definition>
        <skos:prefLabel xml:lang="en">trend analysis</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#trend_term -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#trend_term">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">水文系列中随时程呈增加或减少变化的成分。</skos:definition>
        <skos:prefLabel xml:lang="en">trend term</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#triangular-profile_weir -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#triangular-profile_weir">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">沿水流方向上具有三角形剖面的长底堰。</skos:definition>
        <skos:prefLabel xml:lang="en">triangular-profile weir</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#triangular_notch_weir -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#triangular_notch_weir">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">堰壁缺口形状为等腰三角形的薄壁堰。</skos:definition>
        <skos:prefLabel xml:lang="en">triangular notch weir</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#triangulation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#triangulation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">在地面上布设一系列连续三角形控制点，量测基线长度及测 定各三角形顶点水平坐标位置的作业。</skos:definition>
        <skos:prefLabel xml:lang="en">triangulation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#triaxial_mean_particle_diameter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#triaxial_mean_particle_diameter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">泥沙颗粒在相互垂直的长、中、短三个轴方向量得长度的几何 平均值。</skos:definition>
        <skos:prefLabel xml:lang="en">triaxial mean particle diameter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tributary -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tributary">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">流入干流或湖泊的河流。</skos:definition>
        <skos:prefLabel xml:lang="en">tributary</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tropical_cyclone -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tropical_cyclone">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">发生在热带或副热带洋面上，具有有组织的对流和确定气旋 性环流的非锋面性涡旋的统称。包括热带低压、热带风暴、强热带 风暴、台风（强台风和超强台风）。</skos:definition>
        <skos:prefLabel xml:lang="en">tropical cyclone</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tropical_cyclone_rain -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tropical_cyclone_rain">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">伴随热带气旋而来的雨。</skos:definition>
        <skos:prefLabel xml:lang="en">tropical cyclone rain</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tropical_depression -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tropical_depression">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">中心附近最大风力达6级〜7级的热带气旋。</skos:definition>
        <skos:prefLabel xml:lang="en">tropical depression</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tropical_storm -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tropical_storm">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">中心附近最大风力达8级〜9级的热带气旋。</skos:definition>
        <skos:prefLabel xml:lang="en">tropical storm</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tropical_zone -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tropical_zone">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">一般指南、北纬26°纬圈之间的广大低纬地区。这一带常出 现热带风暴、东风波等热带天气系统。</skos:definition>
        <skos:prefLabel xml:lang="en">tropical zone</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#trough -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#trough">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">水平气压场中，由低压区向较高气压一方延伸突出的狭长区 域，槽线上的气压值低于两侧。</skos:definition>
        <skos:prefLabel xml:lang="en">trough</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#true_value -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#true_value">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
        <skos:definition xml:lang="zh-cn">被测量的量客观存在的值。</skos:definition>
        <skos:prefLabel xml:lang="en">true value</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#turbidity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#turbidity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">由于水体中存在微细分散的悬浮性粒子、可溶的有色有机物 质、浮游生物、微生物，使水透明度降低的程度，是水的光学性质的</skos:definition>
        <skos:prefLabel xml:lang="en">turbidity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#type_A_evaluation_of_uncertainty -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#type_A_evaluation_of_uncertainty">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
        <skos:definition xml:lang="zh-cn">用对观测列进行统计分析的方法，来评定标准不确定度。</skos:definition>
        <skos:prefLabel xml:lang="en">type A evaluation of uncertainty</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#type_B_evaluation_of_uncertainty -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#type_B_evaluation_of_uncertainty">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
        <skos:definition xml:lang="zh-cn">用不同于对观测列进行统计分析的方法，来评定标准不确定度。</skos:definition>
        <skos:prefLabel xml:lang="en">type B evaluation of uncertainty</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#typhoon -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#typhoon">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">发生在西北太平洋和中国南海，中心附近最大风力达12级或 以上的热带气旋。</skos:definition>
        <skos:prefLabel xml:lang="en">typhoon</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#typhoon_rain -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#typhoon_rain">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">伴随台风而来的雨，降雨强度和降雨量都很大。</skos:definition>
        <skos:prefLabel xml:lang="en">typhoon rain</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#typical_basin -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#typical_basin">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">按一定目的和原则选定的、能代表水文相似区域内的某些天 然水文特征的闭合流域。</skos:definition>
        <skos:prefLabel xml:lang="en">typical basin</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#typical_flood_composition_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#typical_flood_composition_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignFlood"/>
        <skos:definition xml:lang="zh-cn">从实测资料中选择几次有代表性的大洪水作为典型，以设计 断面的设计洪量作为控制，按典型洪水的各分区洪量组成比例，计 算各分区相应设计洪量的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">typical flood composition method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#typical_flood_hydrograph -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#typical_flood_hydrograph">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignFlood"/>
        <skos:definition xml:lang="zh-cn">按一定原则和要求，选一次或几次用于推求设计洪水过程线 的实测或调查的洪水过程线。</skos:definition>
        <skos:prefLabel xml:lang="en">typical flood hydrograph</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#typical_hydrograph -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#typical_hydrograph">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">用作设计依据的实测或调查的水文过程线。</skos:definition>
        <skos:prefLabel xml:lang="en">typical hydrograph</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#typical_storm -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#typical_storm">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ProbableMaximumPrecipitationAndProbableMaximumFlood"/>
        <skos:definition xml:lang="zh-cn">推算设计暴雨或可能最大降水所需的实测特大暴雨，包括暴雨量、时程分配和空间分布。</skos:definition>
        <skos:prefLabel xml:lang="en">typical storm</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#typical_year -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#typical_year">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">水文特征值接近设计值，其时空分布作为设计依据的年份。</skos:definition>
        <skos:prefLabel xml:lang="en">typical year</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ultra-short_wave_communication -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ultra-short_wave_communication">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataAcquisitionSystem"/>
        <skos:definition xml:lang="zh-cn">利用频率为30MHz〜300MHz(波长10m〜lm)的无线电波 进行的通信。</skos:definition>
        <skos:prefLabel xml:lang="en">ultra-short wave communication</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ultrasonic_sounder -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ultrasonic_sounder">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DepthAndCross-sectionMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">利用超声波在水中的传播特性和从水下床面的反射特性来测定水深的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">ultrasonic sounder</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ultrasonic_sounder_for_boat -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ultrasonic_sounder_for_boat">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DepthAndCross-sectionMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">安装在测船上，能自动测量水深的超声波测深仪。其自动化程度较高。</skos:definition>
        <skos:prefLabel xml:lang="en">ultrasonic sounder for boat</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ultrasonic_sounder_for_cableway -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ultrasonic_sounder_for_cableway">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DepthAndCross-sectionMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">可以应用于缆道测深的超声波测深仪。</skos:definition>
        <skos:prefLabel xml:lang="en">ultrasonic sounder for cableway</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ultrasonic_stage_recorder -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ultrasonic_stage_recorder">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StageWaterTemporatureAndWaveObservationDevices"/>
        <skos:definition xml:lang="zh-cn">将换能器安装在水下或水上某一已知高程位置，通过记录换 能器发射的声脉冲在换能器与水面之间的往返传播时间和声波传 播速度来计算测量水位的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">ultrasonic stage recorder</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#unbiasedness -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#unbiasedness">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">样本参数估值在待估参数的真值附近摆动，对待估参数的真 值无偏倚。</skos:definition>
        <skos:prefLabel xml:lang="en">unbiasedness</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#uncertainty -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#uncertainty">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
        <skos:definition xml:lang="zh-cn">被测量值按一定规定概率可能出现置信上下限的区间界值。</skos:definition>
        <skos:prefLabel xml:lang="en">uncertainty</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#underground_erosion -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#underground_erosion">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironment"/>
        <skos:definition xml:lang="zh-cn">渗透到地下的水分对岩土的水蚀作用。</skos:definition>
        <skos:prefLabel xml:lang="en">underground erosion</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#underground_river -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#underground_river">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">在喀斯特地区由潜水溶蚀石灰岩形成的经常流水的地下通道。</skos:definition>
        <skos:prefLabel xml:lang="en">underground river</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#underlying_surface -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#underlying_surface">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">与大气下层直接接触的地球表面，包括地形、地质、土壤和植 被等，是影响气候的重要因素之一。</skos:definition>
        <skos:prefLabel xml:lang="en">underlying surface</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#underwater_battery_container -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#underwater_battery_container">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Device"/>
        <skos:definition xml:lang="zh-cn">为产生水下仪器的交直流信号而设置在水下的电池电源。</skos:definition>
        <skos:prefLabel xml:lang="en">underwater battery container</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#underwater_electrode_plate -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#underwater_electrode_plate">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Device"/>
        <skos:definition xml:lang="zh-cn">在测流断面水下放置的具有一定面积的金属板，并以导线与 测流信号接收仪器相连构成“无线”测流信号接收回路。</skos:definition>
        <skos:prefLabel xml:lang="en">underwater electrode plate</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#underwater_ice -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#underwater_ice">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegime"/>
        <skos:definition xml:lang="zh-cn">在水面或冰面以下任何部位结成的冰或冰结合体。</skos:definition>
        <skos:prefLabel xml:lang="en">underwater ice</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#underwater_signaling_apparatus -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#underwater_signaling_apparatus">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Device"/>
        <skos:definition xml:lang="zh-cn">为传输水文仪器的测量信号，在水下产生交直流信号的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">underwater signaling apparatus</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#underwater_topographic_survey -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#underwater_topographic_survey">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">测量江河、湖、库、海水底地形点的高程及其平面位置，以绘制 水下地形图的作业。</skos:definition>
        <skos:prefLabel xml:lang="en">underwater topographic survey</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#undulating_range_of_surface -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#undulating_range_of_surface">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservation"/>
        <skos:definition xml:lang="zh-cn">河流或其他水体的自由水面因风向和风力的影响而出现上下 波动的程度。</skos:definition>
        <skos:prefLabel xml:lang="en">undulating range of surface</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#uniform_flow -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#uniform_flow">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RiverRegime"/>
        <skos:definition xml:lang="zh-cn">流速大小和方向沿流程不变，流线为平行直线的水流。</skos:definition>
        <skos:prefLabel xml:lang="en">uniform flow</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#unit_discharge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#unit_discharge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">单位时间内通过某一垂线为中心的单位宽度的水体体积。</skos:definition>
        <skos:prefLabel xml:lang="en">unit discharge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#unit_hydrograph -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#unit_hydrograph">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">在给定的流域上，单位时段内时空分布均勻的单位净雨量，在 流域出口断面所形成的地表径流过程线。</skos:definition>
