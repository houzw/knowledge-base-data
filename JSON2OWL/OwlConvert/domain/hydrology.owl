    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#mean_flow_velocity_at_a_cross-section -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#mean_flow_velocity_at_a_cross-section">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">断面的流量与其过水断面面积的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">mean flow velocity at a cross-section</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#mean_flow_velocity_at_a_segment -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#mean_flow_velocity_at_a_segment">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">两相邻测速垂线的垂线平均流速的平均值，或岸边垂线流速 乘以相应系数的积。</skos:definition>
        <skos:prefLabel xml:lang="en">mean flow velocity at a segment</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#mean_flow_velocity_at_a_vertical -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#mean_flow_velocity_at_a_vertical">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">测速垂线上各测点流速的加权平均值。</skos:definition>
        <skos:prefLabel xml:lang="en">mean flow velocity at a vertical</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#mean_grain-size_distribution_in_a_cross-section -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#mean_grain-size_distribution_in_a_cross-section">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">垂线平均小于某粒径沙重百分数乘以相应垂线间的部分输沙 率的乘积之和与各垂线间部分输沙率之和的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">mean grain-size distribution in a cross-section</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#mean_grain-size_distribution_in_avertical -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#mean_grain-size_distribution_in_avertical">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">垂线上各测点小于某粒径沙重百分数乘以相应测点输沙率的 乘积之和与相应测点输沙率之和的比值，或测点小于某粒径沙重 百分数乘以相应不同权系数的测点输沙率的乘积之和与不同权系 数的测点输沙率之和的比值，如采用积深法取得的样品，它的颗粒 级配就是垂线平均颗粒级配。</skos:definition>
        <skos:prefLabel xml:lang="en">mean grain-size distribution in avertical</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#mean_particle_diameter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#mean_particle_diameter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">粒径组平均粒径为粒径组上下限粒径几何平均值。样品的平 均粒径为各粒径组平均粒径按各粒径组相应沙重百分数加权的平 均值。</skos:definition>
        <skos:prefLabel xml:lang="en">mean particle diameter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#mean_relative_error_of_every_velocity_stage -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#mean_relative_error_of_every_velocity_stage">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">各检定流速段内，所有流速检定点的流速检定相对误差的平均值。</skos:definition>
        <skos:prefLabel xml:lang="en">mean relative error of every velocity stage</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#mean_sediment_concentration_at_a_cross-section -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#mean_sediment_concentration_at_a_cross-section">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">断面输沙率与断面流量的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">mean sediment concentration at a cross-section</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#mean_sediment_concentration_at_a_vertical -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#mean_sediment_concentration_at_a_vertical">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">垂线上按测点流速和测点部分水深加权的测点含沙量平均 值，或单宽输沙率与单宽流量的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">mean sediment concentration at a vertical</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#mean_sediment_settling_velocity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#mean_sediment_settling_velocity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">粒径组平均沉速为粒径组上下限粒径沉速的几何平均值。断 面平均沉速为各粒径组平均沉速按各粒径组相应沙重百分数加权 的平均值。</skos:definition>
        <skos:prefLabel xml:lang="en">mean sediment settling velocity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#mean_stage -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#mean_stage">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservation"/>
        <skos:definition xml:lang="zh-cn">某观测点不同时段水位的均值或同一水体各观测点同时水位的均值。</skos:definition>
        <skos:prefLabel xml:lang="en">mean stage</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#mean_time_between_failures -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#mean_time_between_failures">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InstrumentAndMeasuring"/>
        <skos:definition xml:lang="zh-cn">可修复产品在相邻两次故障之间工作时间的数学期望值。</skos:definition>
        <skos:prefLabel xml:lang="en">mean time between failures</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#mean_value -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#mean_value">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">水文要素时空变化平均情况的量值分时段平均值和空间平 均值。</skos:definition>
        <skos:prefLabel xml:lang="en">mean value</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#mean_width_of_lake -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#mean_width_of_lake">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">湖泊面积与湖泊长度的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">mean width of lake</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#measured_depth -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#measured_depth">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DepthAndCrossSectionMeasurement"/>
        <skos:definition xml:lang="zh-cn">从测深器具所直接读得的水面至床面的长度值。</skos:definition>
        <skos:prefLabel xml:lang="en">measured depth</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#measured_value -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#measured_value">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">采用测量仪器、测具直接对某被测量的量进行观测（测定）所 得的量值。</skos:definition>
        <skos:prefLabel xml:lang="en">measured value</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#measurement -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#measurement">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
        <skos:definition xml:lang="zh-cn">以确定量值为目的的一组操作。</skos:definition>
        <skos:prefLabel xml:lang="en">measurement</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#measurement_of_soil_water_constant -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#measurement_of_soil_water_constant">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SoilMoistureMonitoring"/>
        <skos:definition xml:lang="zh-cn">在规定条件下测量土壤特征性含水量的一组操作。</skos:definition>
        <skos:prefLabel xml:lang="en">measurement of soil water constant</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#measuring_cross-section -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#measuring_cross-section">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">在测验河段内进行水文要素测验的横断面。</skos:definition>
        <skos:prefLabel xml:lang="en">measuring cross-section</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#measuring_cup -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#measuring_cup">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#GroundwaterObservationDevices"/>
        <skos:definition xml:lang="zh-cn">一种简单的地下水位测具。连接在测绳上的酒盅型测盅，撞 击接触水面时发出声响，由此判别测量地下水面位置。</skos:definition>
        <skos:prefLabel xml:lang="en">measuring cup</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#measuring_error_of_stage_gauge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#measuring_error_of_stage_gauge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">水位计测值和水位约定真值的差值。室内试验时，水位约定 真值是水位检定试验台提供的准确水位值;在现场应用试验时，水 位约定真值是人工观读水尺或性能更好的水位计测值。水位计测 量误差用绝对误差或以水位测量范围的相对值表示。宜用置信水 平为95%的水位测量范围的相对不确定度表示。</skos:definition>
        <skos:prefLabel xml:lang="en">measuring error of stage gauge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#measuring_error_of_tipping_bucket -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#measuring_error_of_tipping_bucket">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">翻斗测量降雨量的误差，用相对误差表示，基本上就是翻斗雨 量计的测量误差。</skos:definition>
        <skos:prefLabel xml:lang="en">measuring error of tipping bucket</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#measuring_platform -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#measuring_platform">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">悬吊在水文缆道下面，可沿测验断面做水平或垂直移动，用以 悬挂测验仪器或携带测验人员进行测验的载体。</skos:definition>
        <skos:prefLabel xml:lang="en">measuring platform</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#measuring_point_sampling_error -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#measuring_point_sampling_error">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
        <skos:definition xml:lang="zh-cn">在一次流量测验过程中用有限的测点测速所造成的流量的误差。</skos:definition>
        <skos:prefLabel xml:lang="en">measuring point sampling error</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#measuring_range -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#measuring_range">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">用仪器的测量下限和测量上限表示的测量区间。</skos:definition>
        <skos:prefLabel xml:lang="en">measuring range</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#measuring_reach -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#measuring_reach">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">为测量水文要素，按照一定技术要求，在河流上选择对水位流 量关系稳定性起控制作用，并设有相应测验设施的河段。</skos:definition>
        <skos:prefLabel xml:lang="en">measuring reach</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#measuring_standard_for_flowvelocity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#measuring_standard_for_flowvelocity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">用于定义、实现、保存或复现流速观测值的测量系统装置。</skos:definition>
        <skos:prefLabel xml:lang="en">measuring standard for flowvelocity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#measuring_standard_for_rainfall -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#measuring_standard_for_rainfall">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">用于定义、实现、保存或复现雨量观测值的测量系统装置。</skos:definition>
        <skos:prefLabel xml:lang="en">measuring standard for rainfall</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#measuring_standard_for_stage -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#measuring_standard_for_stage">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">用于定义、实现、保存或复现水位量值的测量系统装置。</skos:definition>
        <skos:prefLabel xml:lang="en">measuring standard for stage</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#measuring_vertical_sampling_error -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#measuring_vertical_sampling_error">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
        <skos:definition xml:lang="zh-cn">在一次流量测量过程中由于垂线数目的限制所造成流量的误差</skos:definition>
        <skos:prefLabel xml:lang="en">measuring vertical sampling error</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#median -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#median">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">频率曲线上，频率50%对应的变量值。</skos:definition>
        <skos:prefLabel xml:lang="en">median</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#median_particle_diameter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#median_particle_diameter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">小于某粒径的沙重百分数为50%的粒径。</skos:definition>
        <skos:prefLabel xml:lang="en">median particle diameter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#medical_organization_sewage -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#medical_organization_sewage">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">医疗机构门诊、病房、手术室、各类检验室、病理解剖室、放射 室、洗衣房、太平间等处排出的诊疗、生活及粪便污水。</skos:definition>
        <skos:prefLabel xml:lang="en">medical organization sewage</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#medium_Hood -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#medium_Hood">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
