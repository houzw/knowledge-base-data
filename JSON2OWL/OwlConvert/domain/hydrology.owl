        <skos:prefLabel xml:lang="en">standard deviation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#standard_for_drainage_of_surface_water -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#standard_for_drainage_of_surface_water">
