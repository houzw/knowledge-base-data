        <skos:prefLabel xml:lang="en">precipitation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#precipitation_assessment -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#precipitation_assessment">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SurveyAndAssessment"/>
