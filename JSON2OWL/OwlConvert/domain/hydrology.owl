        <skos:prefLabel xml:lang="en">lower limit water level forflood control</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lowest_stage -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lowest_stage">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservation"/>
        <skos:definition xml:lang="zh-cn">一定时段内，某观测点所出现的瞬时最低水位。</skos:definition>
        <skos:prefLabel xml:lang="en">lowest stage</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lumped_hydrologic_model -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lumped_hydrologic_model">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicModel"/>
        <skos:definition xml:lang="zh-cn">不考虑水文现象及其要素空间分布的水文数学模型。</skos:definition>
        <skos:prefLabel xml:lang="en">lumped hydrologic model</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lunar_day -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lunar_day">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">某一地点，月球连续两次上（下）中天的间隔时间。</skos:definition>
        <skos:prefLabel xml:lang="en">lunar day</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lunar_transit -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lunar_transit">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">月球经过测站子午圈的时间。天顶的一次称“上中天”，天底 的一次称“下中天”。</skos:definition>
        <skos:prefLabel xml:lang="en">lunar transit</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lysimeter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lysimeter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservationDevices"/>
        <skos:definition xml:lang="zh-cn">为研究水文循环中的下渗、径流和蒸散发等而设置的带有地 面、地下排水和土壤水测定装置的容器。</skos:definition>
        <skos:prefLabel xml:lang="en">lysimeter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#magnetostrictive_stage_gauge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#magnetostrictive_stage_gauge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StageWaterTemporatureAndWaveObservationDevices"/>
        <skos:definition xml:lang="zh-cn">由测杆、环状浮球、电子仓组成，环状浮球在测杆上随水位升 降，测杆内部波导丝内传播的脉冲通过磁致伸缩效应可以测到环 状浮球的精确位置，得到水位的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">magnetostrictive stage gauge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#main-stream_control_gauging_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#main-stream_control_gauging_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">按规定控制面积，为探索大河水文要素沿程变化规律而布设的水文测站。</skos:definition>
        <skos:prefLabel xml:lang="en">main-stream control gauging station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#main_cable -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#main_cable">
