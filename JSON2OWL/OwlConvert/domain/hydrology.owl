        <skos:prefLabel xml:lang="en">stationary datum</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#stationary_gauging -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#stationary_gauging">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Monitoring"/>
        <skos:definition xml:lang="zh-cn">水文专业人员驻站进行水文测报的作业。</skos:definition>
        <skos:prefLabel xml:lang="en">stationary gauging</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#stationary_gauging_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#stationary_gauging_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">由水文专业人员驻守进行水文测验的水文测站。</skos:definition>
        <skos:prefLabel xml:lang="en">stationary gauging station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#stationary_stochastic_process -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#stationary_stochastic_process">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">统计特征不随时间变化的随机过程。</skos:definition>
        <skos:prefLabel xml:lang="en">stationary stochastic process</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#statistical_parameter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#statistical_parameter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">综合反映水文变量统计规律的一组特征值。</skos:definition>
        <skos:prefLabel xml:lang="en">statistical parameter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#statistical_test_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#statistical_test_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">能人工生成水文系列的随机模拟方法。</skos:definition>
        <skos:prefLabel xml:lang="en">statistical test method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#steady_flow -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#steady_flow">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RiverRegime"/>
        <skos:definition xml:lang="zh-cn">任何一处的流速、压强、密度、水深等运动要素不随时间变化 的水流。</skos:definition>
        <skos:prefLabel xml:lang="en">steady flow</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#steady_infiltration -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#steady_infiltration">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
