        <skos:prefLabel xml:lang="en">water balance test</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#water_body -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#water_body">
