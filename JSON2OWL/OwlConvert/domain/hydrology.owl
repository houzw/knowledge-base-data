    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#peat_mire">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Mire"/>
        <skos:definition xml:lang="zh-cn">有泥炭累积的沼泽。</skos:definition>
        <skos:prefLabel xml:lang="en">peat mire</skos:prefLabel>
    </owl:NamedIndividual>
    


