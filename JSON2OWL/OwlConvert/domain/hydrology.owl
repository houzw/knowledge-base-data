    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#water_demand_for_ecosystem">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydroecologicalAssessment"/>
        <skos:definition xml:lang="zh-cn">将生态系统结构、功能和生态过程维持在一定水平所需要的 水量，是指一定生态保护目标对应的水生态系统对水的需求。</skos:definition>
        <skos:prefLabel xml:lang="en">water demand for ecosystem</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#water_demand_for_water_quality -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#water_demand_for_water_quality">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydroecologicalAssessment"/>
        <skos:definition xml:lang="zh-cn">将生态系统中水体水质维持在一定水平所需要的水量。 </skos:definition>
        <skos:prefLabel xml:lang="en">water demand for water quality</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#water_demand_for_water_temperature -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#water_demand_for_water_temperature">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydroecologicalAssessment"/>
        <skos:definition xml:lang="zh-cn">将生态系统中水体温度维持在一定水平所需要的水量。</skos:definition>
        <skos:prefLabel xml:lang="en">water demand for water temperature</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#water_demand_of_production -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#water_demand_of_production">
