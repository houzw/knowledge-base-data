        <skos:prefLabel xml:lang="en">wading stream gauging</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#wandering_reach -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#wandering_reach">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
