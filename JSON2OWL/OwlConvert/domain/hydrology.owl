        <skos:prefLabel xml:lang="en">direct method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#direct_thermal_stratification -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#direct_thermal_stratification">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">湖水温度沿垂线分布，上层温度较高而下层较低但不低于4°C的状况。</skos:definition>
        <skos:prefLabel xml:lang="en">direct thermal stratification</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#discharge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#discharge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RiverRegime"/>
        <owl:sameAs rdf:resource="http://www.egc.org/ont/domain/hydrology#drought_defense_warning_stage"/>
        <skos:definition xml:lang="zh-cn">单位时间内通过河渠或管道某一过水断面的水体体积。</skos:definition>
        <skos:prefLabel xml:lang="en">discharge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#discharge_and_sediment_discharge_relation_curve_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#discharge_and_sediment_discharge_relation_curve_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">以实测断面输沙率与对应的流量所建立的相关关系曲线，用 以推求断面输沙率的一种方法。</skos:definition>
        <skos:prefLabel xml:lang="en">discharge and sediment discharge relation curve method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#discharge_backrouting -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#discharge_backrouting">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">根据水量平衡原理，由下断面流量反推上断面流量的工作。按设计站与参证站控制面积比对参证站水文统计参数和设计 值所进行的改正。</skos:definition>
        <skos:prefLabel xml:lang="en">discharge backrouting</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#discharge_coefficient -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#discharge_coefficient">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">建筑物测流的流量公式中，表达实际流量与理论流量相联系 的系数。</skos:definition>
        <skos:prefLabel xml:lang="en">discharge coefficient</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#discharge_flow -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#discharge_flow">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">当河流切割含水层时，地下水沿河呈带状排泄。</skos:definition>
        <skos:prefLabel xml:lang="en">discharge flow</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#discharge_for_representative_channel_storage -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#discharge_for_representative_channel_storage">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">在马斯京根法中，指与河段槽蓄量成单一线性关系的流量，其 值等于该槽蓄量所对应的恒定流量。</skos:definition>
        <skos:prefLabel xml:lang="en">discharge for representative channel storage</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#discharge_hydrograph_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#discharge_hydrograph_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">以实测流量为纵坐标及对应时间为横坐标点绘在图中，参照 水位变化过程将各实测点连接成过程线，用以推求流量的一种方法。</skos:definition>
        <skos:prefLabel xml:lang="en">discharge hydrograph method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#discharge_measurement -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#discharge_measurement">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">测量单位时间内通过河渠或管道某一横断面的水体体积的 作业。</skos:definition>
        <skos:prefLabel xml:lang="en">discharge measurement</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#discharge_measurement_of_tidal_day -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#discharge_measurement_of_tidal_day">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfTidalRiver"/>
        <skos:definition xml:lang="zh-cn">在感潮河进行一个完整潮汐日的连续流量测验的全部作业。</skos:definition>
        <skos:prefLabel xml:lang="en">discharge measurement of tidal day</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#discharge_region_of_phreatic_water -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#discharge_region_of_phreatic_water">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">潜水出流的地区。</skos:definition>
        <skos:prefLabel xml:lang="en">discharge region of phreatic water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#dislodging_of_ice_cover -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#dislodging_of_ice_cover">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegime"/>
        <skos:definition xml:lang="zh-cn">整片或分裂成大面积的封冻冰盖，时而顺流滑动，时而停滞不 动的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">dislodging of ice cover</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#displacement_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#displacement_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">将浓缩的水样浑水置于比重瓶中称重，减去瓶加清水重，经换 算推求干沙质量的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">displacement method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#display_unit -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#display_unit">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InstrumentAndMeasuring"/>
        <skos:definition xml:lang="zh-cn">能将被测量值或其转换值以及有关信息通过指针、数字、声、 光或屏幕等直接显示出来的器件。</skos:definition>
        <skos:prefLabel xml:lang="en">display unit</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#dissolved_oxygen -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#dissolved_oxygen">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">以分子状态溶存于水中的氧。</skos:definition>
        <skos:prefLabel xml:lang="en">dissolved oxygen</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#dissolved_oxygen_sag_curve -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#dissolved_oxygen_sag_curve">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">在受需氧有机物污染的河段中，表示溶解氧浓度沿程或随时 间变化的下凹曲线。</skos:definition>
        <skos:prefLabel xml:lang="en">dissolved oxygen sag curve</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#distance_from_initial_point -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#distance_from_initial_point">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DepthAndCrossSectionMeasurement"/>
        <skos:definition xml:lang="zh-cn">在测验断面上，以一岸断面桩为起始点，沿断面方向至另一岸 断面桩间任一点的水平距离。</skos:definition>
        <skos:prefLabel xml:lang="en">distance from initial point</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#distance_survey -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#distance_survey">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">测量地面上两点水平距离的作业。</skos:definition>
        <skos:prefLabel xml:lang="en">distance survey</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#distortion_of_flood_wave -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#distortion_of_flood_wave">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">洪水波波峰处的运动速度大于波前的任一点，使波前的长度 不断减小，附加比降增大，而波后的长度不断增加，附加比降绝对 值不断减小的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">distortion of flood wave</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#distributed_database -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#distributed_database">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicDatabase"/>
        <skos:definition xml:lang="zh-cn">以分布处理方式所支持的一种数据库，其数据不是全部存贮 在一个地区的一台服务器上，而是分散在一个计算机网络的各台 服务器上。</skos:definition>
        <skos:prefLabel xml:lang="en">distributed database</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#distributed_hydrologic_model -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#distributed_hydrologic_model">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicModel"/>
        <skos:definition xml:lang="zh-cn">考虑水文现象及其要素空间分布的水文数学模型。</skos:definition>
        <skos:prefLabel xml:lang="en">distributed hydrologic model</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#distributed_region_of_phreatic_water -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#distributed_region_of_phreatic_water">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">潜水含水层的分布范围。</skos:definition>
        <skos:prefLabel xml:lang="en">distributed region of phreatic water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#distribution_curve -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#distribution_curve">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">某时刻或时段，水文要素值沿空间分布所连成的曲线。</skos:definition>
        <skos:prefLabel xml:lang="en">distribution curve</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#distribution_of_design_annual_runoff_within_a_year -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#distribution_of_design_annual_runoff_within_a_year">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignAnnualRunoff"/>
        <skos:definition xml:lang="zh-cn">设计年径流量在年内各月的分配值或百分数。</skos:definition>
        <skos:prefLabel xml:lang="en">distribution of design annual runoff within a year</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#district_boundary_gauging_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#district_boundary_gauging_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">在出、入行政管辖区境河流与行政区界交汇处附近设置的水 文测站。通常分为省界站、市界站、县（区）界站等。</skos:definition>
        <skos:prefLabel xml:lang="en">district boundary gauging station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#diurnal_tide -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#diurnal_tide">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">在一太阴日内，出现一次高潮和一次低潮的潮。</skos:definition>
        <skos:prefLabel xml:lang="en">diurnal tide</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#diurnal_tide_inequality -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#diurnal_tide_inequality">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">一太阴日内发生两潮不规则的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">diurnal tide inequality</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#divergence_zone_of_water_vapor -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#divergence_zone_of_water_vapor">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">水汽向四周扩散的地区。</skos:definition>
        <skos:prefLabel xml:lang="en">divergence zone of water vapor</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#divide_line -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#divide_line">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">分水岭最高点的连线。</skos:definition>
        <skos:prefLabel xml:lang="en">divide line</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#dominant_discharge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#dominant_discharge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Sediment"/>
        <skos:definition xml:lang="zh-cn">对造床作用最显著的某一级流量，即流量比较大，历时又比较 长的某一级流量。</skos:definition>
        <skos:prefLabel xml:lang="en">dominant discharge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#dominant_species_of_aquatic_organism -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#dominant_species_of_aquatic_organism">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#AquaticEcologyMonitoring"/>
        <skos:definition xml:lang="zh-cn">对水生生物群落的存在和发展有决定性作用的个体数量最多 的生物种。</skos:definition>
        <skos:prefLabel xml:lang="en">dominant species of aquatic organism</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#double_float -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#double_float">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">由上下两个浮标组成，下浮标具有很小的负浮力，用来感应所 在处水层的流速，很小的上浮标起浮托和标志作用，中间以细线相 连，用来测定水面以下一定水层水流速度的浮标。</skos:definition>
        <skos:prefLabel xml:lang="en">double float</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#double_frequency_ultrasonic_sounder -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#double_frequency_ultrasonic_sounder">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DepthAndCross-sectionMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">应用两种测深频率，可以测得水底淤积情况，自动化程度较高 的超声波测深仪。</skos:definition>
        <skos:prefLabel xml:lang="en">double frequency ultrasonic sounder</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#double_mass_curve -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#double_mass_curve">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">以两个地区或流域水文要素或两个不同水文要素的累加值为 纵、横坐标点绘的曲线。</skos:definition>
        <skos:prefLabel xml:lang="en">double mass curve</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#double_weight_function_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#double_weight_function_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">在权函数法基础上，可改善对偏态系数估计的统计参数估计方法。</skos:definition>
        <skos:prefLabel xml:lang="en">double weight function method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#downstream -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#downstream">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">靠近河口或与水流正常方向一致的河段。</skos:definition>
        <skos:prefLabel xml:lang="en">downstream</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#downstream_flow -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#downstream_flow">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">向下游方向流的水流。</skos:definition>
        <skos:prefLabel xml:lang="en">downstream flow</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#drainage_area -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#drainage_area">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">流域地面分水线与河口断面之间所包围的平面面积。</skos:definition>
        <skos:prefLabel xml:lang="en">drainage area</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#drainage_basin -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#drainage_basin">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:prefLabel xml:lang="en">drainage basin</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#drainage_basin_of_groundwater -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#drainage_basin_of_groundwater">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">地面以下由地下水分水岭所限定的汇水区域的统称。</skos:definition>
        <skos:prefLabel xml:lang="en">drainage basin of groundwater</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#drainage_density -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#drainage_density">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">单位流域面积上的河流长度。</skos:definition>
        <skos:prefLabel xml:lang="en">drainage density</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#drainage_divide -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#drainage_divide">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">分隔相邻两个流域的山岭或高地，河水从这里流向两个相反的方向。</skos:definition>
        <skos:prefLabel xml:lang="en">drainage divide</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#drainage_of_subsurface_water -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#drainage_of_subsurface_water">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DrainageOfSurfaceWaterAndDrainageOfSubsurfaceWater"/>
        <skos:definition xml:lang="zh-cn">降低地下水位或消除耕作层上层滞水过多，以免除作物渍害 的工程技术措施。</skos:definition>
        <skos:prefLabel xml:lang="en">drainage of subsurface water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#drainage_of_surface_water -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#drainage_of_surface_water">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DrainageOfSurfaceWaterAndDrainageOfSubsurfaceWater"/>
        <skos:definition xml:lang="zh-cn">排除城市积水或农田内因当地降雨过多而产生的危害作物正 常生长的多余地表水分的工程技术措施。</skos:definition>
        <skos:prefLabel xml:lang="en">drainage of surface water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#drainage_system -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#drainage_system">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">干流、支流和流域内的湖泊、沼泽或地下暗河相互连接组成的 系统。</skos:definition>
        <skos:prefLabel xml:lang="en">drainage system</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#drainage_system_characteristics -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#drainage_system_characteristics">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">河流水系的拓扑学特征与几何学特征的总称。拓扑学特征可 用节点、链来描述，几何学特征可用河长、河弯、弯曲率、河底比降 等来描述。</skos:definition>
        <skos:prefLabel xml:lang="en">drainage system characteristics</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#drainage_system_investigation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#drainage_system_investigation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicInvestigation"/>
        <skos:definition xml:lang="zh-cn">收集量算流域或地区内主要河流、湖泊等水体的几何形态特 征值的工作。</skos:definition>
        <skos:prefLabel xml:lang="en">drainage system investigation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#drift_current -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#drift_current">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">由盛行风对湖面的持续作用所产生的切应力而引起的湖水 运动。</skos:definition>
        <skos:prefLabel xml:lang="en">drift current</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#drift_ice -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#drift_ice">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegime"/>
        <skos:definition xml:lang="zh-cn">冰块或兼有少量冰花随水流流动的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">drift ice</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#drifting_ice_pack -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#drifting_ice_pack">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegime"/>
        <skos:definition xml:lang="zh-cn">冰块或冰花在流动中受阻停滞并堆积于局部河段的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">drifting ice pack</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#drive-point_sampler -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#drive-point_sampler">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#GroundwaterObservationDevices"/>
        <skos:definition xml:lang="zh-cn">直接压入含水层，让地下水通过采样器外壳上的通孔进入采 样器内部的地下水采样器。</skos:definition>
        <skos:prefLabel xml:lang="en">drive-point sampler</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#dropping_pressure_type_stage_recorder -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#dropping_pressure_type_stage_recorder">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StageWaterTemporatureAndWaveObservationDevices"/>
        <skos:definition xml:lang="zh-cn">测压传感器放置在水下固定测点直接测量水压强的压力式水 位计。采用的测压传感器主要分为压阻式和陶瓷电容式。 </skos:definition>
        <skos:prefLabel xml:lang="en">dropping pressure type stage recorder</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#drought -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#drought">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">由于降水稀少等自然因素以及人为原因，对农业、工业、畜牧 业、生活及环境等正常用水造成缺水以致受害的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">drought</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#drought_defense -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#drought_defense">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">为消除或减轻干旱灾害而采取的措施。</skos:definition>
        <skos:prefLabel xml:lang="en">drought defense</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#drought_defense_warning_stage -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#drought_defense_warning_stage">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">江河湖库水位持续偏低，流量持续偏少，影响城乡生活、工农 业生产、生态环境等用水安全，应采取抗旱措施的水位（流量）。</skos:definition>
        <skos:prefLabel xml:lang="en">drought defense warning stage</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#drought_degree -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#drought_degree">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">根据干旱指标对干旱程度的分级。</skos:definition>
        <skos:prefLabel xml:lang="en">drought degree</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#drought_index -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#drought_index">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">表示干旱程度的单因子及多因子组成的综合因子。</skos:definition>
        <skos:prefLabel xml:lang="en">drought index</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#drought_regime -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#drought_regime">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">降水、灌溉和供水满足不了正常用水量的情况，通常包括雨 情、土壤墒情、工程蓄水情况和作物受旱情况等。</skos:definition>
        <skos:prefLabel xml:lang="en">drought regime</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#dry_density_of_reservoir_deposition -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#dry_density_of_reservoir_deposition">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfReservior"/>
        <skos:definition xml:lang="zh-cn">水库淤积物单位体积的质量。</skos:definition>
        <skos:prefLabel xml:lang="en">dry density of reservoir deposition</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#dry_deposition -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#dry_deposition">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">不发生降水时，大气中酸性污染物受重力、颗粒物吸附等作用 由大气沉降到地面的过程。</skos:definition>
        <skos:prefLabel xml:lang="en">dry deposition</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#dune_tracking_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#dune_tracking_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">在推移质呈沙波运行的河流上，通过施测河床地形，了解沙波 的尺度和运行速度，用以推求推移质输沙量的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">dune tracking method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#duration_curve -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#duration_curve">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">某一水文要素等于或大于不同定值的持续时间曲线。</skos:definition>
        <skos:prefLabel xml:lang="en">duration curve</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#duration_curve_of_tidal_level -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#duration_curve_of_tidal_level">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">一种反映潮位及其相对历时的关系曲线，又称潮位历时累积 频率曲线。</skos:definition>
        <skos:prefLabel xml:lang="en">duration curve of tidal level</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#duration_of_ebb_tidal_current -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#duration_of_ebb_tidal_current">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">从涨潮憩流至下一个落潮憩流的间隔时间。</skos:definition>
        <skos:prefLabel xml:lang="en">duration of ebb tidal current</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#duration_of_flood_tidal_current -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#duration_of_flood_tidal_current">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">从落潮憩流至下一个涨潮憩流的间隔时间。</skos:definition>
        <skos:prefLabel xml:lang="en">duration of flood tidal current</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#duration_of_submergence_tolerance_of_crop -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#duration_of_submergence_tolerance_of_crop">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DrainageOfSurfaceWaterAndDrainageOfSubsurfaceWater"/>
        <skos:definition xml:lang="zh-cn">在一定淹水深度内，对农作物不致引起明显减产的允许淹水 时间。</skos:definition>
        <skos:prefLabel xml:lang="en">duration of submergence tolerance of crop</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#duration_of_tidal_current -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#duration_of_tidal_current">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">相邻两次落潮憩流的间隔时间。</skos:definition>
        <skos:prefLabel xml:lang="en">duration of tidal current</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#duration_of_tidal_fall -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#duration_of_tidal_fall">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">一个潮期内，从高潮位至随后的低潮位的间隔时间。</skos:definition>
        <skos:prefLabel xml:lang="en">duration of tidal fall</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#duration_of_tidal_rise -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#duration_of_tidal_rise">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">一个潮期内，从低潮位至随后的高潮位的间隔时间。</skos:definition>
        <skos:prefLabel xml:lang="en">duration of tidal rise</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#duration_of_tide -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#duration_of_tide">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">相邻两次低（高）潮的间隔时间。</skos:definition>
        <skos:prefLabel xml:lang="en">duration of tide</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#dyke -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#dyke">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <owl:sameAs rdf:resource="http://www.egc.org/ont/domain/hydrology#levee"/>
        <skos:prefLabel xml:lang="en">dyke</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#dynamic_programming -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#dynamic_programming">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterConservancyComputation"/>
        <skos:definition xml:lang="zh-cn">求解多阶段决策过程的数学规划。</skos:definition>
        <skos:prefLabel xml:lang="en">dynamic programming</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#dynamic_storage -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#dynamic_storage">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">水库实际水面与坝上游水位水平面之间的容积。</skos:definition>
        <skos:prefLabel xml:lang="en">dynamic storage</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#dynamic_wave -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#dynamic_wave">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">圣•维南方程组的动量方程中各项均不忽略所描述的洪水波。</skos:definition>
        <skos:prefLabel xml:lang="en">dynamic wave</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#dystrophic_lake -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#dystrophic_lake">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">湖泊中氮、磷等营养物质贫乏，藻类、浮游生物和叶绿素很少， 耗氧量小，透明度大的湖泊。</skos:definition>
        <skos:prefLabel xml:lang="en">dystrophic lake</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#easterly_wave -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#easterly_wave">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">热带地区稳定深厚东风气流中由东向西移动的一种波状 扰动。</skos:definition>
        <skos:prefLabel xml:lang="en">easterly wave</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ebb_tidal_current -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ebb_tidal_current">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">沿河槽向海洋流动的潮流。</skos:definition>
        <skos:prefLabel xml:lang="en">ebb tidal current</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ebb_tidal_range -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ebb_tidal_range">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">高潮位至随后的低潮位的差值。</skos:definition>
        <skos:prefLabel xml:lang="en">ebb tidal range</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ebb_tidal_volume -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ebb_tidal_volume">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfTidalRiver"/>
        <skos:definition xml:lang="zh-cn">涨潮憩流至落潮憩流之间通过某断面的总水量。</skos:definition>
        <skos:prefLabel xml:lang="en">ebb tidal volume</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ebbtide -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ebbtide">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">一个潮期内水位下降的过程。</skos:definition>
        <skos:prefLabel xml:lang="en">ebbtide</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ecohydrology -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ecohydrology">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrology"/>
        <skos:definition xml:lang="zh-cn">研究水体中或是陆地上水与生态系统的相互作用，包括蒸散 与植物用水，生物对其水环境的适应性，植被对河川径流与河流功 能的影响，以及生态过程与水文循环间的反馈机制等的学科。</skos:definition>
        <skos:prefLabel xml:lang="en">ecohydrology</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ecological_and_environmental_water_use -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ecological_and_environmental_water_use">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DevelopmentAndUtilization"/>
        <skos:definition xml:lang="zh-cn">通过人为措施补充给生态与环境所需的水量，可分为城镇和农村生态与环境用水两类。城镇生态与环境用水包括河湖补水和 绿化、环境卫生等用水，农村生态与环境用水指对湖泊沼泽湿地补 水、林草植被建设、地下水回灌等用水。</skos:definition>
        <skos:prefLabel xml:lang="en">ecological and environmental water use</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ecological_assessment -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ecological_assessment">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydroecologicalAssessment"/>
        <skos:definition xml:lang="zh-cn">生态评估是指通过一定的技术手段对人类赖以生存的外部环 境的生态功能及其提供生态服务的能力水平进行评价和价值判断 的行为和过程。</skos:definition>
        <skos:prefLabel xml:lang="en">ecological assessment</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ecological_balance -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ecological_balance">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydroecology"/>
        <skos:definition xml:lang="zh-cn">生态系统的结构和功能均处于适应与协调的动态平衡状态。</skos:definition>
        <skos:prefLabel xml:lang="en">ecological balance</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ecological_base_flow -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ecological_base_flow">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydroecologicalAssessment"/>
        <skos:definition xml:lang="zh-cn">维持河流生态系统运转的基本流量。与河流生态系统的结构 和功能有关，与流域的气候、土壤、地质和其他诸多因素有关，同时 还与河流水文的动态特征密切相关。</skos:definition>
        <skos:prefLabel xml:lang="en">ecological base flow</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ecological_benefit -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ecological_benefit">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydroecology"/>
        <skos:definition xml:lang="zh-cn">自然生态系统及其生态过程所形成的维持人类赖以生存的环 境条件及其提供的服务。</skos:definition>
        <skos:prefLabel xml:lang="en">ecological benefit</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ecological_compensation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ecological_compensation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EcologicalRestoration"/>
        <skos:definition xml:lang="zh-cn">通过行政干预手段，使生态效益受益者向生态效益源所有者 和经营者进行经济补偿。为保证生态补偿的正常和顺利实施而设 立的专项资金称为生态补偿基金。</skos:definition>
        <skos:prefLabel xml:lang="en">ecological compensation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ecological_effect -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ecological_effect">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydroecology"/>
        <skos:definition xml:lang="zh-cn">生物因子或非生物因子，在其存在或活动过程中，对其所在生 态系统中的结构、功能所产生的影响。</skos:definition>
        <skos:prefLabel xml:lang="en">ecological effect</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ecological_environment_problem -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ecological_environment_problem">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydroecologicalAssessment"/>
        <skos:definition xml:lang="zh-cn">由于人类活动引起的自然生态系统退化、环境质量恶化及由 此衍生的不良生态环境效应，包括土壤侵蚀、沙漠化、酸雨、土壤盐 渍化、草地退化、生物多样性丧失与水环境污染等。</skos:definition>
        <skos:prefLabel xml:lang="en">ecological environment problem</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ecological_environment_susceptibility -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ecological_environment_susceptibility">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EcologicalRestoration"/>
        <skos:definition xml:lang="zh-cn">生态环境中部分环境要素或因子对人类活动反应的敏感程度，用来反映产生生态失衡与生态环境问题的可能性大小。</skos:definition>
        <skos:prefLabel xml:lang="en">ecological environment susceptibility</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ecological_flow -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ecological_flow">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydroecologicalAssessment"/>
        <skos:definition xml:lang="zh-cn">生态需水中的某个流量，具有某种生态作用。</skos:definition>
        <skos:prefLabel xml:lang="en">ecological flow</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ecological_function_regionalization -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ecological_function_regionalization">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EcologicalRestoration"/>
        <skos:definition xml:lang="zh-cn">根据区域生态环境要素、生态环境敏感性与生态服务功能空 间分异规律，将区域划分成不同生态功能区的过程。</skos:definition>
        <skos:prefLabel xml:lang="en">ecological function regionalization</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ecological_impact_assessment -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ecological_impact_assessment">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydroecologicalAssessment"/>
        <skos:definition xml:lang="zh-cn">评价特定的过程或措施对生态系统或其组成可能带来的各种 影响进行的调查、监测和分析活动。</skos:definition>
        <skos:prefLabel xml:lang="en">ecological impact assessment</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ecological_process -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ecological_process">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EcologicalRestoration"/>
        <skos:definition xml:lang="zh-cn">生态系统中物质、能量、信息的输入、输出、流动、转化、储存与 分配。包括食物链、生态系统演替、能量流动、物质循环、反馈控制等过程。</skos:definition>
        <skos:prefLabel xml:lang="en">ecological process</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ecological_protection_target -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ecological_protection_target">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydroecologicalAssessment"/>
        <skos:definition xml:lang="zh-cn">根据生态系统的需要、社会需要和社会期望，由有关政府部门 设定，要实现的生态系统状态。其为生态系统管理的一部分。</skos:definition>
        <skos:prefLabel xml:lang="en">ecological protection target</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ecological_restoration -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ecological_restoration">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EcologicalRestoration"/>
        <skos:definition xml:lang="zh-cn">按照自然规律，采用适当的技术手段对退化或被破坏的生态 系统进行修复的过程。</skos:definition>
        <skos:prefLabel xml:lang="en">ecological restoration</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ecological_risk_assessment -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ecological_risk_assessment">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydroecologicalAssessment"/>
        <skos:definition xml:lang="zh-cn">评价由于一种或多种外界因素导致可能发生或正在发生对生 态带来不利影响的行为和过程。</skos:definition>
        <skos:prefLabel xml:lang="en">ecological risk assessment</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ecological_safety -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ecological_safety">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydroecologicalAssessment"/>
        <skos:definition xml:lang="zh-cn">生态系统自然平衡状态和生态环境退化的程度。</skos:definition>
        <skos:prefLabel xml:lang="en">ecological safety</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ecological_water_consumption -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ecological_water_consumption">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydroecologicalAssessment"/>
        <skos:definition xml:lang="zh-cn">维系生态系统生物群落生存和一定生态环境质量而实际消耗 的水资源量。</skos:definition>
        <skos:prefLabel xml:lang="en">ecological water consumption</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ecotone -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ecotone">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydroecology"/>
        <skos:definition xml:lang="zh-cn">两个不同群落交界的区域，亦称生态过渡带。</skos:definition>
        <skos:prefLabel xml:lang="en">ecotone</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#effective_depth -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#effective_depth">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DepthAndCrossSectionMeasurement"/>
        <skos:definition xml:lang="zh-cn">在冰期，冰底或冰花底至床面的垂直距离。</skos:definition>
        <skos:prefLabel xml:lang="en">effective depth</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#effective_head -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#effective_head">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RiverRegime"/>
        <skos:definition xml:lang="zh-cn">可用于水电开发某一河段的落差值。</skos:definition>
        <skos:prefLabel xml:lang="en">effective head</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#effective_porosity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#effective_porosity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">岩石中相互连通的孔隙体积与岩土层总体积的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">effective porosity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#efficiency -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#efficiency">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">两种参数估计方法相比，其估计量抽样分布密集性高的估计 方法有效性好，反之有效性差。</skos:definition>
        <skos:prefLabel xml:lang="en">efficiency</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#efficient_irrigation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#efficient_irrigation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DevelopmentAndUtilization"/>
        <skos:definition xml:lang="zh-cn">充分利用灌溉水源，最大限度提高灌溉用水效率与效益的灌 溉模式。</skos:definition>
        <skos:prefLabel xml:lang="en">efficient irrigation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#electric_conductivity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#electric_conductivity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">水传导电流的能力，以能过单位面积的电流除以单位长度的 电压降落来量度。</skos:definition>
        <skos:prefLabel xml:lang="en">electric conductivity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#electric_ice_drill -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#electric_ice_drill">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegimeObservationDevices"/>
        <skos:definition xml:lang="zh-cn">以电为动力的冰钻。</skos:definition>
        <skos:prefLabel xml:lang="en">electric ice drill</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#electric_resistence_soil_hydrometer -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#electric_resistence_soil_hydrometer">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SoilMoistureMonitoriDevices"/>
        <skos:definition xml:lang="zh-cn">应用多孔介质块(石膏）随含水量变化电阻变化的原理，测量 其电阻，得到含水量的土壤水分测定仪。</skos:definition>
        <skos:prefLabel xml:lang="en">electric resistence soil hydrometer</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#electric_wave_current_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#electric_wave_current_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">向水面发射与接收无线电波，利用其频率变化与水流速度成 正比的关系而制成的流速仪。</skos:definition>
        <skos:prefLabel xml:lang="en">electric wave current meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#electromagnetic_current_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#electromagnetic_current_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">应用电磁感应原理，测量仪器所在点流速的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">electromagnetic current meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#electromagnetic_current_profile -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#electromagnetic_current_profile">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">应用电磁感应原理，测量较小断面平均流速的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">electromagnetic current profile</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#electromagnetic_flow-direction_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#electromagnetic_flow-direction_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">可以测量流向的电磁流速仪。</skos:definition>
        <skos:prefLabel xml:lang="en">electromagnetic flow-direction meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#electromagnetic_gauging_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#electromagnetic_gauging_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">利用电磁感应原理，根据水流切割磁场所产生的感应电势与 水流速度成正比的关系来测定流速，并结合断面资料以推求流量 的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">electromagnetic gauging method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#electromagnetic_pipe_flow_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#electromagnetic_pipe_flow_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">应用电磁感应原理测量管内流速的管道流量计。</skos:definition>
        <skos:prefLabel xml:lang="en">electromagnetic pipe flow meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#electronic_gauge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#electronic_gauge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StageWaterTemporatureAndWaveObservationDevices"/>
        <skos:definition xml:lang="zh-cn">仪器呈水尺形状，安装在水中，尺体上外露等距离的导电触 点，尺体内部的电路检测到被水淹没的触点，从而测得水位的 仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">electronic gauge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#electronic_water_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#electronic_water_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#GroundwaterObservationDevices"/>
        <skos:definition xml:lang="zh-cn">具有数字计量等自动化功能的水表。</skos:definition>
        <skos:prefLabel xml:lang="en">electronic water meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#element_of_groundwater_regime -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#element_of_groundwater_regime">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#GroundwaterObservation"/>
        <skos:definition xml:lang="zh-cn">地下水的水位（或埋深）、水量、水温和化学成分等要素的总称。</skos:definition>
        <skos:prefLabel xml:lang="en">element of groundwater regime</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#elevation_head -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#elevation_head">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RiverRegime"/>
        <skos:definition xml:lang="zh-cn">从某一水准基面起算的单位质量水体所具有的位置势能。</skos:definition>
        <skos:prefLabel xml:lang="en">elevation head</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#elevation_of_ice_bottom_and_discharge_relation_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#elevation_of_ice_bottom_and_discharge_relation_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
