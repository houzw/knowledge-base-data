        <skos:prefLabel xml:lang="en">tidal table</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tide -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tide">
