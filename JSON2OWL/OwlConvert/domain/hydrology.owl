        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ProbableMaximumPrecipitationAndProbableMaximumFlood"/>
        <skos:definition xml:lang="zh-cn">用最大可降水与典型可降水的比值放大暴雨。</skos:definition>
        <skos:prefLabel xml:lang="en">moisture enlargement</skos:prefLabel>
    </owl:NamedIndividual>
    


