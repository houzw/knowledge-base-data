    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#storm_duration">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignStorm"/>
        <skos:definition xml:lang="zh-cn">一次降雨大于某给定值的部分降雨的持续时间。</skos:definition>
        <skos:prefLabel xml:lang="en">storm duration</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#storm_enlargement -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#storm_enlargement">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ProbableMaximumPrecipitationAndProbableMaximumFlood"/>
        <skos:definition xml:lang="zh-cn">将影响降水的水汽、动力等主要因子合理放大典型暴雨的 过程。</skos:definition>
        <skos:prefLabel xml:lang="en">storm enlargement</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#storm_flood -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#storm_flood">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">由暴雨所形成的洪水。</skos:definition>
        <skos:prefLabel xml:lang="en">storm flood</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#storm_flood_forecasting -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#storm_flood_forecasting">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">根据前期和现时的场次暴雨等有关资料，对暴雨形成的洪水 过程所作的预报。</skos:definition>
        <skos:prefLabel xml:lang="en">storm flood forecasting</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#storm_investigation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#storm_investigation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicInvestigation"/>
        <skos:definition xml:lang="zh-cn">为查明有关地区暴雨的暴雨量、时程分配、空间分布、天气系 统、灾情、重现期等所进行的调查工作。</skos:definition>
        <skos:prefLabel xml:lang="en">storm investigation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#storm_isoline_map -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#storm_isoline_map">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignStorm"/>
        <skos:definition xml:lang="zh-cn">描述暴雨等值线以用于查算设计地点设计暴雨的图。</skos:definition>
        <skos:prefLabel xml:lang="en">storm isoline map</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#storm_reduction_index -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#storm_reduction_index">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignStorm"/>
        <skos:definition xml:lang="zh-cn">在雨强历时曲线中，反映降雨强度随历时递增速率递减的 参数。</skos:definition>
        <skos:prefLabel xml:lang="en">storm reduction index</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#storm_runoff -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#storm_runoff">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">由暴雨所形成的河川径流。</skos:definition>
        <skos:prefLabel xml:lang="en">storm runoff</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#storm_runoff_relevant_graph_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#storm_runoff_relevant_graph_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignFlood"/>
        <skos:definition xml:lang="zh-cn">通过建立降雨量、前期影响雨量、径流量三变量相关图，用降 雨量减去相应的流域损失量来计算产流量的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">storm runoff relevant graph method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#storm_surge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#storm_surge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">由于热带气旋、温带气旋、冷锋等强烈的天气系统过境，其所 伴随的强风作用和气压骤变引起的局部海面非周期性异常升降 现象。</skos:definition>
        <skos:prefLabel xml:lang="en">storm surge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#storm_surge_forecasting -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#storm_surge_forecasting">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">根据风力、风向、气压等气象要素变化，对沿海高潮位接近、达 到或超过当地警戒潮位时所发布的预报。</skos:definition>
        <skos:prefLabel xml:lang="en">storm surge forecasting</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#storm_surge_inundation_area -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#storm_surge_inundation_area">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#CommonTerms"/>
        <skos:prefLabel xml:lang="en">storm surge inundation area</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#storm_track -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#storm_track">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignStorm"/>
        <skos:definition xml:lang="zh-cn">暴雨中心移动的轨迹。</skos:definition>
        <skos:prefLabel xml:lang="en">storm track</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#storm_transposition_correction -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#storm_transposition_correction">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ProbableMaximumPrecipitationAndProbableMaximumFlood"/>
        <skos:definition xml:lang="zh-cn">在暴雨移置时考虑流域形状、水汽、入流障碍高程等影响因素 的改正。</skos:definition>
        <skos:prefLabel xml:lang="en">storm transposition correction</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#storm_transposition_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#storm_transposition_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ProbableMaximumPrecipitationAndProbableMaximumFlood"/>
        <skos:definition xml:lang="zh-cn">将邻近地区特大暴雨的时面深关系或等雨量线图，移用到同 一气候区内可能发生相同暴雨的设计流域，并对移置暴雨进行改 正和放大来推求可能最大降水的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">storm transposition method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#straight_reach -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#straight_reach">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">河流两侧岸边水边线大体平行，弯曲率小的河段.</skos:definition>
        <skos:prefLabel xml:lang="en">straight reach</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#stream_gauging_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#stream_gauging_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">设在河、渠、湖、库上以测定水位、流量为主的水文测站。根据 需要还可兼测降水、水面蒸发、泥沙、墒情、地下水、水质、气象要素</skos:definition>
        <skos:prefLabel xml:lang="en">stream gauging station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#stream_gauging_station_network -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#stream_gauging_station_network">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StationNetwork"/>
        <skos:definition xml:lang="zh-cn">由观测流量的国家基本水文测站和专用水文测站组成的水文 资料收集系统。它是水文站网中最重要的一种。</skos:definition>
        <skos:prefLabel xml:lang="en">stream gauging station network</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#stream_link -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#stream_link">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:prefLabel xml:lang="en">stream link</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#stream_net -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#stream_net">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:prefLabel xml:lang="en">stream net</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#stream_order -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#stream_order">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:prefLabel xml:lang="en">stream order</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#stream_source -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#stream_source">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:prefLabel xml:lang="en">stream source</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#streamflow -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#streamflow">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">河流中的水流。</skos:definition>
        <skos:prefLabel xml:lang="en">streamflow</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#streams -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#streams">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:prefLabel xml:lang="en">streams</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#structural_characteristics_of_basin -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#structural_characteristics_of_basin">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">流域的结构特征主要包括河网密度、河道维持常数、河流频 度、流域平均坡度和流域不对称系数等。</skos:definition>
        <skos:prefLabel xml:lang="en">structural characteristics of basin</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#structural_measure_of_flood_control -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#structural_measure_of_flood_control">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">通过修建堤防、水库、蓄滞洪区等水利工程的手段，以防御洪 水、减免洪水灾害损失的措施。</skos:definition>
        <skos:prefLabel xml:lang="en">structural measure of flood control</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#structure_stream_gauging -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#structure_stream_gauging">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">利用标准型式的测流建筑物或河渠中已建的水工建筑物来测 定流量的作业。</skos:definition>
        <skos:prefLabel xml:lang="en">structure stream gauging</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#subbasin -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#subbasin">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:prefLabel xml:lang="en">subbasin</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#subcritical_flow -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#subcritical_flow">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RiverRegime"/>
        <skos:definition xml:lang="zh-cn">流速小于波速，外界干扰引起的水面波动能逆流向上游传播 的水流，其弗汝德数Fr&amp;lt;1。</skos:definition>
        <skos:prefLabel xml:lang="en">subcritical flow</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#submerged_bar -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#submerged_bar">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">河槽中经常淹没在水下的沙洲。</skos:definition>
        <skos:prefLabel xml:lang="en">submerged bar</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#submerged_flow -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#submerged_flow">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">受测流建筑物下游水位影响的水流。</skos:definition>
        <skos:prefLabel xml:lang="en">submerged flow</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#submergence_coefficient -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#submergence_coefficient">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">淹没流流量与上游水头相同的自由流流量的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">submergence coefficient</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#submergence_ratio -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#submergence_ratio">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">以堰顶为基准面计算的下游水头与上游水头之比。</skos:definition>
        <skos:prefLabel xml:lang="en">submergence ratio</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#subsurface_float -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#subsurface_float">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">测定水下流速的浮标，其被水流拖曳作用最大的部分位于水 面以下。</skos:definition>
        <skos:prefLabel xml:lang="en">subsurface float</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#subsurface_flow -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#subsurface_flow">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:prefLabel xml:lang="en">subsurface flow</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#subsurface_waterlogging -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#subsurface_waterlogging">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DrainageOfSurfaceWaterAndDrainageOfSubsurfaceWater"/>
        <skos:definition xml:lang="zh-cn">由于地下水位过高或上层滞水，导致耕作层土壤过湿，影响作 物正常生长发育，造成作物减产的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">subsurface waterlogging</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#subtropical_high -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#subtropical_high">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">中心位于副热带地区的暖性高压系统。</skos:definition>
        <skos:prefLabel xml:lang="en">subtropical high</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#subtropical_zone -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#subtropical_zone">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">一般指南、北纬26°-40°纬圈之间的地区，为热带和温带间的 过渡带。副热带高压基本控制着这一带的天气和气候。</skos:definition>
        <skos:prefLabel xml:lang="en">subtropical zone</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#succession -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#succession">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydroecology"/>
        <skos:definition xml:lang="zh-cn">一定区域内，一种生物群落被另一种生物群落所取代的过程。</skos:definition>
        <skos:prefLabel xml:lang="en">succession</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#sum_of_tidal_volume -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#sum_of_tidal_volume">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfTidalRiver"/>
        <skos:definition xml:lang="zh-cn">在一潮流期内，落潮潮量（正值）与涨潮潮量（负值）的代数和， 其正值称净泄量，负值称净进量。</skos:definition>
        <skos:prefLabel xml:lang="en">sum of tidal volume</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#summer_Hood -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#summer_Hood">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">夏季三伏前后由暴雨引起的洪水。</skos:definition>
        <skos:prefLabel xml:lang="en">summer Hood</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#supercritical_flow -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#supercritical_flow">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RiverRegime"/>
        <skos:definition xml:lang="zh-cn">流速大于波速，外界干扰引起的水面波动不能向上游传播的 水流，其弗汝德数Fr&amp;gt;1。</skos:definition>
        <skos:prefLabel xml:lang="en">supercritical flow</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#supply_and_demand_analysis_of_water_resources -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#supply_and_demand_analysis_of_water_resources">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterResourcesPlanning"/>
        <skos:definition xml:lang="zh-cn">在水资源分区内，对不同水平年、不同保证率水资源供求关系 和余缺量所进行的分析研究。</skos:definition>
        <skos:prefLabel xml:lang="en">supply and demand analysis of water resources</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#surface_detention -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#surface_detention">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">在降水期间部分雨水暂时滞留在地面的现象，不包括填洼。</skos:definition>
        <skos:prefLabel xml:lang="en">surface detention</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#surface_erosion -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#surface_erosion">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Sediment"/>
        <skos:definition xml:lang="zh-cn">降雨和径流使坡地表土比较均匀剥蚀的一种水力侵蚀。</skos:definition>
        <skos:prefLabel xml:lang="en">surface erosion</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#surface_float -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#surface_float">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">漂浮于水流表层用来测量水面流速的浮标。</skos:definition>
        <skos:prefLabel xml:lang="en">surface float</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#surface_flow_of_mire -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#surface_flow_of_mire">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Mire"/>
        <skos:definition xml:lang="zh-cn">在沼泽较低的凹地上的表面水流。</skos:definition>
        <skos:prefLabel xml:lang="en">surface flow of mire</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#surface_layer_flow_of_mire -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#surface_layer_flow_of_mire">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Mire"/>
        <skos:definition xml:lang="zh-cn">在沼泽表层多孔介质中的渗流。</skos:definition>
        <skos:prefLabel xml:lang="en">surface layer flow of mire</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#surface_runoff -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#surface_runoff">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">沿地表向河流、湖泊、沼泽、海洋等汇集的水流。</skos:definition>
        <skos:prefLabel xml:lang="en">surface runoff</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#surface_slope -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#surface_slope">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RiverRegime"/>
        <skos:definition xml:lang="zh-cn">沿水流方向，单位水平距离水面的高程差。</skos:definition>
        <skos:prefLabel xml:lang="en">surface slope</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#surface_water -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#surface_water">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#CommonTerms"/>
        <skos:definition xml:lang="zh-cn">存在于河流、湖泊、水库、沼泽、冰川和冰盖等水体中的水分的总称。</skos:definition>
        <skos:prefLabel xml:lang="en">surface water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#surface_water_pollution -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#surface_water_pollution">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">污染物进入江河、湖泊和水库等地表水体，并导致水质下降的 现象。</skos:definition>
        <skos:prefLabel xml:lang="en">surface water pollution</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#surface_water_recharge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#surface_water_recharge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">因地表水与地下水间的水头差，使地表水入渗补给地下水的过程。</skos:definition>
        <skos:prefLabel xml:lang="en">surface water recharge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#surface_water_resources_amount -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#surface_water_resources_amount">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SurveyAndAssessment"/>
        <skos:definition xml:lang="zh-cn">河流、湖泊、冰川等地表水体逐年更新的动态水量，即天然河 川径流量。</skos:definition>
        <skos:prefLabel xml:lang="en">surface water resources amount</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#surface_water_resources_assessment -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#surface_water_resources_assessment">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SurveyAndAssessment"/>
        <skos:definition xml:lang="zh-cn">以河流、湖泊、冰川、沼泽等地表水体作为评价对象，对其数 量、质量、赋存条件、时空分布特征、运动规律和幵发利用条件等进 行的分析评定。</skos:definition>
        <skos:prefLabel xml:lang="en">surface water resources assessment</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#surface_waterlogging -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#surface_waterlogging">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DrainageOfSurfaceWaterAndDrainageOfSubsurfaceWater"/>
        <skos:definition xml:lang="zh-cn">由于降水、分洪等导致城市地面积水或者耕作层土壤过湿或作物受淹，影响作物正常生长发育，造成作物减产的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">surface waterlogging</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#surfacewater_hydrology -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#surfacewater_hydrology">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrology"/>
        <skos:definition xml:lang="zh-cn">研究地球表面不同水体(坡面流、河流、湖泊、湿地、河口、海洋等)水文现象形成、发展变化规律及其相互联系的学科。</skos:definition>
        <skos:prefLabel xml:lang="en">surfacewater hydrology</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#suspended_cableway -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#suspended_cableway">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">用柔性悬索悬吊测量仪器设备的水文缆道。</skos:definition>
        <skos:prefLabel xml:lang="en">suspended cableway</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#suspended_capillary_water -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#suspended_capillary_water">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">在不受地下水补给的情况下，地表上层土壤由于降雨或灌水， 借藉毛管作用所能保持的地表入渗水</skos:definition>
        <skos:prefLabel xml:lang="en">suspended capillary water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#suspended_ice_cover -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#suspended_ice_cover">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegime"/>
        <skos:definition xml:lang="zh-cn">悬于水面以上的封冻冰盖。</skos:definition>
        <skos:prefLabel xml:lang="en">suspended ice cover</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#suspended_load -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#suspended_load">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Sediment"/>
        <skos:definition xml:lang="zh-cn">受水流的紊动作用悬浮于水中并随水流移动的泥沙。</skos:definition>
        <skos:prefLabel xml:lang="en">suspended load</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#suspended_load_discharge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#suspended_load_discharge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">单位时间内通过河渠某一断面的悬移质质量。</skos:definition>
        <skos:prefLabel xml:lang="en">suspended load discharge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#suspended_load_discharge_measurement -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#suspended_load_discharge_measurement">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">测定单位时间内通过河渠某一断面的悬移质泥沙质量的 作业。</skos:definition>
        <skos:prefLabel xml:lang="en">suspended load discharge measurement</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#suspended_load_sampler -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#suspended_load_sampler">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysisDevices"/>
        <skos:definition xml:lang="zh-cn">为测定悬移质含沙量及颗粒级配，采集河渠悬移质水样的 仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">suspended load sampler</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#suspended_rod_cableway -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#suspended_rod_cableway">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">用刚性悬杆悬吊测量仪器设备的水文缆道。</skos:definition>
        <skos:prefLabel xml:lang="en">suspended rod cableway</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#suspending_cable_with_ball -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#suspending_cable_with_ball">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StageWaterTemporatureAndWaveObservationDevices"/>
        <skos:definition xml:lang="zh-cn">悬挂浮子和平衡锤的钢丝绳悬索，其上等距离地固定了小钢 球，工作时和水位轮周上等距离的凹坑准确啮合，保证悬索和水位 轮不发生滑动。</skos:definition>
        <skos:prefLabel xml:lang="en">suspending cable with ball</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#suspending_tape_with_hole -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#suspending_tape_with_hole">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StageWaterTemporatureAndWaveObservationDevices"/>
        <skos:definition xml:lang="zh-cn">悬挂浮子和平衡锤的钢带状悬索，带上等距离地穿孔，工作时 和水位轮周上等距离的销钉准确啮合，保证悬索和水位轮不发生 滑动。</skos:definition>
        <skos:prefLabel xml:lang="en">suspending tape with hole</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#suspension_cable -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#suspension_cable">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">用来悬吊铅鱼和采样器等测验仪器，并能沿铅直方向定位的 活动钢索。它可与绝缘导电芯线相连接并由岸上绞车来控制。</skos:definition>
        <skos:prefLabel xml:lang="en">suspension cable</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#sustainable_water_resources_development -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#sustainable_water_resources_development">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DevelopmentAndUtilization"/>
        <skos:definition xml:lang="zh-cn">水资源供求长期处于良性循环，不致造成可利用水量日益减 少或水体水质下降丧失使用价值，并保持生态环境良好的水资源 开发方式。</skos:definition>
        <skos:prefLabel xml:lang="en">sustainable water resources development</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#sustainable_water_resources_utilization -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#sustainable_water_resources_utilization">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DevelopmentAndUtilization"/>
        <skos:definition xml:lang="zh-cn">通过水资源合理利用，使水资源供求长期处于良性循环，不致 造成可利用水量减少，或水功能区水质下降而丧失使用价值的利 用方式</skos:definition>
        <skos:prefLabel xml:lang="en">sustainable water resources utilization</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#swamp_hydrology -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#swamp_hydrology">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrology"/>
        <skos:definition xml:lang="zh-cn">研究沼泽中水的相态、动态、水分运动与构成沼泽的生物体和 无机物之间的关系，以及沼泽与其周围环境之间的水分交换等的</skos:definition>
        <skos:prefLabel xml:lang="en">swamp hydrology</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#swampiness -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#swampiness">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironment"/>
        <skos:definition xml:lang="zh-cn">地下水位接近地表，土壤长期被水分所饱和，在湿性植物作用 和嫌气条件下，进行有机质的生物积累与矿质元素的还原过程。</skos:definition>
        <skos:prefLabel xml:lang="en">swampiness</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#synchronous_series -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#synchronous_series">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">起迄年份相同且时间对应的两个或多个水文系列。</skos:definition>
        <skos:prefLabel xml:lang="en">synchronous series</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#synoptic_process -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#synoptic_process">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">某种天气及其相应的天气系统发生、发展和消失的演变过程， 如降水（暴雨）天气过程、大风天气过程、梅雨天气过程等。</skos:definition>
        <skos:prefLabel xml:lang="en">synoptic process</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#synoptic_scale_system -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#synoptic_scale_system">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">水平尺度为1000km/3000km、时间尺度为1d/3d的天气系统，如锋面、低涡、台风等。</skos:definition>
        <skos:prefLabel xml:lang="en">synoptic scale system</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#synoptic_situation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#synoptic_situation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">天气图上温压场等的配置所显示的大范围环流型与不同类别天气系统分布的状况。</skos:definition>
        <skos:prefLabel xml:lang="en">synoptic situation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#synoptic_system -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#synoptic_system">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">按照气象要素的空间分布而划分的具有典型特征的大气运动 系统，通常指气压空间分布所组成的系统。其运动形式大都呈涡 旋状或波状，如气旋、反气旋、锋、台风、高空槽脊等。</skos:definition>
        <skos:prefLabel xml:lang="en">synoptic system</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#synthetic_correction -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#synthetic_correction">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ProbableMaximumPrecipitationAndProbableMaximumFlood"/>
        <skos:definition xml:lang="zh-cn">用反映天气因子又反映地形因子的综合指标，对移置暴雨所 作的改正。</skos:definition>
        <skos:prefLabel xml:lang="en">synthetic correction</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#synthetic_hydrograph -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#synthetic_hydrograph">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">各种不同的水文要素值绘在同一时间坐标上所组成的一组过 程线。</skos:definition>
        <skos:prefLabel xml:lang="en">synthetic hydrograph</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#synthetic_uncertainty -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#synthetic_uncertainty">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
        <skos:definition xml:lang="zh-cn">确定测量结果区间的量，合理赋予被测量之值分布的大部分 可望含于此区间。</skos:definition>
        <skos:definition xml:lang="zh-cn">随机不确定度与系统不确定度的综合。</skos:definition>
        <skos:prefLabel xml:lang="en">synthetic uncertainty</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#synthetic_unit_hydrograph -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#synthetic_unit_hydrograph">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">根据流域特征与单位线要素或参数之间的经验关系，由流域 特征推出的单位线。</skos:definition>
        <skos:prefLabel xml:lang="en">synthetic unit hydrograph</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#system_analysis_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#system_analysis_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">将要解决的问题概化为一个系统，对系统要素进行综合分析， 找出可行方案的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">system analysis method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#system_reliability -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#system_reliability">
