        <skos:definition xml:lang="zh-cn">在地下水补给中心，地下水自然形成流向相反的分界线。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater divide</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_dynamics -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_dynamics">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">研究地下水在岩土空隙中运动规律的学科.</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater dynamics</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_excessive_exploitation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_excessive_exploitation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironment"/>
        <skos:definition xml:lang="zh-cn">地下水开采量超过可开采量，使地下水位持续下降的开采。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater excessive exploitation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_exploitation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_exploitation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">在一定时段内由特定区域内地下含水层中所提取的地下水量</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater exploitation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_flow_concentration -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_flow_concentration">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">以地下径流的形式，通过岩土空隙向出口断面的汇集过程。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater flow concentration</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_flow_concentration_curve -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_flow_concentration_curve">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">单位地下入流经过地下沿程滞蓄作用，形成的地下出流过程线。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater flow concentration curve</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_flow_system -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_flow_system">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">由源到汇的流面群构成的，具有统一时空演变过程的地下 水体。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater flow system</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_hydrograph -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_hydrograph">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">地下水水位、水量、水质、水温的监测值与监测时间的关系线。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater hydrograph</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_hydrology -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_hydrology">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrology"/>
        <skos:definition xml:lang="zh-cn">研究地下水动态变化规律和特性，地下水均衡及地下水资源 评价、开发、利用、保护和管理等有关水文问题的学科。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater hydrology</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_model -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_model">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">为模拟地下水现象而建立的实体结构或数学与逻辑结构</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater model</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_movement -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_movement">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">地下水由水位差（或水头差）引起的在岩土孔隙中的流动。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater movement</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_observation_well -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_observation_well">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">为观测地下水的量、质动态变化，在水文地质单元或地下水开 采区等设置的水文测站或地下水监测井（孔）。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater observation well</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_observation_well_network -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_observation_well_network">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StationNetwork"/>
        <skos:definition xml:lang="zh-cn">由地下水观测站（井）、观测地下水的国家基本水文测站和专 用水文测站所组成的水文资料收集系统。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater observation well network</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_pollution -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_pollution">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">污染物沿包气带竖向入渗，并随地下水流扩散和输移，导致地 下水体污染的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater pollution</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_recharge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_recharge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">在天然或开采条件下，单位时间内以各种形式进入到含水层 中的水量。</skos:definition>
        <skos:definition xml:lang="zh-cn">地下水含水层自外界或相邻含水层获得水量的过程。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater recharge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_regime -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_regime">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">地下水的水位、水量、水质和水温等要素随时间和空间变化的 现象和过程。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater regime</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_regime_forecasting -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_regime_forecasting">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">根据地下水动态规律及其主要影响因素之间的相关关系，对 未来地下水位动态所作的预报。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater regime forecasting</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_regime_observation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_regime_observation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#GroundwaterObservation"/>
        <skos:definition xml:lang="zh-cn">在一个地区选择有代表性的泉、井、孔等按照一定的技术要求 进行的地下水动态要素观测、记录和资料整理的过程。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater regime observation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_regime_under_exploitation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_regime_under_exploitation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#GroundwaterObservation"/>
        <skos:definition xml:lang="zh-cn">在人工开采地下水影响下的地下水动态。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater regime under exploitation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_reserve -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_reserve">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">地下水在多年循环交替过程中积存于含水层中的重力水 体积。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater reserve</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_reservoir -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_reservoir">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">由含水层或加建地下坝所形成的地下水调蓄区。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater reservoir</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_resources -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_resources">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">赋存和运动在地层或岩层中，其质和量具有一定利用价值的 地下水。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater resources</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_resources_amount -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_resources_amount">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SurveyAndAssessment"/>
        <skos:definition xml:lang="zh-cn">地下饱和含水层逐年更新的动态水量，即降水和地表水入渗 地下水的补给量</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater resources amount</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_resources_assessment -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_resources_assessment">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SurveyAndAssessment"/>
        <skos:definition xml:lang="zh-cn">对地下水的数量、质量、赋存条件、时空分布特征、开发利用条 件及开采可能引起的环境地质问题等进行的分析评定。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater resources assessment</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_runoff -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_runoff">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">沿潜水层或隔水层间的含水层向河流、湖泊、沼泽、海洋汇集 的水流。</skos:definition>
        <skos:definition xml:lang="zh-cn">潜水层或隔水层间的含水层对河流、湖泊、沼泽、海洋潜出或潜入的水流。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater runoff</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_runoff_coefficient -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_runoff_coefficient">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">地下径流量与同一时间内含水层分布面积上降水总量的 比值。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater runoff coefficient</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_runoff_intensity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_runoff_intensity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">在单位时间内通过单位断面的地下水径流量。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater runoff intensity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_runoff_modulus -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_runoff_modulus">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">km&lt;sup&gt;2&lt;/sup&gt;含水层分布面积上的地下水径流量。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater runoff modulus</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_sampler -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_sampler">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#GroundwaterObservationDevices"/>
        <skos:definition xml:lang="zh-cn">专门用于地下水水样采取的采样器。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater sampler</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_sampling_pump -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_sampling_pump">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#GroundwaterObservationDevices"/>
        <skos:definition xml:lang="zh-cn">专门用于地下水水样采取的采样泵。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater sampling pump</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_stage -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_stage">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">观测井中地下水自由水面的高程。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater stage</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_stage_gauge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_stage_gauge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#GroundwaterObservationDevices"/>
        <skos:definition xml:lang="zh-cn">测量观测井（孔）中地下水位变化过程的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater stage gauge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_system -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_system">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">由具有水量水质输入、运行和输出的一系列相互联系的地下 水基本单元及其组合组成的系统。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater system</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_table_forecast -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_table_forecast">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterResourcesPrediction"/>
        <skos:definition xml:lang="zh-cn">根据对地下水有影响的气象、水文和人类活动的有关资料，对地下水水位的动态变化预报。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater table forecast</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#groundwater_temperature_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#groundwater_temperature_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#GroundwaterObservationDevices"/>
        <skos:definition xml:lang="zh-cn">可以直接测量测井中地下水水温的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">groundwater temperature meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#growth_ratio_of_drainage_area -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#growth_ratio_of_drainage_area">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">沿干流自上而下单位河长增长的部分流域面积与全流域面积 的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">growth ratio of drainage area</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#gully -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#gully">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">坡面水在沿坡面流动，将水集中起来的微小水道。</skos:definition>
        <skos:prefLabel xml:lang="en">gully</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#gullying -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#gullying">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Sediment"/>
