        <skos:prefLabel xml:lang="en">station-year method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#station_control -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#station_control">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">对水文站水位流量关系稳定性起控制作用的断面或河段水力 因素综合影响的总称。</skos:definition>
        <skos:prefLabel xml:lang="en">station control</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#station_datum -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#station_datum">
