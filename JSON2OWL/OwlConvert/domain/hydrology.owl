        <skos:prefLabel xml:lang="en">steady infiltration</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#stepwise_regression_analysis -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#stepwise_regression_analysis">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">通过数学方法筛选相关因子的回归分析。</skos:definition>
        <skos:prefLabel xml:lang="en">stepwise regression analysis</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#still_storage -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#still_storage">
