        <skos:prefLabel xml:lang="en">time series analysis method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#timeinvariant_hydrologic_model -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#timeinvariant_hydrologic_model">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicModel"/>
        <skos:definition xml:lang="zh-cn">参数不随时间变化的水文数学模型。</skos:definition>
        <skos:prefLabel xml:lang="en">timeinvariant hydrologic model</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#timing_error -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#timing_error">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">仪器的时间计量误差。</skos:definition>
        <skos:prefLabel xml:lang="en">timing error</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tipping-bucket_rainfall_recorder -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tipping-bucket_rainfall_recorder">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservationDevices"/>
