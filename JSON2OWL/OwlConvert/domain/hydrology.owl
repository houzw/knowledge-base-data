        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">岩土允许水体透过的能力。</skos:definition>
        <skos:prefLabel xml:lang="en">moisture permeability</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#moisture_property_of_mire -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#moisture_property_of_mire">
