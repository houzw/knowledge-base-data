        <skos:prefLabel xml:lang="en">water body</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#water_conservancy_computation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#water_conservancy_computation">
