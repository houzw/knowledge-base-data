        <skos:prefLabel xml:lang="en">output of water vapor</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#oven-dry_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#oven-dry_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
