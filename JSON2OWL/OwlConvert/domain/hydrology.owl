        <skos:prefLabel xml:lang="en">matric potential</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#maximum_allowable_discharge_amount -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#maximum_allowable_discharge_amount">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#QualityAssessment"/>
