        <skos:prefLabel xml:lang="en">sample processing</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#sample_size -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#sample_size">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">样本中所含随机变量的个数。</skos:definition>
        <skos:prefLabel xml:lang="en">sample size</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#sampling_cross-section -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#sampling_cross-section">
