        <skos:prefLabel xml:lang="en">gullying</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#habitat -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#habitat">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydroecology"/>
        <skos:definition xml:lang="zh-cn">生物个体、种群或群落能够正常生活或繁衍后代的场所。</skos:definition>
        <skos:prefLabel xml:lang="en">habitat</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hail -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hail">
