    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ground_symbol_on_monitoring_setting -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ground_symbol_on_monitoring_setting">
