        <skos:prefLabel xml:lang="en">water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#water-bearing_medium -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#water-bearing_medium">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">赋存地下水且水流在其中运动的岩土物质。渗透系数等与空间坐标无关的多孔介质。</skos:definition>
        <skos:prefLabel xml:lang="en">water-bearing medium</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#water-bearing_rock_formation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#water-bearing_rock_formation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">含水层岩性相近的岩土层所构成的统一的含水岩体。</skos:definition>
        <skos:prefLabel xml:lang="en">water-bearing rock formation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#water-bearing_rock_property -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#water-bearing_rock_property">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">含水层岩土的物理、化学和力学等因素的性质的总称。</skos:definition>
        <skos:prefLabel xml:lang="en">water-bearing rock property</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#water-bearing_rock_series -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#water-bearing_rock_series">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">由某一地质时代的不同岩土层所组成的含水岩体。</skos:definition>
        <skos:prefLabel xml:lang="en">water-bearing rock series</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#water-level_measurement_using_an_air_line -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#water-level_measurement_using_an_air_line">
