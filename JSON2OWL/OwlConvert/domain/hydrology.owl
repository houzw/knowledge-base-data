        <skos:prefLabel xml:lang="en">parallel drainage pattern</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#particle_diameter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#particle_diameter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
