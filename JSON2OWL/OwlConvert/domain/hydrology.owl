        <skos:prefLabel xml:lang="en">wading rod</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#wading_stream_gauging -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#wading_stream_gauging">
