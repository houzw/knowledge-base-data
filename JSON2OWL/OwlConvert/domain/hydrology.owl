    <!-- http://www.egc.org/ont/domain/hydrology#sampling_distribution -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#sampling_distribution">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">样本统计量的概率分布。</skos:definition>
        <skos:prefLabel xml:lang="en">sampling distribution</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#sampling_efficiency -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#sampling_efficiency">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
