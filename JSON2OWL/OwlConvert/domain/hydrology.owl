    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#perviousness_of_mire -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#perviousness_of_mire">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Mire"/>
        <skos:definition xml:lang="zh-cn">沼泽的透水能力，常以渗透系数表示。</skos:definition>
        <skos:prefLabel xml:lang="en">perviousness of mire</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#photoelectric_particle_size_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#photoelectric_particle_size_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysisDevices"/>
        <skos:definition xml:lang="zh-cn">应用消光法原理直接在浑液中测出小于某粒径沙重百分数的 仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">photoelectric particle size meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#photoelectric_sediment_concentration_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#photoelectric_sediment_concentration_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysisDevices"/>
        <skos:definition xml:lang="zh-cn">应用光电效应来测定颗粒较细、粒径均匀、含沙量较小的含沙 量仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">photoelectric sediment concentration meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#photographic_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#photographic_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">在床沙表层未被破坏、粒径较大卵石沙滩断面上取典型地点 沙样，按一定比尺照相，以推求颗粒级配的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">photographic method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#photosedimentation_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#photosedimentation_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">利用泥沙颗粒对光的吸收、散射等消光作用，连续测定泥沙浑 液沉降过程中不同时间的光密度，计算浑液的含沙密度，来推求泥 沙颗粒级配的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">photosedimentation method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#phreatic_water -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#phreatic_water">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">埋藏于地表以下第一个稳定隔水层之上，具有自由水面的地 下水。</skos:definition>
        <skos:prefLabel xml:lang="en">phreatic water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#phreatic_water_evaporation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#phreatic_water_evaporation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">潜水向包气带输送水分，并通过土壤蒸发和散发进入大气的 现象。</skos:definition>
        <skos:prefLabel xml:lang="en">phreatic water evaporation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#phreatic_water_evaporator -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#phreatic_water_evaporator">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservationDevices"/>
        <skos:definition xml:lang="zh-cn">测量潜水蒸发量的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">phreatic water evaporator</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#phreatic_water_level -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#phreatic_water_level">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">潜水的自由水面相对于某一基面的高程。</skos:definition>
        <skos:prefLabel xml:lang="en">phreatic water level</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#phreatic_water_overflow_to_surface -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#phreatic_water_overflow_to_surface">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">从含水层以泉或泉群的形式溢出地表的水量。</skos:definition>
        <skos:prefLabel xml:lang="en">phreatic water overflow to surface</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#phreatic_water_surface -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#phreatic_water_surface">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">潜水的自由水面。</skos:definition>
        <skos:prefLabel xml:lang="en">phreatic water surface</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#phreaticflow -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#phreaticflow">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">潜水在重力作用下由高处流向低处。</skos:definition>
        <skos:prefLabel xml:lang="en">phreaticflow</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#physical_transformation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#physical_transformation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">污染物在水体中的蒸发、渗透、凝聚、吸附、解吸及放射性元素 蜕变等过程。</skos:definition>
        <skos:prefLabel xml:lang="en">physical transformation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#physical_transport -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#physical_transport">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">由水的机械作用和重力作用，污染物被水体携带而进行的 搬迁。</skos:definition>
        <skos:prefLabel xml:lang="en">physical transport</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#physically_based_hydrologic_mathematic_model -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#physically_based_hydrologic_mathematic_model">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicModel"/>
        <skos:definition xml:lang="zh-cn">用数学物理方法描述水文现象内部各物理量之间相互关系的 水文数学模型。</skos:definition>
        <skos:prefLabel xml:lang="en">physically based hydrologic mathematic model</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#physicochemical_transport -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#physicochemical_transport">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">污染物在水体中发生一系列物理化学作用时所伴随的污染物 形态变化及富集。</skos:definition>
        <skos:prefLabel xml:lang="en">physicochemical transport</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#physiographical_characteristics_of_basin -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#physiographical_characteristics_of_basin">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">流域的地理位置、气候条件、岩土性质、地质构造、地形、地貌、 植被等的总称。</skos:definition>
        <skos:prefLabel xml:lang="en">physiographical characteristics of basin</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#pinnate_drainage_pattern -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#pinnate_drainage_pattern">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">干流两侧支流分布较均匀，近似羽毛状排列的水系。</skos:definition>
        <skos:prefLabel xml:lang="en">pinnate drainage pattern</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#pipe_flow_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#pipe_flow_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">测量管道单位时间内通过某一断面水流体积的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">pipe flow meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#pipet -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#pipet">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysisDevices"/>
        <skos:definition xml:lang="zh-cn">用于吸管法作颗粒分析的液管，为标准容积的玻璃质大肚型 的直管。</skos:definition>
        <skos:prefLabel xml:lang="en">pipet</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#pipet_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#pipet_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">沙样做混匀连续沉降，连续测定某一深度处固定容积悬液内 干沙质量的变化，来推求泥沙颗粒级配的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">pipet method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#pit_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#pit_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">用河床上的人工测坑测定推移质输沙率的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">pit method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#planetary_scale_system -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#planetary_scale_system">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">水平尺度为3000km/10000km、时间尺度为3d以上的天气 系统，如沿地球纬向运动的西风带、低纬度东风带、热带辐合带、副热带高压脊和高空长波槽等。</skos:definition>
        <skos:prefLabel xml:lang="en">planetary scale system</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#planned_water_use -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#planned_water_use">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DevelopmentAndUtilization"/>
        <skos:definition xml:lang="zh-cn">各用水户或用水部门依据生产计划和合理的用水定额编制用 水计划，并按计划调度配水的用水方式。</skos:definition>
        <skos:prefLabel xml:lang="en">planned water use</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#planning_for_comprehensive_water_pollution_control -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#planning_for_comprehensive_water_pollution_control">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentProtection"/>
        <skos:definition xml:lang="zh-cn">为保护水资源，防治水污染，根据规划水平年的水域水质保护目标，把水环境同社会经济发展作为一个整体制订防治水污染综 合措施方案。</skos:definition>
        <skos:prefLabel xml:lang="en">planning for comprehensive water pollution control</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#planning_for_drainage_of_surface_water -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#planning_for_drainage_of_surface_water">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DrainageOfSurfaceWaterAndDrainageOfSubsurfaceWater"/>
        <skos:definition xml:lang="zh-cn">根据受涝地区的特点，在分析涝灾成因及其规律的基础上，制 订城市排除涝水和农田调节土壤水分、控制地下水位及防治盐碱 化的专业水利规划。</skos:definition>
        <skos:prefLabel xml:lang="en">planning for drainage of surface water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#plum_rain -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#plum_rain">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">初夏江南梅子黄熟时期，中国江淮流域到日本南部一带出现 的雨期较长的连阴雨天气。</skos:definition>
        <skos:prefLabel xml:lang="en">plum rain</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#point-area_conversion_coefficient -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#point-area_conversion_coefficient">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignStorm"/>
        <skos:definition xml:lang="zh-cn">点暴雨量换算成面暴雨量所用的折减系数。</skos:definition>
        <skos:prefLabel xml:lang="en">point-area conversion coefficient</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#point-area_relationship -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#point-area_relationship">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignStorm"/>
        <skos:definition xml:lang="zh-cn">点雨深与其不同面积平均雨深的关系。</skos:definition>
        <skos:prefLabel xml:lang="en">point-area relationship</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#point-integrating_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#point-integrating_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">用流速仪在测速垂线测定各测点流速，确定垂线平均流速，或 在采沙垂线上选择一点或几点采集水样，推求流量或垂线平均含 沙量的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">point-integrating method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#point_gauge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#point_gauge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StageWaterTemporatureAndWaveObservationDevices"/>
        <skos:definition xml:lang="zh-cn">由一根针形测杆所构成的水尺。测量时将它降低直到针尖接 触水面，根据尺上读数来确定水位。</skos:definition>
        <skos:prefLabel xml:lang="en">point gauge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#point_pollution_source -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#point_pollution_source">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#QualityAssessment"/>
        <skos:definition xml:lang="zh-cn">进人水体污染物有固定范围的排污场所或装置，如城市、工矿 企业等排出的污水，有稳定的排污口。</skos:definition>
        <skos:prefLabel xml:lang="en">point pollution source</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#point_rainfall -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#point_rainfall">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">在一定时段内某一地点的降雨量。</skos:definition>
        <skos:prefLabel xml:lang="en">point rainfall</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#polling-answer_back_system -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#polling-answer_back_system">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataAcquisitionSystem"/>
        <skos:definition xml:lang="zh-cn">中心站可自动定时或随时呼叫遥测站，遥测站响应中心站的 查询并实时采集水文数据发送给中心站的遥测系统。</skos:definition>
        <skos:prefLabel xml:lang="en">polling-answer back system</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#pollutant -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#pollutant">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">由人类活动产生或由自然界释放，进入水体后使水体的正常 组成和性质发生直接或间接有害于人类的物质。</skos:definition>
        <skos:prefLabel xml:lang="en">pollutant</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#pollution_item -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#pollution_item">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#QualityAssessment"/>
        <skos:definition xml:lang="zh-cn">现状水质类别评价中单项水质浓度值不满足设定标准限值要 求的水质项目。</skos:definition>
        <skos:prefLabel xml:lang="en">pollution item</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#pollution_load -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#pollution_load">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#QualityAssessment"/>
        <skos:definition xml:lang="zh-cn">在一定时段内，进入水体的污染物质总量。</skos:definition>
        <skos:prefLabel xml:lang="en">pollution load</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#pollution_source_control -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#pollution_source_control">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#QualityAssessment"/>
        <skos:definition xml:lang="zh-cn">为改善水环境，在污染源调查的基础上，运用法律的、行政的、 经济的、技术的以及其他管理手段和措施，对污染源进行监督，控 制污水的排放量。</skos:definition>
        <skos:prefLabel xml:lang="en">pollution source control</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#pollution_source_of_water_body -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#pollution_source_of_water_body">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#QualityAssessment"/>
        <skos:definition xml:lang="zh-cn">向水体排放污染物或对水环境产生有害影响的场所、设备和 装置等，通常也包括污染物进人水体的途径。</skos:definition>
        <skos:prefLabel xml:lang="en">pollution source of water body</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#population -->

