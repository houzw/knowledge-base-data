    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#brook">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">沿着山间峡谷流动的小股水流。</skos:definition>
        <skos:prefLabel xml:lang="en">brook</skos:prefLabel>
    </owl:NamedIndividual>
    


