        <skos:prefLabel xml:lang="en">gravimetric method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#gravity_drainage -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#gravity_drainage">
