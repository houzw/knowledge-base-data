        <skos:prefLabel xml:lang="en">lower confining bed</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#lower_limit_water_level_forflood_control -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#lower_limit_water_level_forflood_control">
