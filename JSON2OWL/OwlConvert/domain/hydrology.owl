        <skos:prefLabel xml:lang="en">perched stream</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#perched_water -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#perched_water">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
