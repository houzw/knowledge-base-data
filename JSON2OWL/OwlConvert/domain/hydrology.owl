        <skos:prefLabel xml:lang="en">geomorphic unit hydrograph method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#geomorphologic_instantaneous_unit_hydrograph -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#geomorphologic_instantaneous_unit_hydrograph">
