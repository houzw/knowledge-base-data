    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#bed_load_discharge_and_hydraulic_factors_relation_curve_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">以断面推移质输沙率与水力因素所建立的相关关系曲线，用 以推求断面推移质输沙率的一种方法。</skos:definition>
        <skos:prefLabel xml:lang="en">bed load discharge and hydraulic factors relation curve method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#bed_load_discharge_measurement -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#bed_load_discharge_measurement">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">测定单位时间内通过河渠某一断面的推移质质量的作业。</skos:definition>
        <skos:prefLabel xml:lang="en">bed load discharge measurement</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#bed_load_discharge_per_unit_width -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#bed_load_discharge_per_unit_width">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">单位时间内通过单位宽度河床的推移质质量。</skos:definition>
        <skos:prefLabel xml:lang="en">bed load discharge per unit width</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#bed_load_sampler -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#bed_load_sampler">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysisDevices"/>
        <skos:definition xml:lang="zh-cn">采集推移质泥沙的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">bed load sampler</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#bed_material -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#bed_material">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Sediment"/>
        <skos:definition xml:lang="zh-cn">在受泥沙输移影响的那一部分河床中存在的颗粒物质。</skos:definition>
        <skos:prefLabel xml:lang="en">bed material</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#bed_material_armoring -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#bed_material_armoring">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Sediment"/>
        <skos:definition xml:lang="zh-cn">河流冲刷过程中因床沙组成不均匀，细颗粒先被冲走，粗颗粒 遗留下来，致使河床质逐渐变粗，甚至形成抗冲覆盖层的过程。</skos:definition>
        <skos:prefLabel xml:lang="en">bed material armoring</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#bed_material_load -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#bed_material_load">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Sediment"/>
        <skos:definition xml:lang="zh-cn">床沙被带起运动，在运动过程中随时与床沙交换的那一部分 悬移质和推移质的泥沙。</skos:definition>
        <skos:prefLabel xml:lang="en">bed material load</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#bed_material_measurement -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#bed_material_measurement">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">测定床沙颗粒级配的作业。</skos:definition>
        <skos:prefLabel xml:lang="en">bed material measurement</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#bed_material_sampler -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#bed_material_sampler">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysisDevices"/>
        <skos:definition xml:lang="zh-cn">采取床沙样品的设备。沙质河床采样器有拖斗式、横管式、钳 式、钻管式、转轴式等，卵石河床采样器有挖斗式、犁式、沉筒式等。</skos:definition>
        <skos:prefLabel xml:lang="en">bed material sampler</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#beheaded_stream -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#beheaded_stream">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">由于另一河道的溯源发育作用，使一河流源头或上游的水流 被引走的河流。</skos:definition>
        <skos:prefLabel xml:lang="en">beheaded stream</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#benchmark -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#benchmark">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">用水准测量方法测定的高程达到一定精度的控制点。该点相对于某一采用基面的高程一般是已知的，并埋设有标石。</skos:definition>
        <skos:prefLabel xml:lang="en">benchmark</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#benchmark_hydrometric_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#benchmark_hydrometric_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">为监测长周期气候演变引起的水文效应和分析人类活动对水 文情势的影响而设置的基本水文站。</skos:definition>
        <skos:prefLabel xml:lang="en">benchmark hydrometric station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#benchmark_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#benchmark_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">水文计算所参照移用水文数据的测站，或作为分析论证的对 照测站。</skos:definition>
        <skos:prefLabel xml:lang="en">benchmark station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#beneficial_reservoir_capacity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#beneficial_reservoir_capacity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">正常蓄水位至死水位之间的水库容积。</skos:definition>
        <skos:prefLabel xml:lang="en">beneficial reservoir capacity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#bent_reach -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#bent_reach">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">河道水流的平面形态非顺直，曲率较大的河段。</skos:definition>
        <skos:prefLabel xml:lang="en">bent reach</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#bias -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#bias">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
        <skos:definition xml:lang="zh-cn">测量仪器示值的系统误差。</skos:definition>
        <skos:prefLabel xml:lang="en">bias</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#biochemical_oxygen_demand -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#biochemical_oxygen_demand">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">水中有机物质在好氧微生物作用下，在好氧分解过程中所消耗的氧量。</skos:definition>
        <skos:prefLabel xml:lang="en">biochemical oxygen demand</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#biological_assessment_of_water_quality -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#biological_assessment_of_water_quality">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#QualityAssessment"/>
        <skos:definition xml:lang="zh-cn">利用指示水生物对水体污染物会产生某些反应或信息的原 理，对水体质量及其变化趋势进行的分析评定。</skos:definition>
        <skos:prefLabel xml:lang="en">biological assessment of water quality</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#biological_degradation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#biological_degradation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">用需氧微生物以生化方法的污染物降解。</skos:definition>
        <skos:prefLabel xml:lang="en">biological degradation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#biological_monitoring -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#biological_monitoring">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#AquaticEcologyMonitoring"/>
        <skos:definition xml:lang="zh-cn">利用生物个体、种群或群落对环境污染和生态环境破坏的反 应来定期调查、分析环境质量及其变化。</skos:definition>
        <skos:prefLabel xml:lang="en">biological monitoring</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#biological_productivity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#biological_productivity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#AquaticEcologyMonitoring"/>
        <skos:definition xml:lang="zh-cn">单位时间、单位面积上有机物质的生长量。一般分为初级生 产力和次级生产力。</skos:definition>
        <skos:prefLabel xml:lang="en">biological productivity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#biological_productivity_of_water_body -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#biological_productivity_of_water_body">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#AquaticEcologyMonitoring"/>
        <skos:definition xml:lang="zh-cn">水体生产有机物的能力。一般以水体在一定时间内，单位水 面或体积所生产的有机体的总数量表示。</skos:definition>
        <skos:prefLabel xml:lang="en">biological productivity of water body</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#biological_purification_of_water_body -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#biological_purification_of_water_body">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydroecology"/>
        <skos:definition xml:lang="zh-cn"> 水生物类群通过生命活动，使水体中污染物无害也或污染浓 度下降的过程。</skos:definition>
        <skos:prefLabel xml:lang="en">biological purification of water body</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#biological_safety -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#biological_safety">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydroecologicalAssessment"/>
        <skos:definition xml:lang="zh-cn">在生物技术研究，应用以及生物技术产品研究、开发、商品化 生产过程中发生的可能危及生物多样性、环境和人类健康的安全 性问题。</skos:definition>
        <skos:prefLabel xml:lang="en">biological safety</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#biological_transformation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#biological_transformation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">污染物在水体中的生化反应、生物体吸收、代谢作用及生物降解等过程。</skos:definition>
        <skos:prefLabel xml:lang="en">biological transformation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#biological_transport -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#biological_transport">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">污染物通过水生物新陈代谢、生长、死亡等生理过程而伴随的 生物富集。</skos:definition>
        <skos:prefLabel xml:lang="en">biological transport</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#biotic_community -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#biotic_community">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydroecology"/>
        <skos:definition xml:lang="zh-cn">生活在一个地段或水域内，相互间具有直接或间接关系的各 种动植物的总体。</skos:definition>
        <skos:prefLabel xml:lang="en">biotic community</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#biotic_index -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#biotic_index">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydroecology"/>
        <skos:definition xml:lang="zh-cn">根据某类或几类生物数量多少及其比例表达环境质量等级的 简单数学形式。</skos:definition>
        <skos:prefLabel xml:lang="en">biotic index</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#biowater -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#biowater">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#CommonTerms"/>
        <skos:definition xml:lang="zh-cn">在各种生命体系中存在的不同状态的水。</skos:definition>
        <skos:prefLabel xml:lang="en">biowater</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#blackbox_model -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#blackbox_model">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicModel"/>
        <skos:definition xml:lang="zh-cn">模型的关系式无物理意义的水文模型。</skos:definition>
        <skos:prefLabel xml:lang="en">blackbox model</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#bladder_sampling_pump -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#bladder_sampling_pump">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#GroundwaterObservationDevices"/>
        <skos:definition xml:lang="zh-cn">用压缩空气作动力，依靠挤压气囊的方式抽取地下水水样的地下水抽水泵。</skos:definition>
        <skos:prefLabel xml:lang="en">bladder sampling pump</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#blanking_distance -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#blanking_distance">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">距声学换能器一定距离内的区域，以及靠近水底、岸边很近距 离的区域，在此区域内，声学换能器不能提供有效的水位、水深、流速测量数据。应用微波、光波的相应测量仪器也可能存在同样的“盲区”。</skos:definition>
        <skos:prefLabel xml:lang="en">blanking distance</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#blind_drainage_area -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#blind_drainage_area">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">流域内地表水汇集不与海洋或外流河相通的区域。</skos:definition>
        <skos:prefLabel xml:lang="en">blind drainage area</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#blocking_high -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#blocking_high">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">中纬西风带高压脊上发展形成的缓慢移动或准静止状态的闭 合高压，对天气系统的移动有阻塞作用。</skos:definition>
        <skos:prefLabel xml:lang="en">blocking high</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#border_ice -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#border_ice">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegime"/>
        <skos:definition xml:lang="zh-cn">沿河岸冻结的冰带。</skos:definition>
        <skos:prefLabel xml:lang="en">border ice</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#bottled_sampler -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#bottled_sampler">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysisDevices"/>
        <skos:definition xml:lang="zh-cn">具有进水管、排气管，其盛水容器像瓶状的积时式悬移质采 样器。</skos:definition>
        <skos:prefLabel xml:lang="en">bottled sampler</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#bound_water -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#bound_water">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">被岩土颗粒的分子引力和静电引力吸附在颗粒或隙壁表面的 水，包括吸着水和薄膜水。</skos:definition>
        <skos:prefLabel xml:lang="en">bound water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#boundary_condition -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#boundary_condition">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">求解水动力学微分方程，在区域边界上所需满足的一组条件 方程。</skos:definition>
        <skos:prefLabel xml:lang="en">boundary condition</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#boundary_river -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#boundary_river">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">以河流主流线划分国界及行政区划的河流。</skos:definition>
        <skos:prefLabel xml:lang="en">boundary river</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#break-up -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#break-up">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegime"/>
        <skos:definition xml:lang="zh-cn">随着气温的逐渐上升，较长河段没有固定冰盖，敞露水面上下 游贯通，其面积超过河段总面积20%的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">break-up</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#break-up_date -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#break-up_date">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegimeObservation"/>
        <skos:definition xml:lang="zh-cn">在上半年首次出现解冻现象的开始日期。</skos:definition>
