    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#water_edge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#water_edge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
