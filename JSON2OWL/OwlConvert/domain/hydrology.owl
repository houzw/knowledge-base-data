        <skos:prefLabel xml:lang="en">mark test</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#masscurve -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#masscurve">
