    <!-- http://www.egc.org/ont/domain/hydrology#primary_productivity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#primary_productivity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#AquaticEcologyMonitoring"/>
