    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#time-variant_hydrologic_model">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicModel"/>
        <skos:definition xml:lang="zh-cn">参数随时间变化的水文数学模型。</skos:definition>
        <skos:prefLabel xml:lang="en">time-variant hydrologic model</skos:prefLabel>
    </owl:NamedIndividual>
    


