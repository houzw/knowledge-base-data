        <skos:prefLabel xml:lang="en">probability distribution of random error</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#probability_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#probability_method">
