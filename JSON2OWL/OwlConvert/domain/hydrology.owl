    <!-- http://www.egc.org/ont/domain/hydrology#probable_maximum_flood -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#probable_maximum_flood">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ProbableMaximumPrecipitationAndProbableMaximumFlood"/>
        <skos:definition xml:lang="zh-cn">根据可能最大降水和相应的产汇流条件推算的洪水。</skos:definition>
        <skos:prefLabel xml:lang="en">probable maximum flood</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#probable_maximum_precipitation_CPMP -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#probable_maximum_precipitation_CPMP">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ProbableMaximumPrecipitationAndProbableMaximumFlood"/>
        <skos:definition xml:lang="zh-cn">现代气候条件下，给定流域面积在一定历时内理论上可能发 生的最大雨深。</skos:definition>
        <skos:prefLabel xml:lang="en">probable maximum precipitation CPMP</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#processing_by_computer -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#processing_by_computer">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">用电子计算机整编水文资料的全部技术过程。</skos:definition>
        <skos:prefLabel xml:lang="en">processing by computer</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#productive_water_use -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#productive_water_use">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DevelopmentAndUtilization"/>
        <skos:definition xml:lang="zh-cn">第一产业、第二产业和第三产业用水的总称。</skos:definition>
        <skos:prefLabel xml:lang="en">productive water use</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#proficiency_testing -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#proficiency_testing">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentMonitoring"/>
        <skos:definition xml:lang="zh-cn">利用实验室间比对确定实验室的检测能力。</skos:definition>
        <skos:prefLabel xml:lang="en">proficiency testing</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#projected_diameter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#projected_diameter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysis"/>
        <skos:definition xml:lang="zh-cn">和给定泥沙颗粒最稳定的平面投影图像相一致的圆的直径。</skos:definition>
        <skos:prefLabel xml:lang="en">projected diameter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#propeller-type_current_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#propeller-type_current_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">以旋桨作为转子的流速仪，其旋桨绕着水平轴转动，转速与周 围水流的流速成单值对应关系。</skos:definition>
        <skos:prefLabel xml:lang="en">propeller-type current meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#protection_of_monitoring_setting -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#protection_of_monitoring_setting">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#MonitoringSettings"/>
        <skos:definition xml:lang="zh-cn">对确界范围内水文监测环境立体空间的法定保护行为。</skos:definition>
        <skos:prefLabel xml:lang="en">protection of monitoring setting</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#protection_planning_of_water_source_area -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#protection_planning_of_water_source_area">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentProtection"/>
        <skos:definition xml:lang="zh-cn">为保障供水安全，保护人群健康，对供水水源地的污染控制所 作的总体安排。</skos:definition>
        <skos:prefLabel xml:lang="en">protection planning of water source area</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#public_channel_communication -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#public_channel_communication">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataAcquisitionSystem"/>
        <skos:definition xml:lang="zh-cn">利用公用有线、无线通信网作为数据传输、通信信道的通信方 式。常用的有SMS、GPRS、CDMA、PSTN通信方式。</skos:definition>
        <skos:prefLabel xml:lang="en">public channel communication</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#pull_slanting_cable -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#pull_slanting_cable">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">用悬索悬吊铅鱼测深或测深锤测深时，为减小或消除因水流 作用使悬索对偏斜而设置的牵引钢索。</skos:definition>
        <skos:prefLabel xml:lang="en">pull slanting cable</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#pumping_drainage -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#pumping_drainage">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DrainageOfSurfaceWaterAndDrainageOfSubsurfaceWater"/>
        <skos:definition xml:lang="zh-cn">利用抽水设备排出渍水或涝水的排水方式。</skos:definition>
        <skos:prefLabel xml:lang="en">pumping drainage</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#pumping_sampler -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#pumping_sampler">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysisDevices"/>
        <skos:definition xml:lang="zh-cn">具有相应进出水口，并由一个泵系统构成的悬移质采样器。</skos:definition>
        <skos:prefLabel xml:lang="en">pumping sampler</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#pumping_test -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#pumping_test">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#GroundwaterAndSoilWaterExperiment"/>
        <skos:definition xml:lang="zh-cn">通过井孔抽水，确定井孔出水能力，获取含水层的水文地质参 数，判明某些水文地质条件的野外试验工作。</skos:definition>
        <skos:prefLabel xml:lang="en">pumping test</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#pycnometer -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#pycnometer">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysisDevices"/>
        <skos:definition xml:lang="zh-cn">具有固定容积，用称重法测量液体比重的专用瓶。</skos:definition>
        <skos:prefLabel xml:lang="en">pycnometer</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#qualified_ratio_of_scheme -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#qualified_ratio_of_scheme">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">用过去实测水文资料对预报方案进行检验，小于或等于允许 误差的点据数占总点据数的百分数。</skos:definition>
        <skos:prefLabel xml:lang="en">qualified ratio of scheme</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#quality_assessment_of_water_environment -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#quality_assessment_of_water_environment">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#QualityAssessment"/>
        <skos:definition xml:lang="zh-cn">按照一定目的，根据水环境质量标准，对一个区域的水环境质 量进行总体定性和定量的评定。</skos:definition>
        <skos:prefLabel xml:lang="en">quality assessment of water environment</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#quality_assurance -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#quality_assurance">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentMonitoring"/>
        <skos:definition xml:lang="zh-cn">为通过足够的信任表明能够满足质量要求，而实施并根据需 要进行证实的全部有计划和有系统的活动。</skos:definition>
        <skos:prefLabel xml:lang="en">quality assurance</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#quality_control -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#quality_control">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentMonitoring"/>
        <skos:definition xml:lang="zh-cn">为达到质量要求所采取的作业技术活动。</skos:definition>
        <skos:prefLabel xml:lang="en">quality control</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#quality_management -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#quality_management">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentMonitoring"/>
        <skos:definition xml:lang="zh-cn">通过实施质量策划、质量控制、质量保证和质量改进等全部管 理职能的活动。</skos:definition>
        <skos:prefLabel xml:lang="en">quality management</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#quality_of_groundwater -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#quality_of_groundwater">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">地下水的物理性质、化学成分、细菌和其他有害物质的含量的总称。</skos:definition>
        <skos:prefLabel xml:lang="en">quality of groundwater</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#quality_of_water_environment -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#quality_of_water_environment">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
        <skos:definition xml:lang="zh-cn">水环境对人群的生存和繁衍以及社会经济发展的适宜程度， 通常指水环境遭受污染的程度。</skos:definition>
        <skos:prefLabel xml:lang="en">quality of water environment</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#quality_standard_of_water_environment -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#quality_standard_of_water_environment">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentProtection"/>
        <skos:definition xml:lang="zh-cn">为保护人群健康和社会物质财富、维持生态平衡，由政府制定 的限定水体中有害物质或因素的标准，包括生活饮用水卫生标准、 工业用水水质标准、农田灌溉水质标准、渔业水质标准、地面水环 境质量标准等。</skos:definition>
        <skos:prefLabel xml:lang="en">quality standard of water environment</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#quantity_assessment_of_water_resources -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#quantity_assessment_of_water_resources">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SurveyAndAssessment"/>
        <skos:definition xml:lang="zh-cn">对水资源的数量及其时空分布特征和影响因素的分析评定。</skos:definition>
        <skos:prefLabel xml:lang="en">quantity assessment of water resources</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#quantity_of_inflowwater -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#quantity_of_inflowwater">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">注入雨量计承雨口的水量。</skos:definition>
        <skos:prefLabel xml:lang="en">quantity of inflowwater</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#radar_stage_recorder -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#radar_stage_recorder">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StageWaterTemporatureAndWaveObservationDevices"/>
        <skos:definition xml:lang="zh-cn">将微波发射、接收器安装在水上某一已知高程位置，通过测量 微波脉冲从发射、经水面反射到被接收的时间，计算记录水位的 仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">radar stage recorder</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#radiation_rod -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#radiation_rod">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InfrastructureAndMeasurement"/>
        <skos:definition xml:lang="zh-cn">为测算垂线在断面线上的起点距而在岸上设立的辅助性 标杆。</skos:definition>
        <skos:prefLabel xml:lang="en">radiation rod</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#radioactive_tracer -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#radioactive_tracer">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">具有放射性的示踪剂，一般是ϒ射线或β粒子的发射物。</skos:definition>
        <skos:prefLabel xml:lang="en">radioactive tracer</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#radioisotope_sediment_concentration_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#radioisotope_sediment_concentration_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SedimentMeasurementAndParticleSizeAnalysisDevices"/>
        <skos:definition xml:lang="zh-cn">应用放射性同位素通过不同含沙浓度浑液强度有不同程度衰 减的原理来测定含沙量的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">radioisotope sediment concentration meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rain -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rain">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">从大气中降落到地面的液态水滴。</skos:definition>
        <skos:prefLabel xml:lang="en">rain</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rain_and_snow_recorder -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rain_and_snow_recorder">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservationDevices"/>
        <skos:definition xml:lang="zh-cn">能加热融雪或加防冻剂将雪融化为液态进行降水量自动测量 的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">rain and snow recorder</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rain_and_snow_recorder_with_no-freeze_fluid -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rain_and_snow_recorder_with_no-freeze_fluid">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservationDevices"/>
        <skos:definition xml:lang="zh-cn">降雨、降雪落入仪器承雨口中不冻液内，排出不冻液体和雪水 的混合液体，再用测量降雨的方法，测量混合液体量，从而测得雨 雪量的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">rain and snow recorder with no-freeze fluid</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rain_and_snowmelt_flood -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rain_and_snowmelt_flood">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">由融雪和降雨所形成的洪水。</skos:definition>
        <skos:prefLabel xml:lang="en">rain and snowmelt flood</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rain_gauge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rain_gauge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservationDevices"/>
        <skos:definition xml:lang="zh-cn">以收集储存降水的方式，人工观测时段降水量的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">rain gauge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rain_gauge_receiver -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rain_gauge_receiver">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservationDevices"/>
        <skos:definition xml:lang="zh-cn">雨量器中直接承接雨水的具有标准口径，器口壁内直外斜呈 刀刃状的漏斗形圆筒。</skos:definition>
        <skos:prefLabel xml:lang="en">rain gauge receiver</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rain_gauging_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rain_gauging_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn"> 以观测降水量为主的水文测站。</skos:definition>
        <skos:prefLabel xml:lang="en">rain gauging station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rain_gauging_station_network -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rain_gauging_station_network">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StationNetwork"/>
        <skos:definition xml:lang="zh-cn">由降水量（雨量）观测站、观测降水量（雨量）的国家基本水文 测站和专用水文测站所组成的资料收集系统。</skos:definition>
        <skos:prefLabel xml:lang="en">rain gauging station network</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rain_observation_yard -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rain_observation_yard">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#MonitoringSettings"/>
        <skos:definition xml:lang="zh-cn">具有一定环境条件和场地规格要求，进行降水量观测的场地。</skos:definition>
        <skos:prefLabel xml:lang="en">rain observation yard</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rainfall -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rainfall">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservation"/>
        <skos:definition xml:lang="zh-cn">在一定时段内，从大气降落到地面的液态降水量。</skos:definition>
        <skos:definition xml:lang="zh-cn">大气中的水汽凝结后以液态水降落到地面的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">rainfall</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rainfall-runoff_correlation_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rainfall-runoff_correlation_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FloodForecasting"/>
        <skos:definition xml:lang="zh-cn">利用历史资料，建立降雨、径流与前期水文气象因子之间的经 验关系，由现时水文气象信息即可从所建立的经验关系获得预报 值的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">rainfall-runoff correlation method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rainfall-runoff_forecasting_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rainfall-runoff_forecasting_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FloodForecasting"/>
        <skos:definition xml:lang="zh-cn">根据降雨和降雨形成径流的有关影响因素，对流域内一次降雨所形成的径流量及其过程进行预报的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">rainfall-runoff forecasting method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rainfall-runoff_modeling_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rainfall-runoff_modeling_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignAnnualRunoff"/>
        <skos:definition xml:lang="zh-cn">建立人类活动影响前的降雨径流模型，用人类活动影响期各 年的降雨资料，推求不受人类活动影响的年径流系列的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">rainfall-runoff modeling method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rainfall_area -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rainfall_area">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">降雨笼罩的地表面积。</skos:definition>
        <skos:prefLabel xml:lang="en">rainfall area</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rainfall_distribution -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rainfall_distribution">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">雨深在时间和空间上的变化情况。</skos:definition>
        <skos:prefLabel xml:lang="en">rainfall distribution</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rainfall_intensity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rainfall_intensity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservation"/>
        <skos:definition xml:lang="zh-cn">单位时间内的降雨量。</skos:definition>
        <skos:prefLabel xml:lang="en">rainfall intensity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rainfall_intensity-area_curve -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rainfall_intensity-area_curve">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignStorm"/>
        <skos:definition xml:lang="zh-cn">暴雨最大平均降雨强度与相应暴雨笼罩面积之间的关系曲线。</skos:definition>
        <skos:prefLabel xml:lang="en">rainfall intensity-area curve</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rainfall_intensity-duration_curve -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rainfall_intensity-duration_curve">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignStorm"/>
        <skos:definition xml:lang="zh-cn">最大平均降雨强度与相应降雨历时的关系曲线。</skos:definition>
        <skos:prefLabel xml:lang="en">rainfall intensity-duration curve</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rainfall_recorder -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rainfall_recorder">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservationDevices"/>
        <skos:definition xml:lang="zh-cn">能自动连续测记液态降水量及其过程的自记仪器，常用的有 虹吸式和翻斗式两种。</skos:definition>
        <skos:prefLabel xml:lang="en">rainfall recorder</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rainfall_recorder_near_land -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rainfall_recorder_near_land">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservationDevices"/>
        <skos:definition xml:lang="zh-cn">承雨口高度很接近地面的雨量计。</skos:definition>
        <skos:prefLabel xml:lang="en">rainfall recorder near land</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rainfall_runoff -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rainfall_runoff">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">由降雨所形成的河川径流。</skos:definition>
        <skos:prefLabel xml:lang="en">rainfall runoff</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rainwater_utilization -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rainwater_utilization">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DevelopmentAndUtilization"/>
        <skos:definition xml:lang="zh-cn">采用人工措施直接对天然降水进行收集、存储并加以利用。</skos:definition>
        <skos:prefLabel xml:lang="en">rainwater utilization</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rainy_season -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rainy_season">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <skos:definition xml:lang="zh-cn">降雨比较集中的季节。</skos:definition>
        <skos:prefLabel xml:lang="en">rainy season</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#random_error -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#random_error">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
        <skos:definition xml:lang="zh-cn">在同一条件下，对同一量值进行多次测量时，其数值和符号以 不可预见的方式而变化的那部分误差。</skos:definition>
        <skos:prefLabel xml:lang="en">random error</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#random_event -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#random_event">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">在随机试验中，可能出现也可能不出现，而在大量重复试验中 具有某种规律性的事件。</skos:definition>
        <skos:prefLabel xml:lang="en">random event</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#random_series -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#random_series">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">随机变量随时间取离散值所组成的系列。</skos:definition>
        <skos:prefLabel xml:lang="en">random series</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#random_uncertainty -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#random_uncertainty">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
        <skos:definition xml:lang="zh-cn">某一变量的测量中，误差纯属随机性质的不确定度。</skos:definition>
        <skos:prefLabel xml:lang="en">random uncertainty</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#random_variable -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#random_variable">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">受随机因素影响，遵循统计规律的变量。</skos:definition>
        <skos:prefLabel xml:lang="en">random variable</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#range_of_recharge_zone -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#range_of_recharge_zone">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">在有径流补给的含水层中抽水时，流入抽水井的地下水流 宽度。</skos:definition>
        <skos:prefLabel xml:lang="en">range of recharge zone</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rank_order_of_flood -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rank_order_of_flood">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignFlood"/>
        <skos:definition xml:lang="zh-cn">按洪水大小高低排列的洪水事件的顺位。</skos:definition>
        <skos:prefLabel xml:lang="en">rank order of flood</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rate_of_reservoir_storage_change -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rate_of_reservoir_storage_change">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfReservior"/>
        <skos:definition xml:lang="zh-cn">某一时段内，水库蓄水变量与时间的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">rate of reservoir storage change</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rate_of_rotor -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rate_of_rotor">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">测速时，转子总转数与转动历时之比。</skos:definition>
        <skos:prefLabel xml:lang="en">rate of rotor</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rateable_sampler -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rateable_sampler">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentMonitoringDevices"/>
        <skos:definition xml:lang="zh-cn">一种专用的自动水质采样器，采集的水样量随时间和流量成 一定比例，使其任一时段所采集的混合水样的污染物浓度，反映该 时段的平均浓度。</skos:definition>
        <skos:prefLabel xml:lang="en">rateable sampler</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rating_table -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rating_table">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">从水位流量关系曲线上每隔一定水位高度读出一个流量，当 多条曲线时按推流时段分别读数而列成的水位流量关系表。</skos:definition>
        <skos:prefLabel xml:lang="en">rating table</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ratio_of_system_fluency -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ratio_of_system_fluency">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataAcquisitionSystem"/>
        <skos:definition xml:lang="zh-cn">在一定时间内，中心站收到正确数据的遥测站数与向中心站 发送数据的遥测站数的百分比。</skos:definition>
        <skos:prefLabel xml:lang="en">ratio of system fluency</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#ratio_of_water_deficiency -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#ratio_of_water_deficiency">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SurveyAndAssessment"/>
        <skos:definition xml:lang="zh-cn">缺水量与需水量的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">ratio of water deficiency</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rational_examination -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rational_examination">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">为保证水文资料整编的质量，根据水文要素的时空变化规律和各要素间的关系，对整编成果的规律性所作的检验工作。</skos:definition>
        <skos:prefLabel xml:lang="en">rational examination</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rational_formula -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rational_formula">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignFlood"/>
        <skos:definition xml:lang="zh-cn">依据径流成因原理，通常在中小流域由设计暴雨推求工程地 点设计洪峰流量的公式。</skos:definition>
        <skos:prefLabel xml:lang="en">rational formula</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rational_water_resources_allocation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rational_water_resources_allocation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterResourcesPlanning"/>
        <skos:definition xml:lang="zh-cn">在维护社会公平和生态系统可持续性的前提下，以提高水资源利用效率和效益为目标，在区域、行业、和代际间各用水户之间 合理分配水资源。</skos:definition>
        <skos:prefLabel xml:lang="en">rational water resources allocation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#raw_data -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#raw_data">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessing"/>
        <skos:definition xml:lang="zh-cn">在现场对各水文要素进行勘测、调查、测量及计算所获得的第 一性基本成果。</skos:definition>
        <skos:prefLabel xml:lang="en">raw data</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reach -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reach">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">限定两横断面间的河流。</skos:definition>
        <skos:prefLabel xml:lang="en">reach</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#real-time_hydrologic_forecasting -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#real-time_hydrologic_forecasting">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">利用遥测系统收集流域内的实时水文气象数据输入计算机， 直接与水文预报程序和预报误差校正程序连接，即时作出水文要 素的预报。</skos:definition>
        <skos:prefLabel xml:lang="en">real-time hydrologic forecasting</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#real-time_hydrologic_information_database -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#real-time_hydrologic_information_database">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicDatabase"/>
        <skos:definition xml:lang="zh-cn">用计算机存贮、管理和检索水文测站实时测定的水文数据及 相关数据组成的系统。</skos:definition>
        <skos:prefLabel xml:lang="en">real-time hydrologic information database</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#receiving_probability -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#receiving_probability">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataAcquisitionSystem"/>
        <skos:definition xml:lang="zh-cn">有效接收字数占总传输字数的比值，或传输数据被正确接收 的概率值。</skos:definition>
        <skos:prefLabel xml:lang="en">receiving probability</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#recession_curve -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#recession_curve">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">地表径流或地下径流过程线的退水段。</skos:definition>
        <skos:prefLabel xml:lang="en">recession curve</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#recession_curve_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#recession_curve_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Low-flowForecasting"/>
        <skos:definition xml:lang="zh-cn">应用退水曲线公式预报退水流量的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">recession curve method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#recharge_coefficient_of_irrigation_water -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#recharge_coefficient_of_irrigation_water">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">灌溉水入渗补给地下水的量与灌溉水量的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">recharge coefficient of irrigation water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#recharge_coefficient_of_lake -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#recharge_coefficient_of_lake">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">湖泊流域面积(集水面积）与湖泊面积的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">recharge coefficient of lake</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#recharge_region_of_phreatic_water -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#recharge_region_of_phreatic_water">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">大气降水或地表水入渗补给潜水的地区。</skos:definition>
        <skos:prefLabel xml:lang="en">recharge region of phreatic water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reclaimed_water_for_municipal_wastewater -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reclaimed_water_for_municipal_wastewater">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentProtection"/>
        <skos:definition xml:lang="zh-cn">城镇污水经再生工艺处理后具有一定使用功能的水。</skos:definition>
        <skos:prefLabel xml:lang="en">reclaimed water for municipal wastewater</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reclamation_of_sewage -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reclamation_of_sewage">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentProtection"/>
        <skos:definition xml:lang="zh-cn">为使污水重新具有使用价值和经济价值，把污水用作低质水 源、污水经处理后再利用或从污水中回收有用物质的过程。</skos:definition>
        <skos:prefLabel xml:lang="en">reclamation of sewage</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#recorder -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#recorder">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InstrumentAndMeasuring"/>
        <skos:definition xml:lang="zh-cn">能接收传感器或其他测量单元的输出信号（或物理变化量）， 将被测水文参数或其变化过程记录在记录媒体上的装置。</skos:definition>
        <skos:prefLabel xml:lang="en">recorder</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#recovery_ratio -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#recovery_ratio">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentMonitoring"/>
        <skos:definition xml:lang="zh-cn">在水样中加人已知数量的标准物质，加人后与加入前该物质 的测得值之差占所加人量的百分数。它是评记分析方法的精度或 检验分析质量的惯用指标。</skos:definition>
        <skos:prefLabel xml:lang="en">recovery ratio</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#rectangular_notch_weir -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#rectangular_notch_weir">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityAndFlowDirectionAndDischargeMeasurement"/>
        <skos:definition xml:lang="zh-cn">堰壁缺口形状为矩形的薄壁堰。</skos:definition>
        <skos:prefLabel xml:lang="en">rectangular notch weir</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#recurrence_interval -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#recurrence_interval">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">等于及大于（等于及小于）一定量级的水文要素值出现一次的平均间隔年数，由该量级频率的倒数计。</skos:definition>
        <skos:prefLabel xml:lang="en">recurrence interval</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#red_tide -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#red_tide">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydroecology"/>
        <skos:definition xml:lang="zh-cn">在特定的环境条件下，海水中某些浮游植物、原生动物或细菌 爆发性增殖或高度聚集而引起水体变色的一种有害生态现象。</skos:definition>
        <skos:prefLabel xml:lang="en">red tide</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reference_basin -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reference_basin">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">能长期保持天然状态的代表性流域，可用来研究气候变化和 人类活动对代表区域水文特征和水资源长期变化趋势的影响。</skos:definition>
        <skos:prefLabel xml:lang="en">reference basin</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reference_current_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reference_current_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#FlowVelocityFlowDirectionAndDischargeMeasurementDevices"/>
        <skos:definition xml:lang="zh-cn">作为其他常用流速仪比测基础的一种被检定过的备用流 速仪。</skos:definition>
        <skos:prefLabel xml:lang="en">reference current meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reference_mark_on_observation_well -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reference_mark_on_observation_well">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#GroundwaterObservation"/>
        <skos:definition xml:lang="zh-cn"> 在地下水观测井（孔）上设置的或直接标记在井（孔）口上已知 高程的坚固标志点。</skos:definition>
        <skos:prefLabel xml:lang="en">reference mark on observation well</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reference_material -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reference_material">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentMonitoring"/>
        <skos:definition xml:lang="zh-cn">具有一种或多种足够均匀和很好地确定了的特性，用以校准 测量装置、评价测量方法或给材料赋值的一种材料或物质。</skos:definition>
        <skos:prefLabel xml:lang="en">reference material</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#regenerated_glacier -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#regenerated_glacier">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Glacier"/>
        <skos:definition xml:lang="zh-cn">山坡陡岩上部的冰雪悬空崩落到坡脚堆积发育成的冰川。</skos:definition>
        <skos:prefLabel xml:lang="en">regenerated glacier</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#regenerative_border_ice -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#regenerative_border_ice">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegime"/>
        <skos:definition xml:lang="zh-cn">春季开河后，又遇寒潮再次生成的岸冰。</skos:definition>
        <skos:prefLabel xml:lang="en">regenerative border ice</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#regional_flood -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#regional_flood">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">洪水来源区覆盖流域部分区域，形成部分干支流河段或支流 较大以上量级的洪水。</skos:definition>
        <skos:prefLabel xml:lang="en">regional flood</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#regional_hydrologic_forecasting -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#regional_hydrologic_forecasting">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn"> 根据水文条件相似地区内的各河流涨水和退水规律，发布洪 水预报或枯季径流预报。</skos:definition>
        <skos:prefLabel xml:lang="en">regional hydrologic forecasting</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#regional_hydrology -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#regional_hydrology">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrology"/>
        <skos:definition xml:lang="zh-cn">研究某些特殊自然景观地区（如河口、喀斯特地区及干旱区 等）或特定行政区（如国家、省和市等）的水文特征和变化规律的 学科。</skos:definition>
        <skos:prefLabel xml:lang="en">regional hydrology</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#regional_representative_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#regional_representative_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">按规定控制面积，为探索中等河流地区水文要素变化规律而 在有代表性的中等河流上布设的水文测站。</skos:definition>
        <skos:prefLabel xml:lang="en">regional representative station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#regional_synthesis -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#regional_synthesis">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">分析地区水文规律，建立地区参数的经验公式或绘制特征值等值线图的工作。</skos:definition>
        <skos:prefLabel xml:lang="en">regional synthesis</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#regression_analysis -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#regression_analysis">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">分析两个或两个以上水文变量之间相关关系的一种数理统计方法。</skos:definition>
        <skos:prefLabel xml:lang="en">regression analysis</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#regular_indices_of_water_pollution -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#regular_indices_of_water_pollution">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentMonitoring"/>
        <skos:definition xml:lang="zh-cn">反映水污染状况的重要指标，包括臭味、水温、浑浊度、pH 值、电导率、溶解性总固体、悬浮性固体、总氮、总有机碳、溶解氧、 氨氮、生化需氧量、化学需氧量、细菌总数、大肠菌群等，是对水体 进行监测、评价、利用及污染防治的主要依据。</skos:definition>
        <skos:prefLabel xml:lang="en">regular indices of water pollution</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#regulating_period -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#regulating_period">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterConservancyComputation"/>
        <skos:definition xml:lang="zh-cn">水库由库空至蓄满，后放水至库空，循环一次所经历的平均 时间。</skos:definition>
        <skos:prefLabel xml:lang="en">regulating period</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#regulation_of_multipurpose_reservoir -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#regulation_of_multipurpose_reservoir">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterConservancyComputation"/>
        <skos:definition xml:lang="zh-cn">担负两种或两种以上水利开发目标的水库径流调节。</skos:definition>
        <skos:prefLabel xml:lang="en">regulation of multipurpose reservoir</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#regulation_of_reservoir_for_water_supply -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#regulation_of_reservoir_for_water_supply">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterConservancyComputation"/>
        <skos:definition xml:lang="zh-cn">以工业及城市生活、农业灌溉等供水为目标的水库径流 调节。</skos:definition>
        <skos:prefLabel xml:lang="en">regulation of reservoir for water supply</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#relational_database -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#relational_database">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicDatabase"/>
        <skos:definition xml:lang="zh-cn">用关系数据模型来描述的数据库，其最大特点是采用二维表 保存数据。</skos:definition>
        <skos:prefLabel xml:lang="en">relational database</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#relative_depth -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#relative_depth">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DepthAndCrossSectionMeasurement"/>
        <skos:definition xml:lang="zh-cn">从水面算起的垂线上任一测点的深度与垂线水深的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">relative depth</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#relative_error -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#relative_error">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
        <skos:definition xml:lang="zh-cn">绝对误差与真值的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">relative error</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#relative_humidity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#relative_humidity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Hydrometeorology"/>
        <skos:definition xml:lang="zh-cn">空气中实际水汽压与同温下的饱和水汽压的比值，用百分数来表不。</skos:definition>
        <skos:prefLabel xml:lang="en">relative humidity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#relative_soil_moisture -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#relative_soil_moisture">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SoilMoisture"/>
        <skos:definition xml:lang="zh-cn">土壤含水量占田间持水量的百分数。</skos:definition>
        <skos:prefLabel xml:lang="en">relative soil moisture</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#relative_standard_deviation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#relative_standard_deviation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
        <skos:definition xml:lang="zh-cn">标准差与真值的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">relative standard deviation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#relative_standard_deviation_of_calibration_equation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#relative_standard_deviation_of_calibration_equation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">表示转子式流速仪检定公式所有检定点流速相对误差的均方根值。我国曾长期用于表示转子式流速仪检定误差。此表示方式 将被各流速级平均相对误差取代。</skos:definition>
        <skos:prefLabel xml:lang="en">relative standard deviation of calibration equation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#relay_meter -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#relay_meter">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataAcquisitionSystem"/>
        <skos:definition xml:lang="zh-cn">能通过无线或有线信道，自动完成遥测数据信号或指令的接 收及转发的仪器。</skos:definition>
        <skos:prefLabel xml:lang="en">relay meter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#relay_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#relay_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataAcquisitionSystem"/>
        <skos:definition xml:lang="zh-cn">为解决因路径损耗太大、信号微弱或地形影响，在遥测站与中 心站之间设立用于转发中心站指令和遥测站数据信号的接力站。 主要用于超短波通信。</skos:definition>
        <skos:prefLabel xml:lang="en">relay station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#release_channel_for_urban_extraordinary_flood -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#release_channel_for_urban_extraordinary_flood">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#UrbanHydrology"/>
        <skos:definition xml:lang="zh-cn">城市区发生超防洪标准时，为防止洪水无序漫流造成洪灾损 失，利用生态、休闲场地、道路划定的应急洪水通道。</skos:definition>
        <skos:prefLabel xml:lang="en">release channel for urban extraordinary flood</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reliability -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reliability">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterResources"/>
        <skos:definition xml:lang="zh-cn">满足兴利部门需水量或水位要求的程度。</skos:definition>
        <skos:prefLabel xml:lang="en">reliability</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reliability_stage -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reliability_stage">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DataProcessingAndCompiling"/>
        <skos:definition xml:lang="zh-cn">一年中有多少天水位等于高于某一水位的相应保证率（历时） 水位。</skos:definition>
        <skos:prefLabel xml:lang="en">reliability stage</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reliability_symbol -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reliability_symbol">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicDatabase"/>
        <skos:definition xml:lang="zh-cn">表征字组可靠程度的符号，用以反映对精度有直接影响的资料来源状况和数据处理情况。</skos:definition>
        <skos:prefLabel xml:lang="en">reliability symbol</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#relief_channel -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#relief_channel">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">为减轻河流的洪水负担而开辟的分洪水道。</skos:definition>
        <skos:prefLabel xml:lang="en">relief channel</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#remark_code -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#remark_code">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicDatabase"/>
        <skos:definition xml:lang="zh-cn">各种注解符号的组合体。注解码中的注解符号按照观测物符 号、流向符号、涨落符号、岸别符号、闸阀开度符号、可靠性符号</skos:definition>
        <skos:prefLabel xml:lang="en">remark code</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#remark_symbol -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#remark_symbol">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicDatabase"/>
        <skos:definition xml:lang="zh-cn">反映数据的实体形态、变化趋势、完整性、一致性、可靠性等定 性信息的符号。</skos:definition>
        <skos:prefLabel xml:lang="en">remark symbol</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#remote-sensing_monitoring_of_water_pollution -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#remote-sensing_monitoring_of_water_pollution">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentMonitoringDevices"/>
        <skos:definition xml:lang="zh-cn">应用遥感技术从高空或远距离对地表水体污染状况进行的监测。</skos:definition>
        <skos:prefLabel xml:lang="en">remote-sensing monitoring of water pollution</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#remote-sensing_technology_in_hydrology -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#remote-sensing_technology_in_hydrology">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Monitoring"/>
        <skos:definition xml:lang="zh-cn">利用安装在运载工具上的传感仪器，进行远距离收集水体、流 域的图像或波谱，经过处理和分析，获得水文数据的全部技术。</skos:definition>
        <skos:prefLabel xml:lang="en">remote-sensing technology in hydrology</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#renewal_period_of_water_bodies -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#renewal_period_of_water_bodies">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterResources"/>
        <skos:definition xml:lang="zh-cn">通过水循环或人工干预使水体更新一次所需要的时间。</skos:definition>
        <skos:prefLabel xml:lang="en">renewal period of water bodies</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reoxygenation_of_water_body -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reoxygenation_of_water_body">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#QualityAssessment"/>
        <skos:definition xml:lang="zh-cn">水体自然流动或利用堰闸、过水坝、跌水等水工建筑物泄水， 空气与水体发生掺混，气泡中的氧气在水体中扩散，提高水体中溶 解氧浓度的过程。</skos:definition>
        <skos:prefLabel xml:lang="en">reoxygenation of water body</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#repeated_utilization_ratio_of_water_resources -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#repeated_utilization_ratio_of_water_resources">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SurveyAndAssessment"/>
        <skos:definition xml:lang="zh-cn">在供水系统中可重复使用的水量占总用水量的百分数。</skos:definition>
        <skos:prefLabel xml:lang="en">repeated utilization ratio of water resources</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#representative_dew_point -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#representative_dew_point">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ProbableMaximumPrecipitationAndProbableMaximumFlood"/>
        <skos:definition xml:lang="zh-cn">在一定的降水面积及持续时间内，能反映一场暴雨期间输入 雨区的水汽量的地面露点。</skos:definition>
        <skos:prefLabel xml:lang="en">representative dew point</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#representative_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#representative_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">直接代表或通过换算可代表工程所在地点水文数据的测站。</skos:definition>
        <skos:prefLabel xml:lang="en">representative station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#representative_tide -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#representative_tide">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfTidalRiver"/>
        <skos:definition xml:lang="zh-cn">能够反映潮汐、潮流周期变化特征的具有代表性的潮期、潮流 期的总称，如大潮、中潮、小潮。</skos:definition>
        <skos:prefLabel xml:lang="en">representative tide</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#representative_watershed -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#representative_watershed">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">在流域内设置水文站对水文要素进行观测，获得能代表广大 区域的数据，以代替特定地区内所有流域测验工作的流域。</skos:definition>
        <skos:prefLabel xml:lang="en">representative watershed</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reregulation_of_reservoir -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reregulation_of_reservoir">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterConservancyComputation"/>
        <skos:definition xml:lang="zh-cn">为满足下游用水需要，下游水库对上游水库或上游水电站泄 流进行的径流调节。</skos:definition>
        <skos:prefLabel xml:lang="en">reregulation of reservoir</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">在河道、山谷、低洼地有水源或可从另一河道引人水源的地方 修建挡水坝或堤堰，形成的蓄水区域；或在有隔水条件的地下透水 层修建截水墙，形成的地下蓄水区域。</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir_bank_caving -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir_bank_caving">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironment"/>
        <skos:definition xml:lang="zh-cn">水库蓄水后，水库周边岸壁发生的坍塌现象。</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir bank caving</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir_capacity_for_flood_control -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir_capacity_for_flood_control">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">校核洪水位至防洪限制水位之间的水库容积。</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir capacity for flood control</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir_delta -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir_delta">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">由于人库泥沙的淤积，在库区形成的三角形淤积体。</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir delta</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir_deposition_morphology -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir_deposition_morphology">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfReservior"/>
        <skos:definition xml:lang="zh-cn">水库淤积分布在库内纵向和横向的形状。</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir deposition morphology</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir_ecosystem -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir_ecosystem">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#AquaticEcosystern"/>
        <skos:definition xml:lang="zh-cn">由水库水域内所有生物与非生物因素相互作用，通过物质循 环与能量流动构成的具有一定结构和功能的系统。</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir ecosystem</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir_gauging_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir_gauging_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">在水库出口、进口或库区进行水文要素观测而设置的水文测站。</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir gauging station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir_hydrologic_forecasting -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir_hydrologic_forecasting">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#InformationAndForecasting"/>
        <skos:definition xml:lang="zh-cn">根据前期和现时水文、气象资料，对水库未来的水文情势所作 的预报。</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir hydrologic forecasting</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir_inflow -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir_inflow">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfReservior"/>
        <skos:definition xml:lang="zh-cn">从入库河流及库区四周流入库区的时段水量。</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir inflow</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir_inflow_flood -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir_inflow_flood">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignFlood"/>
        <skos:definition xml:lang="zh-cn">通过水库回水末端上游干支流、回水末端至水库周边区间陆 面以及库面降雨所组成的洪水。</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir inflow flood</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir_inundation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir_inundation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironment"/>
        <skos:definition xml:lang="zh-cn">水库蓄水使水库周边地带的地下水壅高，引起土地盐碱化、沼 泽化等次生灾害的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir inundation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir_outflow -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir_outflow">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfReservior"/>
        <skos:definition xml:lang="zh-cn">经水库的引水建筑物、泄流建筑物和水轮机流出水库的时段 水量。</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir outflow</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir_sediment_observation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir_sediment_observation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">对水库库区的泥沙含量、运行规律及有关性质的测验和分析。</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir sediment observation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir_sedimentation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir_sedimentation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">人库水流挟带的泥沙由于流速减小而在库内淤积的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir sedimentation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir_sedimentation_observation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir_sedimentation_observation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">对水库库区的泥沙冲淤数量、淤积分布形态及变化的观测。</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir sedimentation observation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir_sedimentation_survey -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir_sedimentation_survey">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfReservior"/>
        <skos:definition xml:lang="zh-cn">为了解水库内泥沙淤积的部位、形态、数量及其发展规律而进 行的库区内的测量工作。</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir sedimentation survey</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir_sedimentation_volume -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir_sedimentation_volume">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfReservior"/>
        <skos:definition xml:lang="zh-cn">由于河流挟带泥沙进入水库以及库岸崩塌等原因而在库区淤 积的泥沙体积</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir sedimentation volume</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir_seepage -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir_seepage">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">库水沿透水岩土带向外低地渗水的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir seepage</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir_seepage_volume -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir_seepage_volume">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfReservior"/>
        <skos:definition xml:lang="zh-cn">水库中的水沿岩石裂缝或土壤空隙等途径渗漏的时段水量。</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir seepage volume</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir_stage_gauging_station -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir_stage_gauging_station">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricStation"/>
        <skos:definition xml:lang="zh-cn">设在水库区有代表性的地方进行水位观测的水文测站。</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir stage gauging station</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir_storage -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir_storage">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfReservior"/>
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">坝上游水位水平面以下的水库容积。</skos:definition>
        <skos:definition xml:lang="zh-cn">某一时刻水库蓄水的数量。</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir storage</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir_surplus_water -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir_surplus_water">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfReservior"/>
        <skos:definition xml:lang="zh-cn">为防洪和进行径流调节，从水库中排出且未经利用的水量。</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir surplus water</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir_water_loss -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir_water_loss">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">兴建水库后，因改变河流天然状态、库内外水力关系所引起的 水量蒸发损失、渗漏损失和结冰损失等。</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir water loss</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir_water_quality_planning -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir_water_quality_planning">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentProtection"/>
        <skos:definition xml:lang="zh-cn">为使水库水质维持高水平，防治水污染，以规划水平年要求的 水质标准，拟定合理措施，通过模拟、优化技术，寻求保护和改善水 库水质的总体措施安排。</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir water quality planning</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reservoir_water_supply -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reservoir_water_supply">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometryOfReservior"/>
        <skos:definition xml:lang="zh-cn">为满足工农业生产用水、生活用水以及河道内用水，而由水库 提供的水量。</skos:definition>
        <skos:prefLabel xml:lang="en">reservoir water supply</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#residual_border_ice -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#residual_border_ice">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegime"/>
        <skos:definition xml:lang="zh-cn">春季开河后，残留在原地的岸冰。</skos:definition>
        <skos:prefLabel xml:lang="en">residual border ice</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#residual_ice_accumulation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#residual_ice_accumulation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#IceRegime"/>
        <skos:definition xml:lang="zh-cn">春季开河，沿河流两岸、沙滩等边缘堆积的冰块。</skos:definition>
        <skos:prefLabel xml:lang="en">residual ice accumulation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#residual_masscurve -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#residual_masscurve">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#ComputationAndWaterConservancy"/>
        <skos:definition xml:lang="zh-cn">水文系列的数值与算术平均值的差值累积数的过程线。</skos:definition>
        <skos:prefLabel xml:lang="en">residual masscurve</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#resolution -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#resolution">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn">在仪器的测量范围内，能导致可观测到的输出量变化的最小 输入量变化值。</skos:definition>
        <skos:prefLabel xml:lang="en">resolution</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#restored_water_quantity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#restored_water_quantity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DesignAnnualRunoff"/>
        <skos:definition xml:lang="zh-cn">河川径流受人类活动影响的减少或增加的水量。</skos:definition>
        <skos:prefLabel xml:lang="en">restored water quantity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#resultant_discharge -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#resultant_discharge">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
        <skos:definition xml:lang="zh-cn">在有两条或两条以上支流汇入的河段中，同时到达下游站的 各上游站相应流量之和。</skos:definition>
        <skos:prefLabel xml:lang="en">resultant discharge</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#resurgence -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#resurgence">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">河流上游段消失的地面水流，经过地下通道在它的下游段重 现于地面的现象。</skos:definition>
        <skos:prefLabel xml:lang="en">resurgence</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#retention_area_for_urban_extraordinary_flood -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#retention_area_for_urban_extraordinary_flood">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#UrbanHydrology"/>
        <skos:definition xml:lang="zh-cn">城市区发生超防洪标准时，为防止洪水无序漫流造成洪灾损 失和有效利用洪水，利用水库、塘堰或生态休闲场地将洪水蓄存的 区域。</skos:definition>
        <skos:prefLabel xml:lang="en">retention area for urban extraordinary flood</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#retention_capacity -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#retention_capacity">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Groundwater"/>
        <skos:definition xml:lang="zh-cn">含水岩土在重力作用下释水时，由于固体颗粒表面的吸附力 和毛细力的作用，使在其空隙中能保持一定水量的能力。</skos:definition>
        <skos:prefLabel xml:lang="en">retention capacity</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#return_flow -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#return_flow">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SurveyAndAssessment"/>
        <skos:definition xml:lang="zh-cn">水在利用过程中，通过地表或地下回流到河流、湖泊等地表水 体或地下含水层的水。</skos:definition>
        <skos:prefLabel xml:lang="en">return flow</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#return_flow_coefficient -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#return_flow_coefficient">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SurveyAndAssessment"/>
        <skos:definition xml:lang="zh-cn">回归水量与相应的总取水量的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">return flow coefficient</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reuse_of_municipal_wastewater -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reuse_of_municipal_wastewater">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentProtection"/>
        <skos:definition xml:lang="zh-cn">以城镇污水为再生水源，经再生工艺净化处理后，达到可用的 水质标准，通过管道输送或现场使用方式予以利用的全过程。</skos:definition>
        <skos:prefLabel xml:lang="en">reuse of municipal wastewater</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reversing_current -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reversing_current">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:prefLabel xml:lang="en">reversing current</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#reversing_thermometer -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#reversing_thermometer">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#StageWaterTemporatureAndWaveObservationDevices"/>
