        <skos:prefLabel xml:lang="en">roughness</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#routine_monitoring_indices_of_water_quality -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#routine_monitoring_indices_of_water_quality">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentMonitoring"/>
