        <skos:definition xml:lang="zh-cn">一次测流中各实测测点流速中的最大值。</skos:definition>
        <skos:prefLabel xml:lang="en">maximum flow velocity at a point</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#maximum_likelihood_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#maximum_likelihood_method">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">将统计参数代入频率曲线的密度函数中，使水文系列中各值 对应频率密度之乘积为最大的估计总体统计参数的方法。</skos:definition>
        <skos:prefLabel xml:lang="en">maximum likelihood method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#maximum_molecular_moisture_content -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#maximum_molecular_moisture_content">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#SoilMoisture"/>
        <skos:definition xml:lang="zh-cn">土壤中由分子吸附力所能保持的最大水量，包括全部吸着水 和薄膜水</skos:definition>
        <skos:prefLabel xml:lang="en">maximum molecular moisture content</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#maximum_permissible_error -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#maximum_permissible_error">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#Measuring"/>
        <skos:definition xml:lang="zh-cn"> 对给定的测量仪器，规范、规程等所允许的误差极限值。</skos:definition>
        <skos:prefLabel xml:lang="en">maximum permissible error</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#mean -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#mean">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicStatisticsAndStochasticHydrology"/>
        <skos:definition xml:lang="zh-cn">以频数作权重所得的平均数。</skos:definition>
        <skos:prefLabel xml:lang="en">mean</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#mean_basin_elevation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#mean_basin_elevation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">流域内各相邻等高线间的面积乘以其相应平均高程乘积之和 与流域面积的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">mean basin elevation</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#mean_basin_slope -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#mean_basin_slope">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">流域内最高最低等高线长度的一半及各等高线长度乘以等高 线间的高差乘积之和与流域面积的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">mean basin slope</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#mean_basin_width -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#mean_basin_width">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
        <skos:definition xml:lang="zh-cn">流域面积与流域长度的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">mean basin width</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#mean_channel_bed_elevation_profile -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#mean_channel_bed_elevation_profile">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DepthAndCrossSectionMeasurement"/>
        <skos:definition xml:lang="zh-cn">由河段各横断面的河底平均高程点连线和水面所形成的纵剖面。</skos:definition>
        <skos:prefLabel xml:lang="en">mean channel bed elevation profile</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#mean_depth_at_a_cross-section -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#mean_depth_at_a_cross-section">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#DepthAndCrossSectionMeasurement"/>
        <skos:definition xml:lang="zh-cn">水道断面面积与水面宽的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">mean depth at a cross-section</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#mean_depth_of_lake -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#mean_depth_of_lake">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">湖泊容量与湖泊面积的比值。</skos:definition>
        <skos:prefLabel xml:lang="en">mean depth of lake</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#mean_error -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#mean_error">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
