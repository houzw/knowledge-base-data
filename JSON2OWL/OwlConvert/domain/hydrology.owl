    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#peak_time_lag -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#peak_time_lag">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#RunoffYieldAndFlowConcentration"/>
