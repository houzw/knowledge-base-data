        <skos:prefLabel xml:lang="en">orographic rain</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#outlets -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#outlets">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WatershedAndDrainageSystem"/>
