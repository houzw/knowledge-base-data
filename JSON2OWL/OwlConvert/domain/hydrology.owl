    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#water_environment -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#water_environment">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironment"/>
