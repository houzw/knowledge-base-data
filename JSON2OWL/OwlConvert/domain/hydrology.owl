        <skos:prefLabel xml:lang="en">geomorphologic instantaneous unit hydrograph</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#glacial_lake -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#glacial_lake">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#LakeAndReservior"/>
        <skos:definition xml:lang="zh-cn">由冰川作用形成的大面积洼地积水。</skos:definition>
        <skos:prefLabel xml:lang="en">glacial lake</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#glacial_meltwater_runoff -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#glacial_meltwater_runoff">
