    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#opto-chemical_degradation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#opto-chemical_degradation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterEnvironmentQuality"/>
