        <skos:prefLabel xml:lang="en">hail</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#half-submerged_flow -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#half-submerged_flow">
