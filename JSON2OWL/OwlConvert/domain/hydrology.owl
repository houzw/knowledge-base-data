    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tidal_level -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tidal_level">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#EstuarineTide"/>
        <skos:definition xml:lang="zh-cn">受潮汐影响所产生周期性涨落的水位。</skos:definition>
        <skos:prefLabel xml:lang="en">tidal level</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#tidal_level_observation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#tidal_level_observation">
