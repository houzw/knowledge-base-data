        <skos:prefLabel xml:lang="en">oven-dry method</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#over-limit_ratio -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#over-limit_ratio">
