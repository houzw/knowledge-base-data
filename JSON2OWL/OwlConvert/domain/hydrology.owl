        <skos:prefLabel xml:lang="en">pouring water type evaporimeter</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#precipitation -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#precipitation">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrologicCycle"/>
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#PrecipitationAndEvaporationObservation"/>
