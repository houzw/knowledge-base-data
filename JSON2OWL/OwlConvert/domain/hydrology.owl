        <skos:prefLabel xml:lang="en">standard stage value</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#standard_uncertainty -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#standard_uncertainty">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#HydrometricError"/>
        <skos:definition xml:lang="zh-cn">以标准偏差表示的不确定度。</skos:definition>
        <skos:prefLabel xml:lang="en">standard uncertainty</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#state_variable -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#state_variable">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#WaterConservancyComputation"/>
        <skos:definition xml:lang="zh-cn">表达系统所处状态的变量组。</skos:definition>
        <skos:prefLabel xml:lang="en">state variable</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#station-year_method -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#station-year_method">
