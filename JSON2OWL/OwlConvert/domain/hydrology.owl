        <skos:definition xml:lang="zh-cn">采样筒横置，应用落锤撞击，瞬间关上采样筒盖的采样器。</skos:definition>
        <skos:prefLabel xml:lang="en">hammer drive horizontal sampler</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hammer_drive_sampler -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hammer_drive_sampler">
        <rdf:type rdf:resource="http://www.egc.org/ont/domain/hydrology#GroundwaterObservationDevices"/>
        <skos:definition xml:lang="zh-cn">用重锤撞击关闭采样容器的地下水采样器。</skos:definition>
        <skos:prefLabel xml:lang="en">hammer drive sampler</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://www.egc.org/ont/domain/hydrology#hand-operating_cableway -->

    <owl:NamedIndividual rdf:about="http://www.egc.org/ont/domain/hydrology#hand-operating_cableway">
